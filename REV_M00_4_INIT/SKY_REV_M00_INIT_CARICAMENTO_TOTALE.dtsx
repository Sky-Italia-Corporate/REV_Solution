          SQLTask:Connection="{F33C4FB5-5EBC-4917-912B-F01928219B1A}"
          SQLTask:SqlStatementSource="/*&#xA;   il valore STIMA viene azzerato solo al caricamento di una nuova week,&#xA;   se viene ripetuto il caricamento di una stessa week non viene azzerato&#xA;*/&#xA;&#xA;update M_REV_ACTFEE_NN_STD_STIMA&#xA;set STIMA = 0,&#xA;    WEEK_ID = b.WEEK_ID,&#xA;    SCE_ID = b.SCE_ID&#xA;from M_REV_ACTFEE_NN_STD_STIMA a&#xA;join (  select *&#xA;        from M_REV_CALC_WEEK&#xA;      ) b&#xA;on not (a.WEEK_ID = b.WEEK_ID and a.SCE_ID = b.SCE_ID)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
