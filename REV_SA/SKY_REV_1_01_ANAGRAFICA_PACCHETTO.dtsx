          SQLTask:SqlStatementSource="/* AGGIUNGE NUOVI RAGGRUPPAMENTI DI PACCHETTO NON ANCORA PRESENTI NELLO STORICO */&#xA;&#xA;insert into S_REV_STO_PCK_PACCHETTO_RGR([PCK_RGR_DES],[PCK_LEV2_RGR])&#xA;&#x9;select distinct PCK_RGR_DES, PCK_LEV2_RGR&#xA;&#x9;&#x9;from S_REV_PCK_PACCHETTO_SETTIMANALE&#xA;except&#xA;&#x9;select PCK_RGR_DES,  PCK_LEV2_RGR&#xA;&#x9;&#x9;from S_REV_STO_PCK_PACCHETTO_RGR&#xA;&#xA;&#xA;/* COMPLETA ID PACCHETTO E DESCRIZIONE CON IL CODICE RAGGRUPPAMENTO RECUPERATO DALLA TABELLA DELLO STORICO */&#xA;&#xA;update S_REV_PCK_PACCHETTO_SETTIMANALE&#xA;set&#x9;PCK_ID = p.PCK_ID + ' - P'+ right('00' + CAST(s.PCK_RGR_IDS AS VARCHAR), 3), &#xA;&#x9;PCK_DES = p.PCK_ID + ' - P'+ right('00' + CAST(s.PCK_RGR_IDS AS VARCHAR), 3)+ ' - ' + p.PCK_DES&#xA;from S_REV_PCK_PACCHETTO_SETTIMANALE p &#xA;&#x9;inner join S_REV_STO_PCK_PACCHETTO_RGR s &#xA;&#x9;&#x9;ON (p.PCK_RGR_DES = s.PCK_RGR_DES)&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;/* SVUOTA LA TABELLA S_REV_PCK_PACCHETTO  E LA INSERISCE IN TABELLA TEMPORANEA */&#x9;&#x9;&#xA;&#xA;select distinct *&#xA;into #S_REV_PCK_PACCHETTO&#xA;from S_REV_PCK_PACCHETTO&#xA;&#xA;truncate table S_REV_PCK_PACCHETTO&#xA;&#xA;&#xA;/* INSERISCE IN S_REV_PCK_PACCHETTO I PACCHETTI INDIVIDUATI NELLA SETTIMANA DI CALCOLO CHE NON SONO ANCORA IN ANAGRAFICA */&#xA;&#xA;insert into S_REV_PCK_PACCHETTO&#xA;select distinct *&#xA;&#x9;from S_REV_PCK_PACCHETTO_SETTIMANALE a&#xA;&#x9;where a.PCK_RC not in (&#xA;&#x9;&#x9;select distinct b.PCK_RC&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;from #S_REV_PCK_PACCHETTO b&#xA;&#x9;&#x9;    where PCK_RC is not null&#xA;&#x9;)&#xA;&#x9;&#xA;&#x9;&#xA;/* PER I RATE CODE GIA PRESENTI IN ANAGRAFICA UTILIZZA I DATI DEI PACCHETTI COINVOLTI NELLA SETTIMANA DI CALCOLO */&#xA;&#xA;insert into S_REV_PCK_PACCHETTO&#xA;select distinct a.PCK_RC,&#xA;case&#xA;&#x9;when b.PCK_ID is null then a.PCK_ID&#xA;&#x9;else b.PCK_ID &#xA;end as PCK_ID,&#xA;case &#xA;&#x9;when b.PCK_DES is null then a.PCK_DES&#xA;&#x9;else b.PCK_DES&#xA;end as PCK_DES, &#xA;case&#xA;&#x9;when b.PCK_RGR_DES is null then a.PCK_RGR_DES&#xA;&#x9;else b.PCK_RGR_DES&#xA;end as PCK_RGR_DES,&#xA;case&#xA;&#x9;when b.PCK_LEV2_RGR is null then a.PCK_LEV2_RGR&#xA;&#x9;else b.PCK_LEV2_RGR&#xA;end as PCK_LEV2_RGR&#xA;from #S_REV_PCK_PACCHETTO a left outer join S_REV_PCK_PACCHETTO_SETTIMANALE b&#xA;on a.PCK_RC = b.PCK_RC&#xA;&#xA;&#xA;/* CANCELLA PACCHETTI CON RATE CODE E/O ID FITTIZI */&#xA;&#xA;delete&#xA;from S_REV_PCK_PACCHETTO&#xA;where PCK_RC like '%***-NO_PCK - P%'&#xA;&#xA;delete from S_REV_PCK_PACCHETTO&#xA;where PCK_ID = '***-NO_PCK - 0.00 - P001'&#xA;&#xA;--20140826 FRDI begin&#xA;delete from S_REV_PCK_PACCHETTO&#xA;where PCK_ID = '***-NO_PCK - P001'&#xA;--20140826 FRDI end&#xA;&#xA;&#xA;/* INSERISCE IL PACCHETTO FITTIZIO PER OGNI RAGGRUPPAMENTO ED IL FITTIZIO GENERALE*/&#xA;&#xA;insert into S_REV_PCK_PACCHETTO&#xA;select&#x9;'***-NO_PCK - P'+right('00'+cast(PCK_RGR_IDS as varchar), 3), &#xA;&#x9;&#x9;'***-'+a.PCK_RGR_DES, &#xA;&#x9;&#x9;'***-'+a.PCK_RGR_DES, &#xA;&#x9;&#x9;a.PCK_RGR_DES,&#xA;&#x9;&#x9;a.PCK_LEV2_RGR&#xA;from (&#xA;&#x9;select distinct PCK_RGR_DES, PCK_LEV2_RGR&#xA;&#x9;&#x9;from S_REV_PCK_PACCHETTO&#xA;&#x9;)a &#xA;inner join S_REV_STO_PCK_PACCHETTO_RGR b &#xA;&#x9;on a.PCK_RGR_DES = b.PCK_RGR_DES&#xA;union all&#xA;select '***-NO_PCK','***-NO_PCK - 0.00 - P001', '***-NO_PCK - 0.00 - P001 - ***-Raggruppamento Pacchetto Mancante', '***-Raggruppamento Pacchetto Mancante', '***-NO_PCK_LEV_RGR'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
