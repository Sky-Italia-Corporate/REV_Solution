              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT a.GIO_ID
      ,z.WEEK_ID
      ,a.SCE_ID
      ,d.DUR_DES
      ,MOP_ID      
      , cast( CASE 
			WHEN e.TCN_RGR is null and a.TCN_ID = 4 and a.FLG_BB = 0 then 'Standard'
			WHEN e.TCN_RGR is null and a.TCN_ID = 4 and a.FLG_BB = 1 then 'Broadband' 
			ELSE e.TCN_DES 
	  END as varchar(50)) as TCN_DES
      ,cast(i.CLS_DES	 as varchar (50)) as CLS_DES
      ,a.CLU_IPTV  
      ,SUM(NUM_CLI) AS NUM_CLI
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG
  FROM [SKY_REV].[dbo].[T_SBS_SITCOMP] a
join [SKY_SA].[dbo].S_SUB_TAB_TIPO_ABBONATO b on a.tab_id =  b.tab_id 
join [SKY_SA].dbo.S_SUB_DUR_DURATA d on a.dur_id = d.dur_id
join [SKY_SA].dbo.S_SUB_TCN_TECNOLOGIA e on a.tcn_id = e.tcn_id
join [SKY_SA].dbo.S_SUB_SDE_STATO_DECODER k on a.sde_id = k.sde_id
join [SKY_SA].[dbo].[S_SUB_STA_STATO] c on a.sta_id = c.sta_id
join T_SUB_TCN_CLUSTER i on a.cls_id = i.cls_id
join  (
	select [GIO_ID], week_id = 'W' + RIGHT(Wee2_Id,2)
	from [SKY_REV].dbo.M_REV_TEMPO
	) z 
on  a.[GIO_ID] = z.gio_ID

WHERE 
	b.TAB_DES in (
			select elemento 
			from [SKY_REV].dbo.M_REV_MAPPING_DIM_FACT_TABLE 
			where [FACT_TABLE]= 'SF_REV_DECODER_RENTAL' AND DIMENSIONE = 'TIPO_ABBONATO'
			)
	AND c.STA_LEV0_DES in (
			select STA_LEV0_DES 
			from [SKY_SA].dbo.M_SUB_MAPPING_STATO_DATABASE 
			where STA_DATABASE like '%REV%'
			)
	AND k.[SDE_DES] = 'DECODER RENTAL' 

GROUP BY z.[WEEK_ID]
	  ,a.[SCE_ID]
	  ,d.[DUR_DES]
	  ,a.[MOP_ID]
	  ,k.[SDE_DES]
	  ,a.[FLG_BB]	
	  ,i.[CLS_DES] 
	  ,e.[TCN_RGR]
	  ,e.[TCN_DES] 
	  ,a.[GIO_ID]
	  ,a.CLU_IPTV
	  ,a.TCN_ID
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG</property>
