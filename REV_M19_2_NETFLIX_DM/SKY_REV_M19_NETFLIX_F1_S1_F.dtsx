JOIN (
	select t.Gio_Id
	from  M_REV_CALC_WEEK m 
	inner join 
		M_REV_TEMPO t 
	on right(ltrim(rtrim(m.SCE_ID)), 2) = right(ltrim(rtrim(t.Ann_Id)), 2)
		and m.WEEK_ID = t.Wee2_Id
) gio 
	on sf.GIO_ID = gio.Gio_Id</property>
