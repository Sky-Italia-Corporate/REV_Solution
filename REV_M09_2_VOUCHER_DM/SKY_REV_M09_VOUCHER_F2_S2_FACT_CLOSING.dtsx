                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT s.[GIO_ID]
       ,SUM(s.[VOLUMI]) AS VOLUMI
	  ,s.[IMP_CNN]
      ,s.[SCE_ID]
      ,s.[PCK_ID]
      ,s.[BB_CONTRACT_STATUS]
      ,s.[BB_PRD_ID]
      ,s.[BB_ORIGIN_CLUSTER]
      ,s.[BB_VOICE_FLG]
      ,s.[BUNDLE_FLG]
  FROM [SKY_REV].[dbo].[SF_REV_VOUC_CLO] s
where  s.[GIO_ID]  in 
(select distinct t.Gio_Id
from M_REV_CALC_WEEK m inner join M_REV_TEMPO t on right(ltrim(rtrim(m.SCE_ID)), 2) = right(ltrim(rtrim(t.Ann_Id)), 2)
and m.WEEK_ID = t.Wee2_Id)
GROUP BY       s.[GIO_ID],s.[SCE_ID],s.[IMP_CNN],s.[PCK_ID],s.[BB_CONTRACT_STATUS]
      ,s.[BB_PRD_ID]
      ,s.[BB_ORIGIN_CLUSTER]
      ,s.[BB_VOICE_FLG]
      ,s.[BUNDLE_FLG]
order by 1,5</property>
