                  name="OpenRowset">[dbo].[SF_REV_ACCOUNT_PROTETTI]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	sf.FYWK,
	tm.GIO_ID,
	sf.RC_SCONTO_PROTEZIONE,
	sf.CATEGORIA_RC_SCONTO, 
	sf.ID_PACCHETTO_REALE, 
	sf.IMP_SCONTO_PROTEZIONE, 
	sf.TCN_ID,
	sf.CLS_ID,
	sf.VOLUMI, 
	case when tm.Gio_Uda = 'FATT. SOL' then
		((sf.IMP_SCONTO_PROTEZIONE * sf.VOLUMI * 12 / 365) / iva.VAL_IVA) 
	else
		((sf.IMP_SCONTO_PROTEZIONE * sf.VOLUMI / 28) / iva.VAL_IVA) 
	end AS ACCPR_DAY,
	sf.BB_CTR_STA,
	sf.BB_PRD_ID,
	sf.BB_OR_CLS,
	sf.BB_VOICE_FLG,
	sf.BUNDLE_FLG
FROM SF_REV_ACCOUNT_PROTETTI sf
join M_REV_CALC_WEEK wk
	on sf.FYWK = right(wk.SCE_ID,2)+RIGHT(wk.WEEK_ID,2)
left join V_M_REV_TEMPO tm 
	on sf.FYWK = RIGHT(tm.ANN_ID, 2)+RIGHT(tm.WEE2_ID, 2) 
,(SELECT TOP 1 VAL_IVA FROM M_REV_VALORE_IVA WHERE APPL_NAME = 'AccountProtetti') iva</property>
