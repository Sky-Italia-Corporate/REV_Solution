      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          BLOBTempStoragePath=""
          bufferTempStoragePath=""
          engineThreads="5"
          version="1">
          <components>
            <component
              refId="Package\Import From T_REV_DTRS\T_REV_DTRS"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="T_REV_DTRS"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  distinct COD_PROMO_TEC,a.ACCOUNT, a.CATEGORIA_RC, a.TIPO_ABB, a.RC_COD, a.RC_DESCR, a.DATA_ATT, a.DATA_ATT_RC, a.TIPOLOGIA_RC,
	a.PRT_DES, a.COD_PROMO_SKY,a.NOME_PROMO,a.valore, a.VALORE_PACCHETTO,
    FLAG_MOTORE_SCONTI, PROMO_TYPE, PROMO_WEEKLY_RGR, PROMO_CLUSTER, ACC_ID, 
	'ACT_FEE_PRS' as TIPO_INSTALLAZIONE , a.TECNOLOGIA, a.RC_PACCHETTO_REALE, a.RC_PACCHETTO, a.CLUSTER_IPTV,a.COD_LISTINO, a.DATA_ATT_RC as GIO_ID,
	/* Integrazione TV-BB con aggiunta 4 dimensioni da BB */		
	isnull(LTRIM(RTRIM(BB.BB_CONTRACT_STATUS_DES)), 'ZZZ_NO_BB_CONTRACT_STATUS') as BB_CONTRACT_STATUS,
	isnull(LTRIM(RTRIM(BB.PRODUCT_CODE)), 'ZZZ_NO_BB_PRODUCT') as BB_PRD_ID,
	isnull(LTRIM(RTRIM(BB.ORIGIN_CLUSTER)), 'ZZZ_NO_ORIGIN_CLUSTER') as BB_ORIGIN_CLUSTER,
	isnull(LTRIM(RTRIM(BB.VOICE_FLAG)), 'N') as BB_VOICE_FLG,
	ISNULL(LTRIM(RTRIM(Case when (BB.FLG_BUNDLE) = '0' then 'No Bundle'
                            when (BB.FLG_BUNDLE) = '1' then 'Bundle' end)), 'No Bundle') AS BB_FLG_BUNDLE
	from (
    select distinct COD_PROMO_TEC, a.PRT_DES, a.COD_PROMO_SKY,a.NOME_PROMO,sf.GIO_ID, sf.ACC_ID,
    a.TECNOLOGIA,a.DATA_ATT_RC,a.RC_PACCHETTO_REALE,a.account, a.tipo_abb, a.rc_cod,a.RC_DESCR,a.RC_PACCHETTO,a.VALORE,a.VALORE_PACCHETTO,
    a.CATEGORIA_RC,FLAG_MOTORE_SCONTI, PROMO_TYPE, PROMO_WEEKLY_RGR, PROMO_CLUSTER,a.DATA_ATT, a.TIPOLOGIA_RC,a.CLUSTER_IPTV, a.COD_LISTINO
     from
        (
        SELECT
            rs.ACCOUNT, rs.COD_PROMO_TEC, prt.PRT_DES, rs.COD_PROMO_SKY, prs.NOME_PROMO, rs.CATEGORIA_RC, rs.RC_COD, rs.RC_DESCR,  rs.VALORE
            --,rs.COD_LISTINO, rs.DATA_ATT,
             ,rs.DATA_ATT_RC, rs.TIPO_ABB,rs.DATA_ATT,rs.TIPOLOGIA_RC, rs.RC_PACCHETTO, rs.VALORE_PACCHETTO, rs.CLUSTER_IPTV, rs.COD_LISTINO
            --,atps.ACCOUNT    as "CHECK__ATPS"
            ,rs.TECNOLOGIA
			, rs.RC_PACCHETTO_REALE
            ,CASE 
                WHEN promo.COD_PROMO_TEC is null THEN 0 
                ELSE 1 
            END as FLAG_MOTORE_SCONTI 
            ,promo.PROMO_TYPE
            ,promo.PROMO_WEEKLY_RGR
            ,promo.PROMO_CLUSTER
            
        FROM 
            T_REV_DTRS                    rs
        LEFT JOIN 
            (
                Select 
                    distinct ACCOUNT,COD_PROMO_TEC 
                from T_REV_DT_B10_ATPS
            union
                Select 
                    distinct ACCOUNT,COD_PROMO_TEC 
                from T_REV_DT_B10_ATPSQ
            )                            atps    on atps.ACCOUNT = rs.ACCOUNT and atps.COD_PROMO_TEC = rs.COD_PROMO_TEC
        LEFT JOIN
            (
            select
                wtt.COD_PROMO_TEC
                ,promo.PROMO_TYPE, promo.PROMO_WEEKLY_RGR, promo.PROMO_CLUSTER
            from 
                WTT_REV_RC_ATTIVATI_STEP4 wtt
            left join
                S_PROMO                    promo    on promo.PROMO_ID = wtt.COD_PROMO_TEC
            where
                1=1
                and (wtt.COD_PROMO_TEC not like '%0000' or wtt.COD_PROMO_TEC not like '%6666')
            )                            promo    on promo.COD_PROMO_TEC = rs.COD_PROMO_TEC
        LEFT JOIN 
            T_REV_PRT_PROMO_TECNICA        prt        on rs.COD_PROMO_TEC = prt.PRT_ID
        LEFT JOIN 
            (
            select
                distinct * 
            from 
                T_REV_PRS_PROMO_SKY
            )                            prs        on rs.COD_PROMO_SKY = prs.COD_PROMO_SKY
        WHERE 
            1=1
            and CATEGORIA_RC in ('Contributo Att. standard','Sconto Contr. di att.standard','Contributo Att. Premium Serv.','Sconto Contributo Att. Premium')
            and atps.ACCOUNT is null
            ------------------------------
            --Verifica FLAG_MOTORE_SCONTI (ossia tutti qui ratecode che non matchano con la tabella "WTT_REV_RC_ATTIVATI_STEP4")
            --and promo.COD_PROMO_TEC is null
            ------------------------------
            --Caso
            --and rs.ACCOUNT = 13681168
            ------------------------------
        ) a
    left join 
        SKY_SA.dbo.SF_SBF_FIRSTACT sf    
        on sf.ACC_ID = a.ACCOUNT and sf.GIO_ID=a.DATA_ATT_RC 
        --where ACCOUNT = 23637044
)
 a

 left join [SKY_SA_BB].[dbo].[SF_SUB_BBC_SUB_SCBB] bb on  a.DATA_att_rc = BB.GIO_ID and ACCOUNT = BB.ACCOUNT_TV
 where a.ACC_ID is null</property>
