                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select s.[TCN_DES]
      , case when s.[TCN_DES] like '%SKY IPTV TELECOM%' then UPPER(s.CLU_IPTV) 
			 else UPPER(s.CLS_DES) 			
		end as CLS_DES
      ,s.[DUR_DES]
      ,s.[DAT_ID]
      ,s.[MOP_ID]
      ,sum(s.[NUM_CLI]) as [NUM_CLI]
      ,s.[SCE_ID]
      ,s.[PCK_ID]
------Integrazione TV-BB -------------
	  ,s.BB_CONTRACT_STATUS
	  ,s.BB_PRD_ID
	  ,s.BB_ORIGIN_CLUSTER
	  ,CASE WHEN s.BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN s.BB_VOICE_FLG='N_BB' THEN 'N'
		ELSE ISNULL(LTRIM(RTRIM(s.BB_VOICE_FLG)),'N') END AS BB_VOICE_FLG
	  ,s.BUNDLE_FLG
from [SKY_REV].[dbo].[SF_REV_MIN_RIC_CLOSING] s
where  s.[DAT_ID] in 
(select distinct t.Gio_Id
from M_REV_CALC_WEEK m inner join M_REV_TEMPO t on right(ltrim(rtrim(m.SCE_ID)), 2) = right(ltrim(rtrim(t.Ann_Id)), 2)
and m.WEEK_ID = t.Wee2_Id)
group by  s.[TCN_DES]
      , case when s.[TCN_DES] like '%SKY IPTV TELECOM%' then UPPER(s.CLU_IPTV) 
			 else UPPER(s.CLS_DES) 			
		end 
      ,s.[DUR_DES]
      ,s.[DAT_ID]
      ,s.[MOP_ID]
      ,s.[SCE_ID]
      ,s.[PCK_ID]
	  ,s.BB_CONTRACT_STATUS
	  ,s.BB_PRD_ID
	  ,s.BB_ORIGIN_CLUSTER
	  ,CASE WHEN s.BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN s.BB_VOICE_FLG='N_BB' THEN 'N'
		ELSE ISNULL(LTRIM(RTRIM(s.BB_VOICE_FLG)),'N') END
	  ,s.BUNDLE_FLG</property>
