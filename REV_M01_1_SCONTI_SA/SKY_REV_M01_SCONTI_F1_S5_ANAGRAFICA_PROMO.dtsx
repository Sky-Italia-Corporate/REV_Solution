                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
	a.COD_PROMO_TEC as PROMO_ID,
	c.SCE_ID,
	c.WEEK_ID, 
	a.RC_PACCHETTO, 
	a.ACCOUNT,
	a.DATA_ATT_RC,
                a.COD_PROMO_SKY,
	
	case 
		when bs.COD_PROMO_TEC is not null and sgp.COD_PROMO_TEC is not null and sqp.COD_PROMO_TEC is not null then 'BOX SETS'
		when bs.COD_PROMO_TEC is not null and sgp.COD_PROMO_TEC is not null and sqp.COD_PROMO_TEC is null then 'BOX SETS'
		when bs.COD_PROMO_TEC is not null and sgp.COD_PROMO_TEC is null and sqp.COD_PROMO_TEC is null then 'BOX SETS'
		when bs.COD_PROMO_TEC is null and sgp.COD_PROMO_TEC is not null and sqp.COD_PROMO_TEC is not null then 'Sky Go Plus'
		when bs.COD_PROMO_TEC is null and sgp.COD_PROMO_TEC is not null and sqp.COD_PROMO_TEC is null then 'Sky Go Plus'		
		when bs.COD_PROMO_TEC is null and sgp.COD_PROMO_TEC is null and sqp.COD_PROMO_TEC is not null then 'Sky Q Plus'		
		else 'NONE' end
	as PROMO_CLUSTER_DES
from WTT_REV_RC_ATTIVATI_STEP4 a
cross join M_REV_CALC_WEEK c
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Boxsets') bs
	on a.COD_PROMO_TEC = bs.COD_PROMO_TEC
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Skygoplus') sgp
	on a.COD_PROMO_TEC = sgp.COD_PROMO_TEC
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Sky Q Plus') sqp
	on a.COD_PROMO_TEC = sqp.COD_PROMO_TEC
where a.COD_PROMO_TEC not like '%_SQP'
		and a.COD_PROMO_TEC not like '%-9999%'</property>
