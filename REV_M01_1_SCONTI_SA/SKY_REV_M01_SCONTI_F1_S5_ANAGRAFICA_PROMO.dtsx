        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="/*** Sistema anche tutte le promo presenti nello storico - TK ***/&#xA;update a&#xA;set PROMO_CLUSTER = 'NONE', PROMO_WEEKLY_RGR = '***-NO_PROMO_SKY_RGR_TK'&#xA;from S_PROMO a&#xA;join (select distinct&#xA;&#x9;&#x9;&#x9;COD_PROMO_TEC&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP = 'TK') b&#xA;&#x9;on a.PROMO_ID = b.COD_PROMO_TEC&#xA;where a.PROMO_TYPE != 'TK'&#xA;&#xA;/*** Uniforma a TK tutte le promo che prsentano configurazioni multiple e una di queste è TK ***/&#xA;update a&#xA;set a.CATEGORIA_RC = 'Sconto Contributo Ticket'&#xA;from (select&#xA;&#x9;&#x9;&#x9;a.*&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP != 'TK') a&#xA;join (select distinct&#xA;&#x9;&#x9;&#x9;COD_PROMO_TEC&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP = 'TK') b&#xA;&#x9;on a.COD_PROMO_TEC = b.COD_PROMO_TEC&#xA;&#x9;&#xA;/*** Sistema anche tutte le promo presenti nello storico - AC ***/&#xA;update a&#xA;set PROMO_CLUSTER = 'NONE', PROMO_WEEKLY_RGR = '***-NO_PROMO_SKY_RGR_AC'&#xA;from S_PROMO a&#xA;join (select distinct&#xA;&#x9;&#x9;&#x9;COD_PROMO_TEC&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP = 'AC') b&#xA;&#x9;on a.PROMO_ID = b.COD_PROMO_TEC&#xA;where a.PROMO_TYPE != 'AC'&#xA;&#x9;&#xA;/*** Uniforma a AC tutte le promo che prsentano configurazioni multiple e una di queste è AC ***/&#xA;update a&#xA;set a.CATEGORIA_RC = 'Sconto Canone Abb. Option'&#xA;from (select&#xA;&#x9;&#x9;&#x9;a.*&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP != 'AC') a&#xA;join (select distinct&#xA;&#x9;&#x9;&#x9;COD_PROMO_TEC&#xA;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO b&#xA;&#x9;&#x9;&#x9;on a.CATEGORIA_RC = b.CATEGORIA_RC and a.CLASSE = b.CLASSE&#xA;&#x9;&#x9;where b.MAP = 'AC') b&#xA;&#x9;on a.COD_PROMO_TEC = b.COD_PROMO_TEC" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
