  <DTS:Executables>
    <DTS:Executable
      DTS:refId="Package\Aggiorna il campo PROMO_WEEKLY_RGR per le nuove promo con HD o SD"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{854886EF-E2AC-4190-B3A3-C18535A3D7B5}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiorna il campo PROMO_WEEKLY_RGR per le nuove promo con HD o SD"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="UPDATE m&#xA;set m.PROMO_WEEKLY_RGR = a.PROMO_WEEKLY_RGR, m.PROMO_CLUSTER = 'ZZZ'&#xA;FROM    (&#xA;&#x9;SELECT  PROMO_ID, PROMO_ID+'0000' as PROMO_HD, PROMO_ID+'6666' as PROMO_SD, w.PROMO_WEEKLY_RGR&#xA;&#x9;FROM    M_ATT_PROMO m&#xA;&#x9;join&#x9;M_REV_PROMO_WEEKLY_RGR W &#xA;&#x9;on&#x9;&#x9;m.PROMO_ID = W.COD_PROMO_TECNICA&#xA;&#x9;WHERE&#x9;GESTIONE_SD_HD = 'Yes'&#xA;)a&#xA;join M_ATT_PROMO m&#xA;on a.PROMO_HD = m.PROMO_ID or a.PROMO_SD = m.PROMO_ID or a.PROMO_ID = m.PROMO_ID&#xA;WHERE m.PROMO_WEEKLY_RGR like '%***-NO_PROMO_SKY_RGR%'&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Aggiorna il campo PRO_WEEKLY_RGR per le nuove PROMO"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{579A4DB6-EF48-44A3-BDD2-D0D9BB0F310B}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiorna il campo PRO_WEEKLY_RGR per le nuove PROMO"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="/*** Inserisce tutte le promo (non SQP) per cui aggiornare il raggruppamento nella tabella d'appoggio ***/&#xA;select * &#xA;into #M_ATT_PROMO_TEMP&#xA;from M_ATT_PROMO &#xA;where right(PROMO_WEEKLY_RGR, 4) = 'RGR_'&#xA;&#x9;&#x9;and PROMO_ID not like '%_HDQ' and PROMO_ID not like '%_MSQ' and PROMO_ID not like '%_UHDQ'&#xA;&#xA;/*** Inserisce tutte le promo SQP per cui aggiornare il raggruppamento nella tabella d'appoggio ***/&#x9;&#xA;select * &#xA;into #M_ATT_PROMO_SQP_TEMP&#xA;from M_ATT_PROMO &#xA;where right(PROMO_WEEKLY_RGR, 4) = 'RGR_'&#xA;&#x9;&#x9;and (PROMO_ID like '%_HDQ' or PROMO_ID like '%_MSQ' or PROMO_ID like '%_UHDQ')&#xA;&#x9;&#x9;&#xA;&#xA;select *&#xA;into #WTT_REV_RC_ATTIVATI_STEP4_SQP&#xA;from WTT_REV_RC_ATTIVATI_STEP4_SQP&#xA;where CATEGORIA_RC = 'Sconto Canone Abb. Sky Q Plus' &#xA;&#xA;/*** Cancella tutte le promo per cui aggiornare il raggruppamento ***/&#xA;delete from M_ATT_PROMO where right(PROMO_WEEKLY_RGR, 4) = 'RGR_' &#xA;&#xA;/*** Recupera PROMO_TYPE per PROMO che vengono prese da WT_REV_RC_ATTIVATI ***/&#xA;insert into M_ATT_PROMO&#xA;select distinct a.PROMO_ID, d.SCE_ID, d.WEEK_ID, a.PROMO_CLUSTER, (a.PROMO_WEEKLY_RGR + c.MAP) as PROMO_WEEKLY_RGR, a.PROMO_WEEKLY, a.GESTIONE_SD_HD, a.VOL_ATT_PROMO, a.VOUCHER_NO_VOUCHER, a.COD_REVENUE_SHARE, a.PROMO_SKY&#xA;&#x9;from #M_ATT_PROMO_TEMP a&#xA;&#x9;&#x9;left join WTT_REV_RC_ATTIVATI_STEP4 b&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.COD_PROMO_TEC as varchar(50))&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO c&#xA;&#x9;&#x9;&#x9;on b.CATEGORIA_RC = c.CATEGORIA_RC and b.CLASSE = c.CLASSE&#xA;&#x9;&#x9;,M_REV_CALC_WEEK d&#xA;&#x9;where&#xA;&#x9;&#x9;-- Recupera solo promo presenti in WTT_REV_RC_ATTIVATI_HD_SD&#xA;&#x9;&#x9;b.COD_PROMO_TEC is not null and c.MAP is not null&#xA;&#x9;&#xA;/*** Recupera PROMO_TYPE per PROMO che vengono prese da WT_REV_SBF_FIRSTACT ***/&#xA;insert into M_ATT_PROMO&#xA;select distinct a.PROMO_ID, d.SCE_ID, d.WEEK_ID, a.PROMO_CLUSTER, (a.PROMO_WEEKLY_RGR + 'FA') as PROMO_WEEKLY_RGR, a.PROMO_WEEKLY, a.GESTIONE_SD_HD, a.VOL_ATT_PROMO, a.VOUCHER_NO_VOUCHER, a.COD_REVENUE_SHARE, a.PROMO_SKY&#xA;&#x9;from #M_ATT_PROMO_TEMP a&#xA;&#x9;&#x9;left join WTT_REV_SBF_FIRSTACT_HD_SD b&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.PRT_ID as varchar(50))&#xA;&#x9;&#x9;left join T_REV_PRT_PROMO_TECNICA c&#xA;&#x9;&#x9;&#x9;on cast(b.PRT_ID as varchar(50)) = cast(c.PRT_ID as varchar(50))&#xA;&#x9;&#x9;left join M_ATT_PROMO e&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(e.PROMO_ID as varchar(50))&#xA;&#x9;&#x9;,M_REV_CALC_WEEK d&#xA;&#x9;where&#xA;&#x9;&#x9;-- Recupera solo promo appena inserite per cui manca ancora il completamento del campo PROMO_WEEKLY_RGR &#xA;&#x9;&#x9;right(a.PROMO_WEEKLY_RGR, 4) = 'RGR_' &#xA;&#x9;&#x9;-- Recupera solo promo presenti in WTT_REV_SBF_FIRSTACT_HD_SD&#xA;&#x9;&#x9;and b.PRT_ID is not null and e.PROMO_ID is null&#xA;&#x9;&#x9;&#xA;/*** Recupera PROMO_TYPE per PROMO che vengono prese da WT_REV_RC_ATTIVATI ***/&#xA;insert into M_ATT_PROMO&#xA;select distinct a.PROMO_ID, d.SCE_ID, d.WEEK_ID, a.PROMO_CLUSTER, (a.PROMO_WEEKLY_RGR + c.MAP) as PROMO_WEEKLY_RGR, a.PROMO_WEEKLY, a.GESTIONE_SD_HD, a.VOL_ATT_PROMO, a.VOUCHER_NO_VOUCHER, a.COD_REVENUE_SHARE, a.PROMO_SKY&#xA;&#x9;from #M_ATT_PROMO_SQP_TEMP a&#xA;&#x9;&#x9;left join #WTT_REV_RC_ATTIVATI_STEP4_SQP b&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.COD_PROMO_TEC as varchar(50))&#xA;&#x9;&#x9;join M_REV_PROMO_MAP_UNICO c&#xA;&#x9;&#x9;&#x9;on b.CATEGORIA_RC = c.CATEGORIA_RC and b.CLASSE = c.CLASSE&#xA;&#x9;&#x9;,M_REV_CALC_WEEK d&#xA;&#x9;where&#xA;&#x9;&#x9;-- Recupera solo promo presenti in WTT_REV_RC_ATTIVATI_HD_SD&#xA;&#x9;&#x9;b.COD_PROMO_TEC is not null and c.MAP is not null&#xA;&#x9;&#x9;&#xA;/*** Cancella tabella d'appoggio ***/&#xA;drop table #M_ATT_PROMO_TEMP&#xA;drop table #M_ATT_PROMO_SQP_TEMP&#xA;drop table #WTT_REV_RC_ATTIVATI_STEP4_SQP" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Aggiorna S_PROMO se in M_ATT_PROMO sono presenti diversi parametri"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{E2BAE3DC-0C05-47A0-ABFE-D3865301F925}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiorna S_PROMO se in M_ATT_PROMO sono presenti diversi parametri"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="--- Script creato il 19/11/2012 per il progetto Revenues Fast Close&#xA;--- Aggiornato il 28/02/2013 per la gestione delle promo VOUCHER /  NO_VOUCHER&#xA;&#xA;--- Cerca promo con configurazione variata e inserisce i PROMO_ID nella tabella #PROMO_TO_REMOVE&#xA;select a.PROMO_ID, a.CLASSE, a.PROMO_TYPE&#xA;into #PROMO_TO_REMOVE&#xA;from S_PROMO a&#xA;&#x9;join M_ATT_PROMO b&#xA;&#x9;&#x9;on a.PROMO_ID = b.PROMO_ID and&#xA;&#x9;&#x9;&#x9;(a.PROMO_CLUSTER != b.PROMO_CLUSTER or&#xA;&#x9;&#x9;&#x9;a.PROMO_WEEKLY_RGR != b.PROMO_WEEKLY_RGR or&#xA;&#x9;&#x9;&#x9;a.PROMO_WEEKLY != b.PROMO_WEEKLY or&#xA;&#x9;&#x9;&#x9;a.VOUCHER_NO_VOUCHER != b.VOUCHER_NO_VOUCHER)&#xA;go&#xA;&#xA;--- Cancella dalla tabella S_PROMO le promo per cui è stata variata la configurazione e il cui PROMO_ID è nella tabella #PROMO_TO_REMOVE&#xA;delete from S_PROMO&#xA;&#x9;where PROMO_ID in &#xA;&#x9;&#x9;(select PROMO_ID from #PROMO_TO_REMOVE)&#xA;go&#xA;&#x9;&#x9;&#xA;--- Per le promo con configurazione variata recupera la riga dalla tabella M_ATT_PROMO e la inserisce nella tabella S_PROMO&#xA;insert into S_PROMO&#xA;select a.PROMO_ID, b.PRT_DES, ltrim(rtrim(PROMO_CLUSTER)) as PROMO_CLUSTER, ltrim(rtrim(PROMO_WEEKLY_RGR)) as PROMO_WEEKLY_RGR, ltrim(rtrim(PROMO_WEEKLY)) as PROMO_WEEKLY, c.CLASSE, c.PROMO_TYPE, VOUCHER_NO_VOUCHER&#xA;&#x9;from M_ATT_PROMO a&#xA;&#x9;&#x9;join T_REV_PRT_PROMO_TECNICA b&#xA;&#x9;&#x9;&#x9;on casT(a.PROMO_ID as varchar(50)) = cast(b.PRT_ID as varchar(50))&#xA;&#x9;&#x9;join #PROMO_TO_REMOVE c&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(c.PROMO_ID as varchar(50))&#xA;&#x9;where a.PROMO_ID in &#xA;&#x9;&#x9;(select PROMO_ID from #PROMO_TO_REMOVE)&#xA;go&#xA;&#x9;&#x9;&#xA;--- Cancella la tabella temporanea #PROMO_TO_REMOVE&#xA;drop table #PROMO_TO_REMOVE&#xA;go&#xA;&#xA;--- Recupera la classe per le promo se questa era ancora a NULL&#xA;update a&#xA;&#x9;set a.CLASSE = b.CLASSE&#xA;&#x9;from S_PROMO a&#xA;&#x9;join (select cast(COD_PROMO_TEC as varchar(50)) as COD_PROMO_TEC, CLASSE from WTT_REV_RC_ATTIVATI_STEP4&#xA;&#x9;&#x9;&#x9;union&#xA;&#x9;&#x9;  select cast(PRT_ID as varchar(50)) as COD_PROMO_TEC, CLASSE from WTT_REV_SBF_FIRSTACT_HD_SD) b&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.COD_PROMO_TEC as varchar(50))&#xA;&#x9;where a.CLASSE is null and cast(a.PROMO_ID as varchar(50)) = cast(b.COD_PROMO_TEC as varchar(50))" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Aggiornamento Promo Cluster di Sky Q Plus"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{39f3980f-4b7a-458b-9240-f86ef4c94904}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiornamento Promo Cluster di Sky Q Plus"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="update M_ATT_PROMO&#xA;set PROMO_CLUSTER=PROMO_CLUSTER + ' (' + RIGHT(PROMO_WEEKLY_RGR, 2) + ')',&#xA;&#x9;&#x9;PROMO_WEEKLY_RGR=REPLACE(PROMO_WEEKLY_RGR, RIGHT(PROMO_WEEKLY_RGR,3), '_SQP' + RIGHT(PROMO_WEEKLY_RGR,3))&#xA;where PROMO_CLUSTER = 'Sky Q Plus' and PROMO_WEEKLY_RGR like '***%'&#xA;&#xA;update S_PROMO&#xA;set PROMO_CLUSTER=PROMO_CLUSTER + ' (' + PROMO_TYPE + ')',&#xA;&#x9;&#x9;PROMO_WEEKLY_RGR=REPLACE(PROMO_WEEKLY_RGR, RIGHT(PROMO_WEEKLY_RGR,3), '_SQP' + RIGHT(PROMO_WEEKLY_RGR,3))&#xA;where PROMO_CLUSTER = 'Sky Q Plus' and PROMO_WEEKLY_RGR like '***%'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Aggiornamento promo cluster per promo nuove"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{6B76EFB6-DF68-4788-8AB1-55A1CD65A030}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
