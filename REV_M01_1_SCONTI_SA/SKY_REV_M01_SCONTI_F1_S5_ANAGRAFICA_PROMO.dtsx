      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="--- Script creato il 20/11/2012 per il progetto Revenues Fast Close&#xA;&#xA;--- Cancella dalla tabella S_ATT_PROMO tutti i dati inerenti alla settimana di calcolo&#xA;delete from S_ATT_PROMO&#xA;&#x9;where SCE_ID = (select SCE_ID from M_REV_CALC_WEEK) &#x9;&#xA;&#x9;&#x9;and WEEK_ID = (select WEEK_ID from M_REV_CALC_WEEK)&#xA;go&#xA;&#xA;--- Inserisci i dati dalla tabella M_ATT_PROMO nella tabella S_ATT_PROMO storicizzandoli come dati relativi alla settimana di caricamento&#x9;&#x9;&#xA;insert into S_ATT_PROMO&#xA;&#x9;select a.PROMO_ID, b.SCE_ID, b.WEEK_ID, a.GESTIONE_SD_HD, a.VOL_ATT_PROMO, a.COD_REVENUE_SHARE, a.PROMO_SKY /*ANCC 20170321 salita Promo Sky*/&#xA;&#x9;from M_ATT_PROMO a, &#xA;&#x9;&#x9;M_REV_CALC_WEEK b&#x9;&#x9;&#xA;go&#xA;&#x9;&#x9;&#xA;--- Cancella tutto il contenuto della tabella M_ATT_PROMO&#xA;truncate table M_ATT_PROMO&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Inserisce in S_PROMO le nuove promo contenute in M_ATT_PROMO"
      DTS:CreationName="Microsoft.Pipeline"
      DTS:Description="Data Flow Task"
      DTS:DTSID="{B38DA90C-D56F-498B-B2D0-25FA680323CD}"
      DTS:ExecutableType="Microsoft.Pipeline"
      DTS:LocaleID="-1"
      DTS:ObjectName="Inserisce in S_PROMO le nuove promo contenute in M_ATT_PROMO"
      DTS:TaskContact="Performs high-performance data extraction, transformation and loading;Microsoft Corporation; Microsoft SQL Server v9; (C) 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
      <DTS:Variables />
