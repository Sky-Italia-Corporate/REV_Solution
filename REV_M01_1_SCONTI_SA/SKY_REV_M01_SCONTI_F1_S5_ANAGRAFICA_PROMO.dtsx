        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <pipeline
          BLOBTempStoragePath=""
          bufferTempStoragePath=""
          engineThreads="5"
          version="1">
          <components>
            <component
              refId="Package\Inserisce in S_PROMO le nuove promo contenute in M_ATT_PROMO\M_ATT_PROMO"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="M_ATT_PROMO"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct 
	a.PROMO_ID, b.PRT_DES, ltrim(rtrim(a.PROMO_CLUSTER)) as PROMO_CLUSTER, ltrim(rtrim(a.PROMO_WEEKLY_RGR)) as PROMO_WEEKLY_RGR, ltrim(rtrim(a.PROMO_WEEKLY)) as PROMO_WEEKLY,
	case
		when e.CLASSE_ATTIVATI is NULL then h.CLASSE_FIRSTACT
		else e.CLASSE_ATTIVATI
	end as CLASSE,
	right(a.PROMO_WEEKLY_RGR, 2) as PROMO_TYPE,
	a.VOUCHER_NO_VOUCHER
from M_ATT_PROMO a
	left join T_REV_PRT_PROMO_TECNICA b
		on cast(a.PROMO_ID as varchar(50)) = cast(b.PRT_ID as varchar(50))
	left join (
			select distinct c.COD_PROMO_TEC, c.CLASSE as CLASSE_ATTIVATI 
				from WTT_REV_RC_ATTIVATI_STEP4_SQP c ) e
