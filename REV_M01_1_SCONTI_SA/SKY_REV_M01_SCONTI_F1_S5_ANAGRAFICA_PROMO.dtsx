      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="truncate table M_ATT_PROMO_PROMO_NUOVE&#xA;go&#xA;&#xA;&#xA;insert into M_ATT_PROMO_PROMO_NUOVE&#xA;SELECT  *&#xA;FROM    M_ATT_PROMO&#xA;WHERE&#x9;PROMO_CLUSTER = 'ZZZ'&#xA;&#xA;select distinct PROMO_WEEKLY_RGR, PROMO_CLUSTER&#xA;into&#x9;#M_ATT_PROMO_NO_ZZZ&#xA;from&#x9;M_ATT_PROMO&#xA;WHERE&#x9;PROMO_CLUSTER &lt;&gt; 'ZZZ'&#xA;&#xA;&#xA;declare @cnt int =&#xA;(SELECT  COUNT(*) as CNT&#xA;FROM    (&#xA;&#x9;SELECT  PROMO_WEEKLY_RGR, COUNT(*) as CNT&#xA;&#x9;FROM    #M_ATT_PROMO_NO_ZZZ&#xA;&#x9;WHERE&#x9;PROMO_WEEKLY_RGR not like '%***-NO_PROMO_SKY_RGR%'&#xA;&#x9;GROUP BY PROMO_WEEKLY_RGR&#xA;&#x9;having COUNT(*) &gt; 1&#xA;)a&#xA;)&#xA;&#xA;&#xA;if @cnt &gt; 0&#xA;BEGIN &#xA;&#xA;UPDATE M_ATT_PROMO&#xA;set&#x9;PROMO_CLUSTER = 'NONE'&#xA;where&#x9;PROMO_CLUSTER = 'ZZZ'&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#xA;UPDATE m&#xA;set &#x9;m.PROMO_CLUSTER = a.CLUSTER_NO_ZZZ&#xA;FROM    (&#x9;SELECT&#x9;&#x9;Z.PROMO_ID, Z.PROMO_CLUSTER as CLUSTER_ZZZ, isnull(N.PROMO_CLUSTER,'NONE') as CLUSTER_NO_ZZZ&#xA;&#x9;FROM&#x9;&#x9;M_ATT_PROMO_PROMO_NUOVE Z&#xA;&#x9;left join&#x9;#M_ATT_PROMO_NO_ZZZ N&#xA;&#x9;on&#x9;&#x9;N.PROMO_WEEKLY_RGR = Z.PROMO_WEEKLY_RGR)a&#xA;join M_ATT_PROMO m&#xA;on a.PROMO_ID = m.PROMO_ID&#xA;&#xA;END&#xA;&#xA;&#xA;&#xA;drop table #M_ATT_PROMO_NO_ZZZ" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Aggiornamento promo type per promo nuove"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{710B6D0E-B3B8-46A8-97E5-2A0654C574AE}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiornamento promo type per promo nuove"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="--Vengono inserite in S_PROMO le promo nuove impostate nella maschera M_REV_PROMO_WEEKLY_RGR aventi una categoria e una classe per la mappatura&#xA;insert into S_PROMO&#xA;select distinct &#xA;&#x9;a.PROMO_ID, b.PRT_DES, ltrim(rtrim(a.PROMO_CLUSTER)) as PROMO_CLUSTER, ltrim(rtrim(a.PROMO_WEEKLY_RGR)) as PROMO_WEEKLY_RGR, ltrim(rtrim(a.PROMO_WEEKLY)) as PROMO_WEEKLY,&#xA;&#x9;case&#xA;&#x9;&#x9;when e.CLASSE_ATTIVATI is NULL then h.CLASSE_FIRSTACT&#xA;&#x9;&#x9;else e.CLASSE_ATTIVATI&#xA;&#x9;end as CLASSE,&#xA;&#x9;--right(a.PROMO_WEEKLY_RGR, 2) as PROMO_TYPE,&#xA;&#x9;c.MAP as PROMO_TYPE,&#xA;&#x9;a.VOUCHER_NO_VOUCHER&#xA;from M_ATT_PROMO a&#xA;&#x9;left join T_REV_PRT_PROMO_TECNICA b&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.PRT_ID as varchar(50))&#xA;&#x9;left join (&#xA;&#x9;&#x9;&#x9;select distinct c.COD_PROMO_TEC, c.CLASSE as CLASSE_ATTIVATI, c.CATEGORIA_RC &#xA;&#x9;&#x9;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4_SQP c ) e&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(e.COD_PROMO_TEC as varchar(50))&#xA;&#x9;left join M_REV_PROMO_MAP_UNICO c&#xA;&#x9;&#x9;&#x9;on e.CATEGORIA_RC = c.CATEGORIA_RC and e.CLASSE_ATTIVATI = c.CLASSE&#xA;&#x9;left join (&#xA;&#x9;&#x9;&#x9;select distinct f.PRT_ID, f.CLASSE as CLASSE_FIRSTACT &#xA;&#x9;&#x9;&#x9;&#x9;from WTT_REV_SBF_FIRSTACT_HD_SD f ) h&#x9;&#x9;&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(h.PRT_ID as varchar(50))&#xA;&#x9;left join S_PROMO i&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(i.PROMO_ID as varchar(50))&#xA;&#x9;left join M_ATT_PROMO_PROMO_NUOVE z&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(z.PROMO_ID as varchar(50))&#xA;where i.PROMO_ID is null and c.MAP is not null&#xA;&#x9;&#x9;and z.PROMO_ID is not null&#xA;go&#xA;&#xA;&#xA;&#xA;--Vengono inserite in S_PROMO le promo nuove impostate nella maschera M_REV_PROMO_WEEKLY_RGR che non hanno una categoria e una classe per la mappatura&#xA;insert into S_PROMO&#xA;select distinct &#xA;&#x9;a.PROMO_ID, b.PRT_DES, ltrim(rtrim(a.PROMO_CLUSTER)) as PROMO_CLUSTER, ltrim(rtrim(a.PROMO_WEEKLY_RGR)) as PROMO_WEEKLY_RGR, ltrim(rtrim(a.PROMO_WEEKLY)) as PROMO_WEEKLY,&#xA;&#x9;case&#xA;&#x9;&#x9;when e.CLASSE_ATTIVATI is NULL then h.CLASSE_FIRSTACT&#xA;&#x9;&#x9;else e.CLASSE_ATTIVATI&#xA;&#x9;end as CLASSE,&#xA;&#x9;--right(a.PROMO_WEEKLY_RGR, 2) as PROMO_TYPE,&#xA;&#x9;'FA' as PROMO_TYPE,&#xA;&#x9;a.VOUCHER_NO_VOUCHER&#xA;from M_ATT_PROMO a&#xA;&#x9;left join T_REV_PRT_PROMO_TECNICA b&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(b.PRT_ID as varchar(50))&#xA;&#x9;left join (&#xA;&#x9;&#x9;&#x9;select distinct c.COD_PROMO_TEC, c.CLASSE as CLASSE_ATTIVATI, c.CATEGORIA_RC &#xA;&#x9;&#x9;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4_SQP c ) e&#xA;&#x9;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(e.COD_PROMO_TEC as varchar(50))&#xA;&#x9;left join (&#xA;&#x9;&#x9;&#x9;select distinct f.PRT_ID, f.CLASSE as CLASSE_FIRSTACT &#xA;&#x9;&#x9;&#x9;&#x9;from WTT_REV_SBF_FIRSTACT_HD_SD f ) h&#x9;&#x9;&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(h.PRT_ID as varchar(50))&#xA;&#x9;left join S_PROMO i&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(i.PROMO_ID as varchar(50))&#xA;&#x9;left join M_ATT_PROMO_PROMO_NUOVE z&#xA;&#x9;&#x9;on cast(a.PROMO_ID as varchar(50)) = cast(z.PROMO_ID as varchar(50))&#xA;where i.PROMO_ID is null and z.PROMO_ID is not null&#xA;go&#xA;&#xA;&#xA;&#xA;--riconciliazione promo_type se diverso =&gt; casistica che si verifica nel caso di inserimento nella M_REV_PROMO_WEEKLY_RGR per promo HD/SD&#xA;SELECT  PROMO_ID, COUNT(distinct PROMO_TYPE) as CNT&#xA;into&#x9;#PROMO_TYPE_DIVERSO&#xA;FROM    (&#xA;&#x9;SELECT  case when PROMO_ID like '%0000' and PROMO_ID not in (select COD_PROMO_TEC from M_REV_PROMO_0000_6666) then LEFT(PROMO_ID, len(PROMO_ID)-4)&#xA;&#x9;&#x9;&#x9;&#x9; when PROMO_ID like '%6666' and PROMO_ID not in (select COD_PROMO_TEC from M_REV_PROMO_0000_6666) then LEFT(PROMO_ID, len(PROMO_ID)-4)&#xA;&#x9;&#x9;&#x9;&#x9; else PROMO_ID end as PROMO_ID, PROMO_TYPE--, COUNT(*) as CNT&#xA;&#x9;FROM    S_PROMO&#xA;&#x9;WHERE&#x9; PROMO_ID is not null and PROMO_ID not like '-9999%'--PROMO_ID &gt; 0&#xA;&#x9;--ORDER BY 1&#xA;)a&#xA;GROUP BY PROMO_ID&#xA;having COUNT(distinct PROMO_TYPE) &gt; 1&#xA;&#xA;&#xA;UPDATE S &#xA;set&#x9;&#x9;s.PROMO_TYPE = b.PROMO_TYPE&#x9;&#xA;from (&#xA;&#x9;SELECT  S.*&#xA;&#x9;FROM    S_PROMO S &#xA;&#x9;join&#x9;M_ATT_PROMO m&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(m.PROMO_ID as varchar(50))&#xA;&#x9;left join&#x9;M_ATT_PROMO_PROMO_NUOVE r&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(r.PROMO_ID as varchar(50))&#xA;&#x9;join&#x9;#PROMO_TYPE_DIVERSO p&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID as varchar(50)) or cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID+'0000' as varchar(50)) or cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID+'6666' as varchar(50))&#xA;&#x9;WHERE&#x9;r.PROMO_ID is not null and&#xA;&#x9;&#x9;&#x9;m.VOL_ATT_PROMO = 0 &#xA;)a&#x9;&#xA;join (&#xA;SELECT  S.*&#xA;&#x9;FROM    S_PROMO S &#xA;&#x9;join&#x9;M_ATT_PROMO m&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(m.PROMO_ID as varchar(50))&#xA;&#x9;left join&#x9;M_ATT_PROMO_PROMO_NUOVE r&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(r.PROMO_ID as varchar(50))&#xA;&#x9;join&#x9;#PROMO_TYPE_DIVERSO p&#xA;&#x9;on&#x9;&#x9;cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID as varchar(50)) or cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID+'0000' as varchar(50)) or cast(s.PROMO_ID as varchar(50)) = cast(p.PROMO_ID+'6666' as varchar(50))&#xA;&#x9;WHERE&#x9;r.PROMO_ID is not null and&#xA;&#x9;&#x9;&#x9;m.VOL_ATT_PROMO &lt;&gt; 0)b&#xA;on cast(a.PROMO_ID as varchar(50)) = cast(b.PROMO_ID as varchar(50)) or cast(a.PROMO_ID+'0000' as varchar(50))=cast(b.PROMO_ID as varchar(50)) or cast(a.PROMO_ID+'6666' as varchar(50))=cast(b.PROMO_ID as varchar(50))&#xA;join S_PROMO s &#xA;on&#x9;&#x9;s.PROMO_ID = a.PROMO_ID&#xA;WHERE a.PROMO_TYPE &lt;&gt; b.PROMO_TYPE&#xA;&#xA;&#xA;drop table #PROMO_TYPE_DIVERSO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Cancella PROMO per cui è stato riusato il CODICE PROMO"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{6215EC98-7529-4EC4-8411-8DCFFA2A6E4A}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cancella PROMO per cui è stato riusato il CODICE PROMO"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="/*** CANCELLA PROMO PER CUI E' CAMBIATA LA DESCRIZIONE e QUINDI E STATO RIUSATO IL CODICE PROMO ***/&#xA;/*** LA CANCELLAZIONE VIENE EFFETTUATA SIA PER LA TABELLA S_PROMO CHE PER S_ATT_PROMO ***/&#xA;&#xA;delete from S_PROMO where PROMO_ID in (&#xA;&#x9;select distinct PROMO_ID from (&#xA;&#x9;&#x9;select distinct&#xA;&#x9;&#x9;&#x9;b.PROMO_ID&#xA;&#x9;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;&#x9;join S_PROMO b&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.COD_PROMO_TEC as varchar(50)) = cast(b.PROMO_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;join T_REV_PRT_PROMO_TECNICA c&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.COD_PROMO_TEC as varchar(50)) = cast(c.PRT_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;where b.PROMO_DES != c.PRT_DES&#xA;&#x9;&#x9;union&#x9;&#x9;&#xA;&#x9;&#x9;select distinct&#xA;&#x9;&#x9;&#x9;b.PROMO_ID&#xA;&#x9;&#x9;&#x9;from WTT_REV_SBF_FIRSTACT_HD_SD a&#xA;&#x9;&#x9;&#x9;join S_PROMO b&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.PRT_ID as varchar(50))= cast(b.PROMO_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;join T_REV_PRT_PROMO_TECNICA c&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.PRT_ID as varchar(50)) = cast(c.PRT_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;where a.GCV_ID not in (select GCV_ID from M_REV_FILTRO_GCV)&#xA;&#x9;&#x9;&#x9;&#x9;and b.PROMO_DES != c.PRT_DES ) a&#xA;)&#xA;&#xA;delete from S_ATT_PROMO where PROMO_ID in (&#xA;&#x9;select distinct PROMO_ID from (&#xA;&#x9;&#x9;select distinct&#xA;&#x9;&#x9;&#x9;b.PROMO_ID&#xA;&#x9;&#x9;&#x9;from WTT_REV_RC_ATTIVATI_STEP4 a&#xA;&#x9;&#x9;&#x9;join S_PROMO b&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.COD_PROMO_TEC as varchar(50)) = cast(b.PROMO_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;join T_REV_PRT_PROMO_TECNICA c&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.COD_PROMO_TEC as varchar(50)) = cast(c.PRT_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;where b.PROMO_DES != c.PRT_DES&#xA;&#x9;&#x9;union&#x9;&#x9;&#xA;&#x9;&#x9;select distinct&#xA;&#x9;&#x9;&#x9;b.PROMO_ID&#xA;&#x9;&#x9;&#x9;from WTT_REV_SBF_FIRSTACT_HD_SD a&#xA;&#x9;&#x9;&#x9;join S_PROMO b&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.PRT_ID as varchar(50)) = cast(b.PROMO_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;join T_REV_PRT_PROMO_TECNICA c&#xA;&#x9;&#x9;&#x9;&#x9;on cast(a.PRT_ID as varchar(50)) = cast(c.PRT_ID as varchar(50))&#xA;&#x9;&#x9;&#x9;where a.GCV_ID not in (select GCV_ID from M_REV_FILTRO_GCV)&#xA;&#x9;&#x9;&#x9;&#x9;and b.PROMO_DES != c.PRT_DES ) a&#xA;)&#x9;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Concatena il campo PROMO SKY per in caso di PROMO TECNICHE con più PROMO SKY associate"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{1a50e1e1-d6ea-404f-9b6c-0f4956a2f67d}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Concatena il campo PROMO SKY per in caso di PROMO TECNICHE con più PROMO SKY associate"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C666E1CD-96CB-4D21-939F-DB6CFC6D8583}"
          SQLTask:SqlStatementSource="/*ANCC 20170322 Concatenazione di più promo SKY associate ad una stessa promo tecnica nel campo PROMO_SKY*/&#xA;&#xA;SELECT ROW_NUMBER() OVER(PARTITION BY Promo_ID &#xA;ORDER BY Promo_ID ,PROMO_SKY) AS RowNumber,*&#xA;into #TEMP_ROW_NUMBER_ATT_PROMO&#xA;from  M_ATT_PROMO&#xA;&#xA;/* Inserimento righe con singola promo SKY*/&#xA;      &#xA;SELECT *&#xA;INTO #M_ATT_PROMO_TEST&#xA;from  #TEMP_ROW_NUMBER_ATT_PROMO&#xA;where RowNumber=1&#xA;&#xA;/* Concatenazione in caso di più di una promo sky associata */&#xA;  &#xA;DECLARE @ROW INT = 2;&#xA;DECLARE @MAXROW INT =(select MAX(RowNumber) from #TEMP_ROW_NUMBER_ATT_PROMO)&#xA;    &#xA;WHILE @ROW &lt;= @MAXROW&#xA;BEGIN&#xA;&#x9;UPDATE T&#xA;&#x9;set T.PROMO_SKY= T.PROMO_SKY +', '+P.PROMO_SKY&#xA;&#x9;from #M_ATT_PROMO_TEST T join #TEMP_ROW_NUMBER_ATT_PROMO P on T.PROMO_ID=P.PROMO_ID&#xA;&#x9;where P.RowNumber=@ROW&#xA;   &#x9;SET @ROW = @ROW + 1;&#xA;END;&#xA;&#xA;/* Aggiornamento M_ATT_PROMO */&#xA;&#xA;truncate table M_ATT_PROMO&#xA;&#xA;INSERT into M_ATT_PROMO&#xA;select PROMO_ID,SCE_ID,WEEK_ID,PROMO_CLUSTER,PROMO_WEEKLY_RGR,PROMO_WEEKLY,GESTIONE_SD_HD,VOL_ATT_PROMO,VOUCHER_NO_VOUCHER,COD_REVENUE_SHARE,PROMO_SKY&#xA;from  #M_ATT_PROMO_TEST&#xA;&#xA;/* Pulizia tabelle temporanee */&#xA;&#xA;drop table #TEMP_ROW_NUMBER_ATT_PROMO&#xA;drop table #M_ATT_PROMO_TEST&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
