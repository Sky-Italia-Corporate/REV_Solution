cross join M_REV_CALC_WEEK b
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Boxsets') bs
	on cast(a.PRT_ID as varchar(50)) = cast(bs.COD_PROMO_TEC as varchar(50))
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Skygoplus') sgp
	on cast(a.PRT_ID as varchar(50)) = cast(sgp.COD_PROMO_TEC as varchar(50))
left join (select distinct COD_PROMO_TEC from WTT_REV_RC_ATTIVATI_STEP4 where CATEGORIA_RC = 'Sconto Canone Abb. Sky Q Plus') sqp
	on cast(a.PRT_ID as varchar(50)) = cast(sqp.COD_PROMO_TEC as varchar(50))
/*** filtro inserito per far quadrare con il sistema in produzione 25/10/2012 - valutare se rimuvoere ***/

where	cast(PRT_ID as varchar(50)) in (	select distinct cast(COD_PROMO_TEC as varchar(50))
					from WTT_REV_RC_ATTIVATI_STEP4 a
					join M_REV_PROMO_MAP_UNICO b
						on a.CLASSE = b.CLASSE and a.CATEGORIA_RC = b.CATEGORIA_RC
					where b.MAP = 'FA') 
		or GCV_ID &lt;&gt; (select GCV_ID from M_REV_FILTRO_GCV)</property>
