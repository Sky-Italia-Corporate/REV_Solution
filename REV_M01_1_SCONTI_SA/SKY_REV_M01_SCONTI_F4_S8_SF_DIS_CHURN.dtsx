                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	vAtt.Period_ID as ATT_PER_ID,
	'AUTO' as TIPO_DATO, 
	'V_NO' as VOUCHER,
	volChurn.RC_PACCHETTO,
	volChurn.COD_PROMO_TEC,  
	volChurn.TECNOLOGIA,
	volChurn.CLUSTER_IPTV,
	volChurn.BB_CONTRACT_STATUS,
	volChurn.BB_PRD_ID,
	volChurn.BB_ORIGIN_CLUSTER,
	volChurn.BB_VOICE_FLG,
	volChurn.BUNDLE_FLG,
	volChurn.DATA_ATT_RC,
	volChurn.FLG_CAP_ID,
	sum(volChurn.VOLUMI_CHURN) as VOLUMI_CHURN, 
	sum(isnull(disChurn.CHURN_DISCOUNT,0)) as CHURN_DISCOUNT
from (/*** Query che recupera i volumi di churn ***/
	select
		stoAtt.ATT_PER_ID, stoAtt.COD_PROMO_TEC, stoAtt.DATA_ATT_RC,
		stoAtt.FLG_CAP_ID, stoAtt.TECNOLOGIA, stoAtt.CLUSTER_IPTV, 	
		stoAtt.BB_CONTRACT_STATUS,
		stoAtt.BB_PRD_ID,
		stoAtt.BB_ORIGIN_CLUSTER,
		stoAtt.BB_VOICE_FLG,
		stoAtt.BUNDLE_FLG,
		stoAtt.RC_PACCHETTO, count(distinct delta.ACC_ID) as VOLUMI_CHURN
	from T_SBD_DELTA delta
	--join WT_REV_STORICO_ATTIVAZIONI stoAtt
               join ST_REV_STORICO_ATTIVAZIONI stoAtt
		on delta.ACC_ID = stoAtt.ACCOUNT 
			and delta.GIO_ID &gt; stoAtt.DATA_ATT_RC
	join M_REV_CALENDARIO_SOLARE calendSol
		on stoAtt.DATA_ATT_RC = calendSol.Gio_Id
	join M_REV_FIRST_LAST_DAYS firstLastDay
		on firstLastDay.Mes2_Id = 
			case 
				when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0
					then 'M12'
				else 'M'+right(('00'+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 as varchar)), 2)
			end
		and firstLastDay.Ann_Id = 
			case 
				when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0  
					then 'FY'+right('0'+(cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)-1), 2)
				else 'FY'+right('00'+cast((cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)) as varchar), 2)
			end
	join V_M_REV_PERIOD_ATTIVAZIONE vAtt
		on vAtt.Period_ID = stoAtt.ATT_PER_ID
	, M_REV_RELEASE_DATE_MOTORE_SCONTI reldat
	where  right(left(vAtt.FiscalWeek,4),2)+right(left(vAtt.FiscalWeek,8),2)&gt;RIGHT(RELDAT.SCE_ID,2)+RIGHT(RELDAT.WEEK_ID,2)
	group by stoAtt.ATT_PER_ID, stoAtt.COD_PROMO_TEC, stoAtt.FLG_CAP_ID, stoAtt.TECNOLOGIA, stoAtt.CLUSTER_IPTV, 
			stoAtt.BB_CONTRACT_STATUS,
			stoAtt.BB_PRD_ID,
			stoAtt.BB_ORIGIN_CLUSTER,
			stoAtt.BB_VOICE_FLG,
			stoAtt.BUNDLE_FLG,stoAtt.RC_PACCHETTO, stoAtt.DATA_ATT_RC
	---------------------------------------
		/*20141021 LOSO: recupera volumi di churn utilizzando come sorgente: le cessazioni promo parco + lo storico*/
		UNION
	---------------------------------------
	select
		stoAtt.ATT_PER_ID, stoAtt.COD_PROMO_TEC, stoAtt.DATA_ATT_RC,
		stoAtt.FLG_CAP_ID, stoAtt.TECNOLOGIA, stoAtt.CLUSTER_IPTV, 
		stoAtt.BB_CONTRACT_STATUS,
			stoAtt.BB_PRD_ID,
			stoAtt.BB_ORIGIN_CLUSTER,
			stoAtt.BB_VOICE_FLG,
			stoAtt.BUNDLE_FLG,
		stoAtt.RC_PACCHETTO, 
		count(distinct promoCess.ACCOUNT) as VOLUMI_CHURN
	--from TT_REV_DT_CESSAZIONI_PROMO_PARCO_CDW_BI_CHRS promoCess
                from ST_REV_DT_CESSAZIONI_PROMO_PARCO_CDW_BI_CHRS promoCess
	--join WT_REV_STORICO_ATTIVAZIONI stoAtt
               join ST_REV_STORICO_ATTIVAZIONI stoAtt
		on promoCess.ACCOUNT = stoAtt.ACCOUNT and 
			promoCess.DATA_CHURN &gt; stoAtt.DATA_ATT_RC
	join M_REV_CALENDARIO_SOLARE calendSol
		on stoAtt.DATA_ATT_RC = calendSol.Gio_Id
	join M_REV_FIRST_LAST_DAYS firstLastDay
		on firstLastDay.Mes2_Id = 
			case 
				when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
					then 'M12'
				else 'M'+right(('00'+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 as varchar)), 2)
			end
		and firstLastDay.Ann_Id = 
			case 
				when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
					then 'FY'+right('0'+(cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)-1), 2)
				else 'FY'+right('00'+cast((cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)) as varchar), 2)
			end
	
	join V_M_REV_PERIOD_ATTIVAZIONE vAtt
		on vAtt.Period_ID = stoAtt.ATT_PER_ID
	, M_REV_RELEASE_DATE_MOTORE_SCONTI reldat
	where  right(left(vAtt.FiscalWeek,4),2)+right(left(vAtt.FiscalWeek,8),2)&gt;RIGHT(RELDAT.SCE_ID,2)+RIGHT(RELDAT.WEEK_ID,2)
	
	and   ltrim(rtrim(promoCess.CAUSALE_CESSAZIONE)) in (	select distinct ltrim(rtrim(CAUSALE_CESSAZIONE)) 
															from M_REV_CFG_CAUSALI_CESSAZIONE_PROMO_PARCO)
			and LTRIM(rtrim(promoCess.account)) not in (	select distinct acc_id 
															from T_SBD_DELTA)
	
	group by stoAtt.ATT_PER_ID, stoAtt.COD_PROMO_TEC, stoAtt.FLG_CAP_ID, stoAtt.TECNOLOGIA, stoAtt.CLUSTER_IPTV, 
			stoAtt.BB_CONTRACT_STATUS,
			stoAtt.BB_PRD_ID,
			stoAtt.BB_ORIGIN_CLUSTER,
			stoAtt.BB_VOICE_FLG,
			stoAtt.BUNDLE_FLG,
			stoAtt.RC_PACCHETTO, stoAtt.DATA_ATT_RC
) volChurn
left join (
	/*** Query per recuperare il churn discount ***/
	select
		ATT_PER_ID, COD_PROMO_TEC, DATA_ATT_RC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, 
		BB_CONTRACT_STATUS,
		BB_PRD_ID,
		BB_ORIGIN_CLUSTER,
		BB_VOICE_FLG,
		BUNDLE_FLG,
		RC_PACCHETTO, sum(VALORE_PROMO) as CHURN_DISCOUNT
	from WT_REV_VOLUMI_CHURN_X_SF
	group by ATT_PER_ID, COD_PROMO_TEC, DATA_ATT_RC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, 
			 BB_CONTRACT_STATUS,
			BB_PRD_ID,
			BB_ORIGIN_CLUSTER,
			BB_VOICE_FLG,
			BUNDLE_FLG,
			RC_PACCHETTO ) disChurn
on volChurn.ATT_PER_ID = disChurn.ATT_PER_ID and volChurn.COD_PROMO_TEC = disChurn.COD_PROMO_TEC 
	and volChurn.FLG_CAP_ID = disChurn.FLG_CAP_ID and volChurn.TECNOLOGIA = disChurn.TECNOLOGIA and volChurn.CLUSTER_IPTV = disChurn.CLUSTER_IPTV
	and volChurn.BB_CONTRACT_STATUS = disChurn.BB_CONTRACT_STATUS
	and volChurn.BB_PRD_ID = disChurn.BB_PRD_ID
	and volChurn.BB_ORIGIN_CLUSTER = disChurn.BB_ORIGIN_CLUSTER
	and volChurn.BB_VOICE_FLG = disChurn.BB_VOICE_FLG
	and volChurn.BUNDLE_FLG = disChurn.BUNDLE_FLG 
	and volChurn.RC_PACCHETTO  = disChurn.RC_PACCHETTO and volChurn.DATA_ATT_RC = disChurn.DATA_ATT_RC
--,M_REV_CALC_WEEK calcWeek
/*** Non conteggio le promo che hanno cash discount a 0 visto che non le conto nemmeno per l'indicatore
	volumi attivazioni con sconto ***/
join V_M_REV_PERIOD_ATTIVAZIONE vAtt
on volChurn.ATT_PER_ID = vAtt.Period_ID
WHERE  CHURN_DISCOUNT &lt;&gt; 0
and left(vAtt.FiscalWeek,8) = (select REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID from M_REV_CALC_WEEK)
group by vAtt.Period_ID, volChurn.RC_PACCHETTO, volChurn.COD_PROMO_TEC, volChurn.TECNOLOGIA, volChurn.CLUSTER_IPTV, 
	volChurn.BB_CONTRACT_STATUS,
	volChurn.BB_PRD_ID,
	volChurn.BB_ORIGIN_CLUSTER,
	volChurn.BB_VOICE_FLG,
	volChurn.BUNDLE_FLG,volChurn.DATA_ATT_RC, volChurn.FLG_CAP_ID</property>
