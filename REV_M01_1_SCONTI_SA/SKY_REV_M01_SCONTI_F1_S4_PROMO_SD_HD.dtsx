      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{59FF52A1-EDFE-4F39-943A-ECD611F45F26}"
          SQLTask:SqlStatementSource="/*** Salva le promo da sdoppiare automaticamente in un anagrafica temporanea ***/&#xA;select distinct&#xA;&#x9;case&#xA;&#x9;&#x9;when b.PROMO_ID is not null then b.PROMO_ID&#xA;&#x9;&#x9;else a.COD_PROMO_TEC&#xA;&#x9;end as PROMO_ID,&#xA;&#x9;c.SCE_ID,&#xA;&#x9;c.WEEK_ID,&#xA;&#x9;case&#xA;&#x9;&#x9;when b.PROMO_CLUSTER is not null then b.PROMO_CLUSTER&#xA;&#x9;&#x9;else 'NONE'&#xA;&#x9;end as PROMO_CLUSTER,&#xA;&#x9;case&#xA;&#x9;&#x9;when b.PROMO_WEEKLY_RGR is not null then b.PROMO_WEEKLY_RGR&#xA;&#x9;&#x9;else '***-NO_PROMO_SKY_RGR_'+(select distinct MAP from M_REV_PROMO_MAP_UNICO where CATEGORIA_RC = a.CATEGORIA_RC and CLASSE = a.CLASSE and MAP is not null)&#xA;&#x9;end as PROMO_WEEKLY_RGR,&#xA;&#x9;case&#xA;&#x9;&#x9;when b.PROMO_WEEKLY is not null then b.PROMO_WEEKLY&#xA;&#x9;&#x9;else (select distinct PRT_DES from T_REV_PRT_PROMO_TECNICA where PRT_ID = a.COD_PROMO_TEC)&#xA;&#x9;end as PROMO_WEEKLY,&#xA;&#x9;'Yes' as GESTIONE_SD_HD,&#xA;&#x9;0 as VOL_ATT_PROMO,&#xA;&#x9;case&#xA;&#x9;&#x9;when b.VOUCHER_NO_VOUCHER is not null then b.VOUCHER_NO_VOUCHER&#xA;&#x9;&#x9;else 'No'&#xA;&#x9;end as VOUCHER_NO_VOUCHER,&#xA;&#x9;isnull(COD_REVENUE_SHARE,'No Share') as COD_REVENUE_SHARE,&#xA;&#x9;b.PROMO_SKY /* ANCC 20170321 salita promo Sky*/&#xA;&#x9;into #TEMP_PROMO&#xA;&#x9;from WT_REV_RC_ATTIVATI a&#xA;&#x9;left join M_ATT_PROMO b&#xA;&#x9;&#x9;on a.COD_PROMO_TEC = b.PROMO_ID&#xA;&#x9;,M_REV_CALC_WEEK c&#xA;&#x9;where a.CATEGORIA_RC like '%Canone Abb. HD%'&#xA;&#xA;/*** Cancella da M_ATT_PROMO le PROMO che devono essere sdoppiate automaticamente ***/&#xA;delete from M_ATT_PROMO&#xA;&#x9;where PROMO_ID in (&#xA;&#x9;&#x9;select distinct COD_PROMO_TEC&#xA;&#x9;&#x9;&#x9;from WT_REV_RC_ATTIVATI&#xA;&#x9;&#x9;&#x9;where CATEGORIA_RC like '%Canone Abb. HD%' )&#xA;&#xA;/*** Riporta le promo dall'anagrafica temporanea ***/&#x9;&#x9;&#x9;&#xA;insert into M_ATT_PROMO&#xA;select * from #TEMP_PROMO&#xA;&#xA;drop table #TEMP_PROMO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
