	on tcn.TCN_ID = rcFA.TCN_ID
join M_ATT_PRODOTTO prod
	on (cast(PRT_ID as varchar(40)) + '-' + GCV_ID) = prod.RC_COD	
join M_REV_CALENDARIO_SOLARE solare 
	on rcFA.DAT_ATT = solare.Gio_Id
join M_REV_FIRST_LAST_DAYS first_last 
	on first_last.Mes2_Id = 
		case 
			when (cast(right(ltrim(rtrim(solare.Mes2_Id)), 2) as integer)+isnull(prod.OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'M12'
			else 'M'+right(('00'+cast((cast(right(ltrim(rtrim(solare.Mes2_Id)), 2) as integer)+isnull(prod.OFFSET_INIZIO_MESI, 0))%12 as varchar)), 2)
		end
	and first_last.Ann_Id = 
		case 
			when (cast(right(ltrim(rtrim(solare.Mes2_Id)), 2) as integer)+isnull(prod.OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'FY'+right('0'+(cast(right(ltrim(rtrim(solare.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(solare.Mes2_Id)), 2) as integer)+isnull(prod.OFFSET_INIZIO_MESI, 0))/12 as integer)-1), 2)
			else 'FY'+right('00'+cast((cast(right(ltrim(rtrim(solare.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(solare.Mes2_Id)), 2) as integer)+isnull(prod.OFFSET_INIZIO_MESI, 0))/12 as integer)) as varchar), 2)
		end  
join WT_REV_CASH_DISCOUNT_MGMT disMGMT
  on (cast(PRT_ID as varchar(40)) + '-' + GCV_ID)= disMGMT.RC_COD
	  and rcFA.PCK_ID = disMGMT.RC_PACCHETTO
	  and rcFA.PRT_ID = disMGMT.COD_PROMO_TEC
	  and tcn.TCN_DES = disMGMT.TECNOLOGIA
	  and rcFA.CLU_IPTV = disMGMT.CLUSTER_IPTV
	  and rcFA.BB_CONTRACT_STATUS = disMGMT.BB_CONTRACT_STATUS
	and rcFA.BB_PRD_ID = disMGMT.BB_PRD_ID
	and rcFA.BB_ORIGIN_CLUSTER = disMGMT.BB_ORIGIN_CLUSTER
	and rcFA.BB_VOICE_FLG = disMGMT.BB_VOICE_FLG
	and rcFA.BUNDLE_FLG = disMGMT.BUNDLE_FLG
	  and rcFA.DAT_ATT = disMGMT.DATA_ATT_RC
	  and rcFA.FLG_CAP_ID = disMGMT.FLG_CAP_ID
group by disMGMT.RC_COD,
		 disMGMT.RC_PACCHETTO,
		 disMGMT.COD_PROMO_TEC,
		 tcn.TCN_DES,
		 rcFA.CLU_IPTV,
		 rcFA.BB_CONTRACT_STATUS,
		 rcFA.BB_PRD_ID,
		 rcFA.BB_ORIGIN_CLUSTER,
		 rcFA.BB_VOICE_FLG,
		 rcFA.BUNDLE_FLG,
		 rcFA.DAT_ATT,  
		 disMGMT.DATA_INI,
		 disMGMT.DATA_FIN,
		 disMGMT.DURATA_GIORNI,
		 disMGMT.DURATA_MESI,
		 disMGMT.PRO_RATA,
		 disMGMT.DATA_ATT_RC,
		 disMGMT.DATA_CES_RC) [refTable]
