	cast(isnull(rcAtt.DATA_ATT_RC,'99991231') as varchar(8)) as DATA_ATT_RC_ORIG,
	FLG_CAP_ID, -- da utilizzare solo per il calcolo degli indicatori Durata_RGT_Promo, ARPU_FA_NET_MGMG e ARPU_FA_NET_USGAAP
