                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
	cast(isnull(rcAtt.ACCOUNT,'0') as varchar(10)) as ACCOUNT,	
	cast(isnull(rcAtt.DATA_ATT,'99991231') as varchar(8)) as DATA_ATT,
	cast(isnull(dtrs.RC_PACCHETTO,'Undefined') as varchar(100)) as RC_PCK_VISIONE, 
	cast(isnull(rcAtt.RC_PACCHETTO,'Undefined') as varchar(100)) as RC_PCK_REALE,
	cast(isnull(rcAtt.VALORE_PACCHETTO,'0') as decimal(20,10)) as VALORE_PACCHETTO_REALE,
	cast(isnull(rcAtt.COD_LISTINO,'Undefined') as varchar(100)) as COD_LISTINO,
	cast(isnull(rcAtt.COD_PROMO_TEC,'Undefined') as varchar(100))  as COD_PROMO_TEC,
	cast(isnull(rcAtt.COD_PROMO_SKY,'Undefined') as varchar(100)) as COD_PROMO_SKY,
	cast(isnull(rcAtt.RC_COD,'Undefined') as varchar(100)) as RC_SCONTO,
	cast(isnull(rcAtt.DATA_ATT_RC,'99991231') as varchar(8)) as DATA_ATT_RC_ORIG,
	FLG_CAP_ID, -- da utilizzare solo per il calcolo degli indicatori Durata_RGT_Promo, ARPU_FA_NET_MGMG e ARPU_FA_NET_USGAAP
	rcAtt.TECNOLOGIA,
	isnull(rcAtt.CLUSTER_IPTV, '***-NO_CLUSTER') as CLUSTER_IPTV,
	cast(isnull(rcAtt.BB_CONTRACT_STATUS, 'ZZZ_NO_BB_CONTRACT_STATUS') as varchar(50)) as BB_CONTRACT_STATUS,
	cast(isnull(rcAtt.BB_PRD_ID, 'ZZZ_NO_BB_PRODUCT') as varchar(80)) as BB_PRD_ID,
	cast(isnull(rcAtt.BB_ORIGIN_CLUSTER, 'ZZZ_NO_BB_PRODUCT') as varchar(80)) as BB_ORIGIN_CLUSTER,
	cast(isnull(rcAtt.BB_VOICE_FLG, 'N') as varchar(4)) as BB_VOICE_FLG,
	cast(isnull(rcAtt.BUNDLE_FLG, 'No Bundle') as varchar(80)) as BUNDLE_FLG
from WTT_REV_RC_ATTIVATI_STEP4_SQP rcAtt
join S_PROMO promo
	on rcAtt.COD_PROMO_TEC = promo.PROMO_ID
join (	select distinct account, rc_pacchetto 
		from T_REV_DTRS) dtrs
	on rcAtt.ACCOUNT = dtrs.ACCOUNT
where promo.PROMO_TYPE &lt;&gt; 'FA'</property>
