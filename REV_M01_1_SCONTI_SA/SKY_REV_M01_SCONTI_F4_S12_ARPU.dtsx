            <component
              refId="Package\popola WT_REV_EXPORT_DTCD_CALCOLO_SCONTO\Seleziono gli ACCOUNT con Voucher da WTT_REV_SBF_FIRSTACT_HD_SD"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              localeId="1033"
              name="Seleziono gli ACCOUNT con Voucher da WTT_REV_SBF_FIRSTACT_HD_SD"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	cast(isnull(firstact.ACC_ID,'0') as varchar(10)) as ACCOUNT,
	cast(isnull(firstact.DAT_ATT,'99991231') as varchar(8)) as DATA_ATT,
	cast(isnull(sky_sa_firstact.PVN_ID,'Undefined') as varchar(100)) as RC_PCK_VISIONE, 
	cast(isnull(PCK_ID,'Undefined') as varchar(100)) as RC_PCK_REALE, 
	cast(isnull(convert(decimal(18,10),(max(INP_CNN) * iva.VAL_IVA)),'0') as decimal(20,10)) as VALORE_PACCHETTO_REALE,
	cast(isnull(LST_ID ,'Undefined') as varchar(100)) as COD_LISTINO,
	cast(isnull(PRT_ID ,'Undefined') as varchar(100)) as COD_PROMO_TEC,
	cast(isnull(PRS_ID ,'Undefined') as varchar(100)) as COD_PROMO_SKY,
	cast(isnull((cast(PRT_ID as varchar(40)) + '-' + GCV_ID),'Undefined') as varchar(100)) as RC_SCONTO,
	cast(isnull( firstact.DAT_ATT,'99991231') as varchar(8)) as DATA_ATT_RC_ORIG,
	FLG_CAP_ID, -- da utilizzare solo per il calcolo degli indicatori Durata_RGT_Promo, ARPU_FA_NET_MGMG e ARPU_FA_NET_USGAAP	
	cast(isnull(tcn.TCN_DES,'***-NO_TECNOLOGIA') as varchar(50)) as TECNOLOGIA,
	cast(isnull(firstact.CLU_IPTV, '***-NO_CLUSTER') as varchar(50)) as CLUSTER_IPTV,
	cast(isnull(firstact.BB_CONTRACT_STATUS, 'ZZZ_NO_BB_CONTRACT_STATUS') as varchar(50)) as BB_CONTRACT_STATUS,
	cast(isnull(firstact.BB_PRD_ID, 'ZZZ_NO_BB_PRODUCT') as varchar(80)) as BB_PRD_ID,
	cast(isnull(firstact.BB_ORIGIN_CLUSTER, 'ZZZ_NO_BB_PRODUCT') as varchar(80)) as BB_ORIGIN_CLUSTER,
	cast(isnull(firstact.BB_VOICE_FLG, 'N') as varchar(4)) as BB_VOICE_FLG,
	cast(isnull(firstact.BUNDLE_FLG, 'No Bundle') as varchar(80)) as BUNDLE_FLG

from WTT_REV_SBF_FIRSTACT_HD_SD firstact	
left join S_REV_TCN_TECNOLOGIA tcn 
	on tcn.TCN_ID = firstact.TCN_ID
join S_PROMO promo
	on firstact.PRT_ID = promo.PROMO_ID
join (	select distinct acc_id, dat_att, dat_ven, gio_id, pvn_id 
		from SKY_SA.dbo.SF_SBF_FIRSTACT) sky_sa_firstact
	on firstact.ACC_ID = sky_sa_firstact.acc_id
		and firstact.DAT_ATT = sky_sa_firstact.DAT_ATT
		and firstact.DAT_VEN = sky_sa_firstact.DAT_VEN
		and firstact.GIO_ID = sky_sa_firstact.GIO_ID	
cross join (	select top 1 VAL_IVA 
				from M_REV_VALORE_IVA 
				where APPL_NAME = 'Act_Promo') iva
where promo.PROMO_TYPE = 'FA'
