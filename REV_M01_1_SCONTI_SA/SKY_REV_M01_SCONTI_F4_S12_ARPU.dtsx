                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with TEMP_CALCOLO_ARPU as (
select 
	wt.ACCOUNT,
	V.Period_ID as ATT_PER_ID,
	RC_PCK_REALE,
	cast((VALORE_PACCHETTO_REALE/iva.VAL_IVA ) as decimal(20,10)) as VALORE_PACCHETTO_REALE,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	BB_CONTRACT_STATUS,
	BB_PRD_ID,
	BB_ORIGIN_CLUSTER,
	BB_VOICE_FLG,
	BUNDLE_FLG,
	wt.COD_PROMO_TEC,
	DURATA_PROMO_GG,
	SCONTO_MESE_MGMT,
	SCONTO_MESE_USGAAP,
	MAX(DURATA_PROMO_GG) OVER( partition by wt.ACCOUNT,
	V.Period_ID,
	RC_PCK_REALE,
	VALORE_PACCHETTO_REALE,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	BB_CONTRACT_STATUS,
	BB_PRD_ID,
	BB_ORIGIN_CLUSTER,
	BB_VOICE_FLG,
	BUNDLE_FLG,
	wt.COD_PROMO_TEC) as DURATA_PROMO_GG_MAX
from WT_REV_EXPORT_DTCD_CALCOLO_SCONTO WT
join V_M_REV_PERIOD_ATTIVAZIONE V
on cast(WT.DATA_ATT_ORIG as datetime) between V.PeriodStart and V.PeriodEnd
cross join (select top 1 VAL_IVA from M_REV_VALORE_IVA where APPL_NAME = 'Act_Promo') iva
left join (
	select distinct ACCOUNT,COD_PROMO_TEC from WT_REV_EXPORT_DTCD_CALCOLO_SCONTO group by ACCOUNT, COD_PROMO_TEC having COUNT(*) &gt; 1) promo_multiple
on wt.ACCOUNT = promo_multiple.ACCOUNT
and wt.COD_PROMO_TEC = promo_multiple.COD_PROMO_TEC
where promo_multiple.ACCOUNT is not null
)

select ATT_PER_ID,
	RC_PCK_REALE,
	VALORE_PACCHETTO_REALE,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	BB_CONTRACT_STATUS,
	BB_PRD_ID,
	BB_ORIGIN_CLUSTER,
	BB_VOICE_FLG,
	BUNDLE_FLG,
	COD_PROMO_TEC,
	DURATA_PROMO_GG_MAX as DURATA_PROMO_GG,
	SUM(SCONTO_MESE_MGMT*DURATA_PROMO_GG)/(DURATA_PROMO_GG_MAX) as SCONTO_MESE_MGMT,
	SUM(SCONTO_MESE_USGAAP*(case
								when DURATA_PROMO_GG &gt;= 365 then DURATA_PROMO_GG else 365+(365/(12*2)) end)
							)/(case 
								when DURATA_PROMO_GG_MAX &gt;= 365 then DURATA_PROMO_GG_MAX else 365+(365/(12*2)) end)
	 as SCONTO_MESE_USGAAP
	 from TEMP_CALCOLO_ARPU
group by ACCOUNT,
	ATT_PER_ID,
	RC_PCK_REALE,
	VALORE_PACCHETTO_REALE,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	BB_CONTRACT_STATUS,
	BB_PRD_ID,
	BB_ORIGIN_CLUSTER,
	BB_VOICE_FLG,
	BUNDLE_FLG,
	COD_PROMO_TEC,
	DURATA_PROMO_GG_MAX</property>
