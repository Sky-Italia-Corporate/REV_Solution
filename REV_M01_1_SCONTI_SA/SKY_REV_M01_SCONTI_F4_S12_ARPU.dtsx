from WTT_REV_RC_ATTIVATI_STEP4_SQP rcAtt
join S_PROMO promo
	on rcAtt.COD_PROMO_TEC = promo.PROMO_ID
join (	select distinct account, rc_pacchetto 
		from T_REV_DTRA) dtra
	on rcAtt.ACCOUNT = dtra.ACCOUNT
where  promo.PROMO_TYPE = 'FA'</property>
