      DTS:LocaleID="-1"
      DTS:ObjectName="Dimezza volumi di ultima settimana e, se presente, prima settimana"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8A40438F-0E0B-4C21-9319-D5AFCB1D71A8}"
          SQLTask:SqlStatementSource="/*** RIPORTA I VOLUMI DALLA TABELLA DEI VOLUMI DI CHURN ****/&#xA;update WT_REV_VOLUMI_CHURN_X_SF&#xA;set VOLUMI = volChurn.VOLUMI&#xA;from WT_REV_VOLUMI_CHURN_X_SF volChurnSF&#xA;join (&#x9;select &#xA;&#x9;&#x9;&#x9;volChurn.ATT_PER_ID,&#xA;&#x9;&#x9;&#x9;vAtt.Period_ID as PER_ATT_RC,&#xA;&#x9;&#x9;&#x9;volChurn.COD_PROMO_TEC,&#xA;&#x9;&#x9;&#x9;volChurn.FLG_CAP_ID,&#xA;&#x9;&#x9;&#x9;volChurn.TECNOLOGIA, &#xA;            volChurn.DATA_ATT_RC, /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;&#x9;&#x9;&#x9;volChurn.CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;volChurn.BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;volChurn.BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;volChurn.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;volChurn.BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;volChurn.BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;volChurn.RC_COD,&#xA;&#x9;&#x9;&#x9;volChurn.RC_PACCHETTO,&#xA;&#x9;&#x9;&#x9;cast((sum(volChurn.VOLUMI) * (-1)) as decimal (18, 2))  as VOLUMI,&#xA;&#x9;&#x9;&#x9;volChurn.SORGENTE_DATI /*LOSO 20141014*/&#xA;&#x9;&#x9;from WT_REV_VOLUMI_CHURN volChurn&#xA;&#x9;&#x9;join M_REV_TEMPO mTempo&#xA;&#x9;&#x9;&#x9;on volChurn.DATA_ATT_RC = mTempo.GIO_ID&#xA;&#x9;&#x9;inner hash join (&#x9;select distinct&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ATT_PER_ID, RC_COD, OFFSET_INIZIO_MESI, OFFSET_INIZIO_GIORNI, DURATA_MESI, &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DURATA_GIORNI, SCADENZA, PRO_RATA&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from WT_REV_STORICO_ATTIVAZIONI) stoAtt&#xA;&#x9;&#x9;&#x9;on volChurn.ATT_PER_ID = stoAtt.ATT_PER_ID and &#xA;&#x9;&#x9;&#x9;&#x9;volChurn.RC_COD = stoAtt.RC_COD&#xA;&#x9;&#x9;join (&#x9;select a.Gio_Id, b.FIRST_DAY &#xA;&#x9;&#x9;&#x9;&#x9;from M_REV_CALENDARIO_SOLARE a&#xA;&#x9;&#x9;&#x9;&#x9;join M_REV_FIRST_LAST_DAYS b&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;on right(a.Next_Ann_NC_Id, 2)+right(a.Next_Mes_NC_Id, 2) = right(b.Ann_Id, 2)+right(b.Mes2_Id, 2)) firstLastDay--f&#xA;&#x9;&#x9;&#x9;on volChurn.DATA_ATT_RC = firstLastDay.GIO_ID&#xA;&#x9;&#x9;join M_REV_TEMPO dateAtt&#xA;&#x9;&#x9;&#x9;on case&#xA;&#x9;&#x9;&#x9;&#x9;when isnull(stoAtt.OFFSET_INIZIO_MESI, 0) &lt;&gt; 0 or isnull(stoAtt.OFFSET_INIZIO_GIORNI, 0) &lt;&gt; 0 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;or stoAtt.PRO_RATA = 1 or isnull(stoAtt.SCADENZA,0) &lt;&gt; 0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;then volChurn.DATA_ATT_RC &#xA;&#x9;&#x9;&#x9;&#x9;else firstLastDay.FIRST_DAY&#xA;&#x9;&#x9;&#x9;end = dateAtt.GIO_ID&#xA;&#x9;&#x9;join V_M_REV_PERIOD_ATTIVAZIONE vAtt&#xA;&#x9;&#x9;&#x9;on cast(dateAtt.GIO_ID as datetime) between vAtt.PeriodStart and vAtt.PeriodEnd&#xA;&#x9;&#x9;group by volChurn.ATT_PER_ID,&#xA;&#x9;&#x9;&#x9;vAtt.Period_ID,&#xA;&#x9;&#x9;&#x9;volChurn.COD_PROMO_TEC,&#xA;&#x9;&#x9;&#x9;volChurn.FLG_CAP_ID,&#xA;&#x9;&#x9;&#x9;volChurn.TECNOLOGIA,&#xA;            volChurn.DATA_ATT_RC, /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;&#x9;&#x9;&#x9;volChurn.CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;volChurn.BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;volChurn.BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;volChurn.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;volChurn.BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;volChurn.BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;volChurn.RC_COD,&#xA;&#x9;&#x9;&#x9;volChurn.RC_PACCHETTO,&#xA;&#x9;&#x9;&#x9;volChurn.SORGENTE_DATI /*LOSO 20141014*/) volChurn&#xA;on volChurnSF.ATT_PER_ID = volChurn.ATT_PER_ID and volChurnSF.COD_PROMO_TEC = volChurn.COD_PROMO_TEC&#xA;&#x9;and volChurnSF.FLG_CAP_ID = volChurn.FLG_CAP_ID and volChurnSF.TECNOLOGIA = volChurn.TECNOLOGIA and volChurnSF.CLUSTER_IPTV = volChurn.CLUSTER_IPTV&#xA;&#x9;and volChurnSF.BB_CONTRACT_STATUS = volChurn.BB_CONTRACT_STATUS&#xA;&#x9;and volChurnSF.BB_PRD_ID = volChurn.BB_PRD_ID&#xA;&#x9;and volChurnSF.BB_ORIGIN_CLUSTER = volChurn.BB_ORIGIN_CLUSTER&#xA;&#x9;and volChurnSF.BB_VOICE_FLG = volChurn.BB_VOICE_FLG &#xA;&#x9;and volChurnSF.BUNDLE_FLG = volChurn.BUNDLE_FLG&#xA;&#x9;and volChurnSF.RC_PACCHETTO = volChurn.RC_PACCHETTO and volChurnSF.RC_COD = volChurn.RC_COD and volChurnSF.RC_PACCHETTO = volChurn.RC_PACCHETTO &#xA;&#x9;and volChurnSF.PER_ATT_RC = volChurn.PER_ATT_RC and volChurnSF.DATA_ATT_RC = volChurn.DATA_ATT_RC /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;&#xA;/*** DIMEZZA I VOLUMI DELLA PRIMA SETTIMANA ***/&#xA;update WT_REV_VOLUMI_CHURN_X_SF&#xA;set VOLUMI = (VOLUMI / 2.00)&#xA;where CMP_PER_ID = PER_ATT_RC&#xA;&#xA;/*** DIMEZZA I VOLUMI DELL'ULTIMA SETTIMANA ***/&#xA;update WT_REV_VOLUMI_CHURN_X_SF&#xA;set VOLUMI = (VOLUMI / 2.00)&#xA;from WT_REV_VOLUMI_CHURN_X_SF volChurnSF&#xA;&#x9;join (&#x9;select&#xA;&#x9;&#x9;&#x9;&#x9;ATT_PER_ID,&#xA;&#x9;&#x9;&#x9;&#x9;RC_PACCHETTO, &#xA;&#x9;&#x9;&#x9;&#x9;RC_COD, &#xA;&#x9;&#x9;&#x9;&#x9;COD_PROMO_TEC,&#xA;&#x9;&#x9;&#x9;&#x9;FLG_CAP_ID,&#xA;&#x9;&#x9;&#x9;&#x9;TECNOLOGIA,&#xA;                DATA_ATT_RC, /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;&#x9;&#x9;&#x9;&#x9;CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;&#x9;BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;&#x9;BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;&#x9;BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;&#x9;BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;max(CMP_PER_ID) as LAST_PER,&#xA;&#x9;&#x9;&#x9;&#x9;SORGENTE_DATI /*LOSO 20141014*/&#xA;&#x9;&#x9;&#x9;from WT_REV_VOLUMI_CHURN_X_SF&#xA;&#x9;&#x9;&#x9;group by ATT_PER_ID,&#xA;&#x9;&#x9;&#x9;&#x9;RC_PACCHETTO, &#xA;&#x9;&#x9;&#x9;&#x9;RC_COD, &#xA;&#x9;&#x9;&#x9;&#x9;COD_PROMO_TEC,&#xA;&#x9;&#x9;&#x9;&#x9;FLG_CAP_ID,&#xA;&#x9;&#x9;&#x9;&#x9;TECNOLOGIA,&#xA;                DATA_ATT_RC, /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;&#x9;&#x9;&#x9;&#x9;CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;&#x9;BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;&#x9;BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;&#x9;BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;&#x9;BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;SORGENTE_DATI /*LOSO 20141014*/ ) volChurnLastWeek&#xA;&#x9;&#x9;on volChurnSF.ATT_PER_ID = volChurnLastWeek.ATT_PER_ID and volChurnSF.RC_PACCHETTO = volChurnLastWeek.RC_PACCHETTO&#xA;&#x9;&#x9;&#x9;and volChurnSF.RC_COD = volChurnLastWeek.RC_COD and volChurnSF.COD_PROMO_TEC = volChurnLastWeek.COD_PROMO_TEC&#xA;&#x9;&#x9;&#x9;/* FRMA Progetto Telecom */&#xA;&#x9;&#x9;&#x9;and volChurnSF.FLG_CAP_ID = volChurnLastWeek.FLG_CAP_ID &#xA;&#x9;&#x9;&#x9;and volChurnSF.TECNOLOGIA = volChurnLastWeek.TECNOLOGIA&#xA;&#x9;&#x9;&#x9;and volChurnSF.CLUSTER_IPTV = volChurnLastWeek.CLUSTER_IPTV&#xA;&#x9;&#x9;&#x9;and volChurnSF.BB_CONTRACT_STATUS = volChurnLastWeek.BB_CONTRACT_STATUS&#xA;&#x9;&#x9;&#x9;and volChurnSF.BB_PRD_ID = volChurnLastWeek.BB_PRD_ID&#xA;&#x9;&#x9;&#x9;and volChurnSF.BB_ORIGIN_CLUSTER = volChurnLastWeek.BB_ORIGIN_CLUSTER&#xA;&#x9;&#x9;&#x9;and volChurnSF.BB_VOICE_FLG = volChurnLastWeek.BB_VOICE_FLG&#xA;&#x9;&#x9;&#x9;and volChurnSF.BUNDLE_FLG = volChurnLastWeek.BUNDLE_FLG&#xA;&#x9;&#x9;&#x9;and volChurnSF.SORGENTE_DATI = volChurnLastWeek.SORGENTE_DATI&#xA;            and volChurnSF.DATA_ATT_RC = volChurnLastWeek.DATA_ATT_RC /* MAMU 20171129 Introdotta per ristrutturazione del tempo */&#xA;where CMP_PER_ID = volChurnLastWeek.LAST_PER" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
