                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*** Oltre a recuperare le informazioni sulle attivazioni degli account che hanno churnato nella week di riferimento recupero già anche le date 
che eventualmente andrò ad utilizzare come DATA_ATT_RC nel caso in cui il rate code di
 attivazione di sconto abbia impostato un OFFSET_INIZIO_MESI o OFFSET_INIZIO_GIORNI ***/
 
select
	stoAtt.ATT_PER_ID,
	delta.GIO_ID as DATA_CHURN,
	stoAtt.ACCOUNT,
	stoAtt.COD_PROMO_TEC,
	stoAtt.FLG_CAP_ID,
	stoAtt.TECNOLOGIA,
	stoAtt.CLUSTER_IPTV,
	stoAtt.BB_CONTRACT_STATUS,
	stoAtt.BB_PRD_ID,
	stoAtt.BB_ORIGIN_CLUSTER,
	stoAtt.BB_VOICE_FLG,
	stoAtt.BUNDLE_FLG,
	stoAtt.RC_COD,
	stoAtt.DATA_ATT_RC,
	stoAtt.RC_PACCHETTO,
	stoAtt.OFFSET_INIZIO_MESI,
	stoAtt.OFFSET_INIZIO_GIORNI,
	stoAtt.DURATA_MESI,
	stoAtt.DURATA_GIORNI,
	stoAtt.SCADENZA,
	stoAtt.PRO_RATA,
	firstDay.FIRST_DAY, 
	convert(varchar, dateadd(day, cast(OFFSET_INIZIO_GIORNI as int), DATA_ATT_RC), 112) as DATA_ATT_RC_OFFSET,
	stato.STA_LEV0_DES as SORGENTE_DATI /*ANCC 20170403 Improvement Maschera Churn*/
from WT_REV_STORICO_ATTIVAZIONI stoAtt
join  T_SBD_DELTA delta 
	/*** Devono essere churnate solo le promo attivate precedentemente al movimento di churn ***/
	on	stoAtt.ACCOUNT = delta.ACC_ID and 
		stoAtt.DATA_ATT_RC &lt; delta.GIO_ID
join M_REV_CALENDARIO_SOLARE calcWeek 
	on stoAtt.DATA_ATT_RC = calcWeek.Gio_Id
join M_REV_FIRST_LAST_DAYS firstDay
	on firstDay.Mes2_Id = 
		case 
			when (cast(right(ltrim(rtrim(calcWeek.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'M12'
			else 'M'+right(('00'+cast((cast(right(ltrim(rtrim(calcWeek.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 as varchar)), 2)
		end
	and firstDay.Ann_Id = 
		case 
			when (cast(right(ltrim(rtrim(calcWeek.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'FY'+right('0'+(cast(right(ltrim(rtrim(calcWeek.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calcWeek.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)-1), 2)
			else 'FY'+right('00'+cast((cast(right(ltrim(rtrim(calcWeek.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calcWeek.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)) as varchar), 2)
		end

LEFT JOIN SKY_SA.dbo.S_SUB_STA_STATO stato
on stato.STA_ID=delta.STA_ID  /*ANCC 20170403 Improvement Maschera Churn*/
where stoAtt.ACCOUNT in (	select distinct ACC_ID 
							from T_SBD_DELTA)</property>
