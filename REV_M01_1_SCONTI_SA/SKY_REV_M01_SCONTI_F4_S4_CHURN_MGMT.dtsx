      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8A40438F-0E0B-4C21-9319-D5AFCB1D71A8}"
          SQLTask:SqlStatementSource="/*** Salva nella tabella di storico le attivazioni che ho churnato nella week precedente ***/&#xA;insert into WT_REV_STORICO_ATTIVAZIONI_CHURNATE&#xA;select&#xA;&#x9;stoAtt.ATT_PER_ID,&#xA;                calcWeek.SCE_ID, calcWeek.WEEK_ID,&#xA;&#x9;stoAtt.ACCOUNT, stoAtt.COD_PROMO_TEC, &#xA;&#x9;stoAtt.RC_COD, stoAtt.DATA_ATT, stoAtt.DATA_ATT_RC, stoAtt.RC_PACCHETTO, &#xA;&#x9;stoAtt.VALORE_PACCHETTO, stoAtt.VALORE, stoAtt.OFFSET_INIZIO_MESI, stoAtt.OFFSET_INIZIO_GIORNI, &#xA;&#x9;stoAtt.DURATA_MESI, stoAtt.DURATA_GIORNI, stoAtt.SCADENZA, stoAtt.PRO_RATA, &#xA;&#x9;stoAtt.FLG_CAP_ID, stoAtt.TECNOLOGIA, stoAtt.CLUSTER_IPTV&#xA;from WT_REV_STORICO_ATTIVAZIONI stoAtt&#xA;join WT_REV_APPOGGIO_ACCOUNT_CHURN appAccChurn&#xA;&#x9;on&#x9;stoAtt.ACCOUNT = appAccChurn.ACCOUNT and &#xA;&#x9;&#x9;stoAtt.DATA_ATT_RC &lt; appAccChurn.GIO_ID&#xA;cross join M_REV_CALC_WEEK calcWeek&#xA;where appAccChurn.ATT_PER_ID not in (&#xA;&#x9;&#x9;&#x9;select distinct Period_ID&#xA;&#x9;&#x9;&#x9;from V_M_REV_PERIOD_ATTIVAZIONE V&#xA;&#x9;&#x9;&#x9;join M_REV_CALC_WEEK M&#xA;&#x9;&#x9;&#x9;on left(v.FiscalWeek,8) = (select REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID from M_REV_CALC_WEEK)&#xA;&#x9;&#x9;&#x9;)&#xA;&#xA;/*** Cancella dalla tabella dello storico attivazioni quelle che ho churnato nella week precedente ***/&#xA;delete WT_REV_STORICO_ATTIVAZIONI&#xA;from WT_REV_STORICO_ATTIVAZIONI stoAtt&#xA;join WT_REV_APPOGGIO_ACCOUNT_CHURN appAccChurn&#xA;&#x9;on&#x9;stoAtt.ACCOUNT = appAccChurn.ACCOUNT &#xA;&#x9;&#x9;and stoAtt.DATA_ATT_RC &lt; appAccChurn.GIO_ID&#xA;where appAccChurn.ATT_PER_ID not in (&#xA;&#x9;&#x9;&#x9;select distinct Period_ID&#xA;&#x9;&#x9;&#x9;from V_M_REV_PERIOD_ATTIVAZIONE V&#xA;&#x9;&#x9;&#x9;join M_REV_CALC_WEEK M&#xA;&#x9;&#x9;&#x9;on left(v.FiscalWeek,8) = (select REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID from M_REV_CALC_WEEK)&#xA;&#x9;&#x9;&#x9;)&#xA;&#xA;/*** Cancella la tabella di appoggio che contiene gli account che hanno churnato la week precedente ***/&#xA;truncate table WT_REV_APPOGGIO_ACCOUNT_CHURN&#xA;&#xA;/*** Inserisce nella tabella di appoggio gli account che churnano questa week ***/&#xA;insert into WT_REV_APPOGGIO_ACCOUNT_CHURN&#xA;select V.Period_ID as ATT_PER_ID, delta.GIO_ID, delta.ACC_ID&#xA;from T_SBD_DELTA delta &#xA;join V_M_REV_PERIOD_ATTIVAZIONE V&#xA;on cast(delta.GIO_ID as datetime) between V.PeriodStart and V.PeriodEnd" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
