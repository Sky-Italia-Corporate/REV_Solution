                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*** Query che calcola la competenziazione negativa delle code per gli account che hanno churnato ***/
select
	volChurn.SORGENTE_DATI, /*LOSO 20141014 riporto il flag per differenziare i due flussi provenienti da DELTA e CESSAZIONI PROMO PARCO*/
	volChurn.ATT_PER_ID,
	compMGMT.CMP_PER_ID,
	compMGMT.RC_PACCHETTO,
	compMGMT.RC_COD,
	cast(storico.PROMO_ID as varchar(80)) as PROMO_ID,	
	compMGMT.FLG_CAP_ID,
	compMGMT.TECNOLOGIA,
    compMGMT.DATA_ATT_RC, /*MAMU 20171129 aggiunto campo per valorizzazione nella SF_ATTIVAZIONI */
	compMGMT.CLUSTER_IPTV,
	vAtt2.Period_ID as PER_ATT_RC,
/*** I volumi vengono calcolati ma poi verranno sostituiti dalla query di update in quanto calcolati tramite il
max durante l'aggregazione non risultano corretti ma devono essere riportati dalla tabella 
WT_REV_VOLUMI_CHURN ***/
	(max(volChurn.VOLUMI)*(-1)) as VOLUMI,
/*** Il campo compMGMT.VOLUMI sono i volumi totali calcolati nella tabella dello storico di competenziazione. Di 
conseguenza quel valore di sconto deve essere diviso per questi volumi e per i giorni della settimana per cui
quella competenziazione è stata storicizzata. Il valore deve essere poi moltiplicato per i volumi di churn 
rappresentati dal campo volChurn.VOLUMI ***/
	sum((compMGMT.SCONTO_PROMO / compMGMT.GIORNI_SETTIMANA / compMGMT.VOLUMI) * (-1) * volChurn.VOLUMI) as SCONTO_PROMO
from WT_REV_VOLUMI_CHURN volChurn
	inner hash join WT_REV_STORICO_COMPETENZIAZIONE_MGMT compMGMT
		on volChurn.ATT_PER_ID = compMGMT.ATT_PER_ID and volChurn.COD_PROMO_TEC = compMGMT.COD_PROMO_TEC
			and volChurn.RC_COD = compMGMT.RC_COD and volChurn.DATA_ATT_RC = compMGMT.DATA_ATT_RC and volChurn.RC_PACCHETTO = compMGMT.RC_PACCHETTO
			and volChurn.FLG_CAP_ID = compMGMT.FLG_CAP_ID
			and volChurn.TECNOLOGIA = compMGMT.TECNOLOGIA and volChurn.CLUSTER_IPTV = compMGMT.CLUSTER_IPTV /* FRMA CR255 Progetto Telecom */
	join V_M_REV_PERIOD_COMPETENZA vCmp
		on compMGMT.CMP_PER_ID = vCmp.Period_ID
	join M_REV_TEMPO mTempo
		on	cast(mTempo.Gio_Id as datetime) between vCmp.PeriodStart and vCmp.PeriodEnd
	join (	select distinct
				ATT_PER_ID, RC_COD, OFFSET_INIZIO_MESI, OFFSET_INIZIO_GIORNI, DURATA_MESI, 
				DURATA_GIORNI, SCADENZA, PRO_RATA
			from WT_REV_STORICO_ATTIVAZIONI) stoAtt
		on	volChurn.ATT_PER_ID = stoAtt.ATT_PER_ID and 
			compMGMT.RC_COD = stoAtt.RC_COD
	join (	select a.Gio_Id, b.FIRST_DAY 
			from M_REV_CALENDARIO_SOLARE a
			join M_REV_FIRST_LAST_DAYS b
				on right(a.Next_Ann_NC_Id, 2)+right(a.Next_Mes_NC_Id, 2) = right(b.Ann_Id, 2)+right(b.Mes2_Id, 2) ) calendSol
		on volChurn.DATA_ATT_RC = calendSol.GIO_ID
/*** Tabella utilizzata per recuperare la settimana e la week di attivazione del RC, questi due valori tengono
conto anche di eventuali OFFSET_INIZIO_MESI o OFFSET_INIZIO_GIORNI o PRO_RATA impostati per il rate code di 
attivazione (infatti in questi casi il join viene fatto con il campo DATA_ATT_RC negli altri con il primo giorno
del mese successivo all'attivazione del rate code). Settimana e Anno di attivazione effettiva del RC vengono
utilizzata successivamente per il dimezzamento dei volumi ***/
	inner hash join M_REV_TEMPO dataAtt
		on case
			when isnull(stoAtt.OFFSET_INIZIO_MESI, 0) &lt;&gt; 0 or isnull(stoAtt.OFFSET_INIZIO_GIORNI, 0) &lt;&gt; 0 or stoAtt.PRO_RATA = 1 or isnull(stoAtt.SCADENZA,0) &lt;&gt; 0
				then volChurn.DATA_ATT_RC 
			else calendSol.FIRST_DAY
		end = dataAtt.GIO_ID
	left join V_M_REV_PERIOD_ATTIVAZIONE vAtt2
		on cast(dataAtt.GIO_ID as datetime) between vAtt2.PeriodStart and vAtt2.PeriodEnd
/*** Della coda delle promo recupera soltano i giorni per cui la promo rientra nell'intervallo tra la data
	di churn e la data di cessazione della promo ***/
join S_PROMO promo
	on compMGMT.COD_PROMO_TEC = promo.PROMO_ID
join S_PROMO_STORICO storico
	on CASE WHEN REPLACE(LEFT(promo.PROMO_WEEKLY_RGR,75),'€','E') = 'PREV_1 Premium Pack "Decreasing Discount" for 12 Months' THEN 'PREV_1 Premium Pack  Decreasing Discount  for 12 Months'
			ELSE REPLACE(LEFT(promo.PROMO_WEEKLY_RGR,75),'€','E')
			END
			= REPLACE(storico.PROMO_WEEKLY_RGR,'€','E')	
, M_REV_RELEASE_DATE_MOTORE_SCONTI reldat
where  right(left(vAtt2.FiscalWeek,4),2)+right(left(vAtt2.FiscalWeek,8),2)&lt;=RIGHT(RELDAT.SCE_ID,2)+RIGHT(RELDAT.WEEK_ID,2)  
and right(mTempo.ANN_ID, 2)+right(mTempo.WEE2_ID,2) &gt;= (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)
	and mTempo.GIO_ID &gt;= volChurn.DATA_CHURN and mTempo.GIO_ID &gt;= volChurn.DATA_ATT_RC and mTempo.GIO_ID &lt;= compMGMT.DATA_CES_RC
group by 
	volChurn.SORGENTE_DATI,/*LOSO 20141014 riporto il flag per differenziare i due flussi provenienti da DELTA e CESSAZIONI PROMO PARCO*/
	volChurn.ATT_PER_ID,
	compMGMT.CMP_PER_ID,
	compMGMT.RC_PACCHETTO,
	compMGMT.RC_COD,
	storico.PROMO_ID,
	compMGMT.FLG_CAP_ID,
	compMGMT.TECNOLOGIA,
    compMGMT.DATA_ATT_RC, /*MAMU 20171129 aggiunto campo per valorizzazione nella SF_ATTIVAZIONI */
	compMGMT.CLUSTER_IPTV,
	vAtt2.Period_ID</property>
