      on compMGMT.DATA_ATT_RC_CHURN = calendSol.GIO_ID&#xA;    inner hash join WTT_REV_TEMPO_ATTIVAZIONE vAtt2&#xA;        on case&#xA;            when stoAtt.OFFSET_INIZIO_MESI &lt;&gt; 0 or stoAtt.OFFSET_INIZIO_GIORNI &lt;&gt; 0 or stoAtt.PRO_RATA = 1 or stoAtt.SCADENZA &lt;&gt; 0&#xA;                then compMGMT.DATA_ATT_RC_CHURN &#xA;            else calendSol.FIRST_DAY&#xA;        end = vAtt2.GIO_ID&#xA;    join M_REV_TEMPO mTempChurn&#xA;        on mTempChurn.Gio_Id = compMGMT.DATA_CHURN&#xA;    join M_REV_TEMPO mTempAtt&#xA;        on mTempAtt.Gio_Id = compMGMT.DATA_ATT_RC_CHURN&#xA;    , M_REV_RELEASE_DATE_MOTORE_SCONTI reldat&#xA;where  &#xA;compMGMT.DATA_ATT_RC_CHURN &gt; reldat.DATA&#xA;and right(vCmp.ANN_ID, 2)+right(vCmp.WEE2_ID,2) &gt;= (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)&#xA;    and &#xA;        (&#xA;          (&#xA;            (right(vCmp.ANN_ID, 2)+right(vCmp.WEE2_ID,2) &gt;= right(mTempChurn.ANN_ID, 2)+right(mTempChurn.WEE2_ID,2))&#xA;            and&#xA;            (mTempAtt.Wee2_Id = mTempChurn.Wee2_Id and left(mTempChurn.Gio_Des,3) &lt;&gt; left(mTempAtt.Gio_Des,3)) &#xA;          )&#xA;          or&#xA;          (&#xA;            (vCmp.GIO_ID &gt;= compMGMT.DATA_CHURN)&#xA;            and&#xA;            not (mTempAtt.Wee2_Id = mTempChurn.Wee2_Id and left(mTempChurn.Gio_Des,3) &lt;&gt; left(mTempAtt.Gio_Des,3))&#xA;          )&#xA;        )&#xA;    and vCmp.GIO_ID &gt;= compMGMT.DATA_ATT_RC_CHURN and vCmp.GIO_ID &lt;= compMGMT.DATA_CES_RC&#xA;group by &#xA;    compMGMT.SORGENTE_DATI,&#xA;    compMGMT.ATT_PER_ID,&#xA;    compMGMT.CMP_PER_ID,&#xA;    compMGMT.RC_PACCHETTO,&#xA;    compMGMT.RC_COD,&#xA;    compMGMT.COD_PROMO_TEC,&#xA;    compMGMT.FLG_CAP_ID,&#xA;    compMGMT.TECNOLOGIA,&#xA;    compMGMT.DATA_ATT_RC_MGMT, &#xA;    compMGMT.CLUSTER_IPTV,&#xA;    vAtt2.Period_ID" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
