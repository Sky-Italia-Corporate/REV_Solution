                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	stoAtt.ATT_PER_ID,
	promoCess.DATA_CHURN,
	stoAtt.ACCOUNT,
	stoAtt.COD_PROMO_TEC,
	stoAtt.FLG_CAP_ID,
	stoAtt.TECNOLOGIA,
	stoAtt.CLUSTER_IPTV,
	stoAtt.BB_CONTRACT_STATUS,
	stoAtt.BB_PRD_ID,
	stoAtt.BB_ORIGIN_CLUSTER,
	stoAtt.BB_VOICE_FLG,
	stoAtt.BUNDLE_FLG,
	stoAtt.RC_COD,
	stoAtt.DATA_ATT_RC,
	stoAtt.RC_PACCHETTO,
	stoAtt.OFFSET_INIZIO_MESI,
	stoAtt.OFFSET_INIZIO_GIORNI,
	stoAtt.DURATA_MESI,
	stoAtt.DURATA_GIORNI,
	stoAtt.SCADENZA,
	stoAtt.PRO_RATA,
	firstDay.FIRST_DAY, 
	convert(varchar, dateadd(day, cast(OFFSET_INIZIO_GIORNI as int), DATA_ATT_RC), 112) as DATA_ATT_RC_OFFSET,
	promoCess.CAUSALE_CESSAZIONE as SORGENTE_DATI
from WT_REV_STORICO_ATTIVAZIONI stoAtt 
--join TT_REV_DT_CESSAZIONI_PROMO_PARCO_CDW_BI_CHRS promoCess
join WT_REV_DT_CESSAZIONI_PROMO_PARCO_CDW_BI_CHRS promoCess
	/*** Devono essere churnate solo le promo attivate precedentemente al movimento di churn ***/
	on stoAtt.ACCOUNT = promoCess.ACCOUNT and 
		stoAtt.RC_COD = promoCess.RC_COD and 
		stoAtt.COD_PROMO_TEC = promoCess.COD_PROMO_TEC and 
		stoAtt.DATA_ATT_RC &lt; promoCess.DATA_CHURN
join M_REV_CALENDARIO_SOLARE calendSol 
	on stoAtt.DATA_ATT_RC = calendSol.Gio_Id
join M_REV_FIRST_LAST_DAYS firstDay
	on firstDay.Mes2_Id = 
		case 
			when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'M12'
			else 'M'+right(('00'+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 as varchar)), 2)
		end
	and firstDay.Ann_Id = 
		case 
			when (cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))%12 = 0 
				then 'FY'+right('0'+(cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)-1), 2)
			else 'FY'+right('00'+cast((cast(right(ltrim(rtrim(calendSol.Ann_Id)), 2) as integer)+cast((cast(right(ltrim(rtrim(calendSol.Mes2_Id)), 2) as integer)+isnull(OFFSET_INIZIO_MESI, 0))/12 as integer)) as varchar), 2)
		end
/*20141021 LOSO: Filtro le causali di cessazione utilizzando una tabella manuale delle cessazioni*/
where ltrim(rtrim(promoCess.CAUSALE_CESSAZIONE)) in (	select distinct ltrim(rtrim(CAUSALE_CESSAZIONE)) 
														from M_REV_CFG_CAUSALI_CESSAZIONE_PROMO_PARCO)
	  and ltrim(rtrim(promoCess.ACCOUNT)) not in (	select distinct ACC_ID 
													from T_SBD_DELTA)</property>
