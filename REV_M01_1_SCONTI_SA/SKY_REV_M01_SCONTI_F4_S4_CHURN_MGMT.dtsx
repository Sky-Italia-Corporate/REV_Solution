    <DTS:Executable
      DTS:refId="Package\Dimezza volumi di ultima settimana e, se presente, prima settimana"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2B96DE38-354A-4C70-91E1-B44F3D6F1E44}"
