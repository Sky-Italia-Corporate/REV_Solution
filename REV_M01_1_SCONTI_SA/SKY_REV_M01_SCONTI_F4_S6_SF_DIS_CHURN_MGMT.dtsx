        <SQLTask:SqlTaskData
          SQLTask:Connection="{53508C5F-8891-4EC8-ACB5-FA69D9A8DBB5}"
          SQLTask:SqlStatementSource="/*** Al cambio della week salva qui i valori della SF relativi alla week scorsa ***/&#xA;&#xA;IF  EXISTS (select * from WT_REV_DIS_CHURN_MGMT_APPOGGIO where right(WORK_SCE,2)+right(WORK_WEEK,2) != (select right(PREV_YEAR,2)+right(PREV_WEEK,2) from M_REV_PREV_WEEK))&#xA;BEGIN&#xA;&#xA;&#x9;truncate table WT_REV_DIS_CHURN_MGMT_APPOGGIO&#xA;&#xA;&#x9;insert into WT_REV_DIS_CHURN_MGMT_APPOGGIO&#xA;&#x9;select &#xA;&#x9;&#x9;prevWeek.PREV_YEAR, prevWeek.PREV_WEEK, &#xA;&#x9;&#x9;churnMGMT.ATT_PER_ID, churnMGMT.CMP_PER_ID,&#xA;&#x9;&#x9;churnMGMT.TIPO_DATO,churnMGMT.VOUCHER,churnMGMT.RC_PACCHETTO,churnMGMT.COD_PROMO_TEC,&#xA;&#x9;&#x9;churnMGMT.TECNOLOGIA,churnMGMT.FLG_CAP_ID,churnMGMT.CLUSTER_IPTV,&#xA;&#x9;&#x9;churnMGMT.BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;churnMGMT.BB_PRD_ID,&#xA;&#x9;&#x9;churnMGMT.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;churnMGMT.BB_VOICE_FLG,&#xA;&#x9;&#x9;churnMGMT.BUNDLE_FLG,&#xA;&#x9;&#x9;churnMGMT.VOLUMI,churnMGMT.VALORE_PROMO&#xA;&#x9;&#x9;from SF_REV_DIS_CHURN_MGMT churnMGMT&#xA;&#x9;&#x9;,M_REV_PREV_WEEK prevWeek&#xA;&#x9;&#x9;WHERE churnMGMT.ATT_PER_ID &lt;= (select distinct min(Period_ID)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from V_M_REV_PERIOD_ATTIVAZIONE vAtt&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;join M_REV_PREV_WEEK&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;on LEFT(vAtt.FiscalWeek,8) = REPLACE(PREV_YEAR,'ACT','FY')+'-'+PREV_WEEK )&#xA;&#xA;END&#xA;&#xA;/**** truncate table ST_REV_DIS_CHURN_MGMT ****/&#xA;truncate table ST_REV_DIS_CHURN_MGMT&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
