      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2D34F445-E757-42D1-81E8-F96CFB3C76FE}"
          SQLTask:SqlStatementSource="--eliminazione della calc week dalla tabella di storico&#xA;delete&#xA;from M_REV_STO_CONFIGURAZIONE_PROMO_MANUALI_PRS&#xA;where WEEK_ID = (select top 1 WEEK_ID from M_REV_CALC_WEEK)&#xA;and SCE_ID = (select top 1 SCE_ID from M_REV_CALC_WEEK)&#xA;&#xA;delete&#xA;from M_REV_STO_CONFIGURAZIONE_PROMO_STORICHE_PRS&#xA;where WEEK_ID = (select top 1 WEEK_ID from M_REV_CALC_WEEK)&#xA;and SCE_ID = (select top 1 SCE_ID from M_REV_CALC_WEEK)&#xA;&#xA;--inserimento della week nella tabella di storico&#xA;insert into M_REV_STO_CONFIGURAZIONE_PROMO_MANUALI_PRS&#xA;select b.SCE_ID, b.WEEK_ID, a.*&#xA;from M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS a, M_REV_CALC_WEEK b&#xA;&#xA;truncate table M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS&#xA;&#xA;insert into M_REV_STO_CONFIGURAZIONE_PROMO_STORICHE_PRS&#xA;select b.SCE_ID, b.WEEK_ID, a.*&#xA;from M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS a, M_REV_CALC_WEEK b&#xA;&#xA;truncate table M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS&#xA;&#xA;-- Riporta valori dal passato&#xA;insert into M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS&#xA;select a.PROMO_TYPE, a.PROMO_CLUSTER, a.PROMO_WEEKLY_RGR, isnull(a.PROMO_WEEKLY, a.PROMO_SKY) as PROMO_WEEKLY, a.PROMO_SKY--, null AS NUM_ATTIVAZIONI &#xA;from M_REV_STO_CONFIGURAZIONE_PROMO_MANUALI_PRS a&#xA;left outer join M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS b on a.PROMO_SKY = b.PROMO_SKY  &#xA;where SCE_ID+'-'+WEEK_ID=(select max(SCE_ID+'-'+WEEK_ID) from M_REV_STO_CONFIGURAZIONE_PROMO_MANUALI_PRS where PROMO_SKY = a.PROMO_SKY)&#xA;&#xA;insert into M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS&#xA;select a.PROMO_TYPE, a.PROMO_CLUSTER, a.PROMO_WEEKLY_RGR, isnull(a.PROMO_WEEKLY, a.PROMO_SKY) as PROMO_WEEKLY, a.PROMO_SKY--, null AS NUM_ATTIVAZIONI &#xA;from M_REV_STO_CONFIGURAZIONE_PROMO_STORICHE_PRS a&#xA;left outer join M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS b on a.PROMO_SKY = b.PROMO_SKY  &#xA;where a.SCE_ID+'-'+a.WEEK_ID=(select max(SCE_ID+'-'+WEEK_ID) from M_REV_STO_CONFIGURAZIONE_PROMO_STORICHE_PRS where PROMO_SKY = a.PROMO_SKY)&#xA;&#xA;update M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS&#xA;set PROMO_WEEKLY = PROMO_SKY&#xA;where PROMO_WEEKLY is null&#xA;&#xA;update M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS&#xA;set PROMO_WEEKLY = PROMO_SKY&#xA;where PROMO_WEEKLY is null&#xA;&#xA;--creazione tabella S&#xA;delete&#xA;from S_REV_PRS_PROMO_SKY_MANUALI&#xA;where PROMO_SKY in (select distinct replace(replace(PROMO_SKY, char(9), ''), char(10), '') as PROMO_SKY &#xA;       from M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS)&#xA;&#xA;insert into S_REV_PRS_PROMO_SKY_MANUALI&#xA;select distinct a.PROMO_TYPE, a.PROMO_WEEKLY_RGR, a.PROMO_CLUSTER, &#xA;case&#xA; when a.PROMO_WEEKLY is null then replace(replace(a.PROMO_SKY, char(9), ''), char(10), '')&#xA; else replace(replace(a.PROMO_WEEKLY, char(9), ''), char(10), '') &#xA;end as PROMO_WEEKLY, replace(replace(a.PROMO_SKY, char(9), ''), char(10), '') as PROMO_SKY &#xA;from M_REV_CONFIGURAZIONE_PROMO_MANUALI_PRS a&#xA;&#xA;delete&#xA;from S_REV_PRS_PROMO_SKY_STORICHE&#xA;where PROMO_SKY in (select distinct replace(replace(PROMO_SKY, char(9), ''), char(10), '') as PROMO_SKY &#xA;       from M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS)&#xA;&#xA;insert into S_REV_PRS_PROMO_SKY_STORICHE&#xA;select distinct a.PROMO_TYPE, a.PROMO_WEEKLY_RGR, a.PROMO_CLUSTER, &#xA;case&#xA; when a.PROMO_WEEKLY is null then replace(replace(a.PROMO_SKY, char(9), ''), char(10), '')&#xA; else replace(replace(a.PROMO_WEEKLY, char(9), ''), char(10), '') &#xA;end as PROMO_WEEKLY, replace(replace(a.PROMO_SKY, char(9), ''), char(10), '') as PROMO_SKY &#xA;from M_REV_CONFIGURAZIONE_PROMO_STORICHE_PRS a&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
