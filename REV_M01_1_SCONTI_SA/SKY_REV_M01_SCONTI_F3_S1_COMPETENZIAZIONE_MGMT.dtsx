                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	t.GIO_ID,
	cast(datediff(day, DATA_ATT_RC, DATA_INI) as numeric(38,10)) as DATA_PER_CALCOLO,
	DURATA_MESI,
	CASH_DISCOUNT,
