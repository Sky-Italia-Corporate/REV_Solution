  <DTS:Executables>
    <DTS:Executable
      DTS:refId="Package\Aggiorna il valore SCONTO_PROMO per le promo con configurazione in Revenue Share e Cash_Dis_ADJ diverso da 0"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{25D505A3-86D9-4FBD-9859-A17904989D0F}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Aggiorna il valore SCONTO_PROMO per le promo con configurazione in Revenue Share e Cash_Dis_ADJ diverso da 0"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F28BDCAE-9994-4E54-8E97-C9DAF2B6D21D}"
          SQLTask:SqlStatementSource="/***** &#xA;&#xA;LERO - DAMO - 20150317 - CR251&#xA;&#xA;Per le promo configurate in Revenue share viene modificato il valore di cash discount nella &#xA;WT_REV_CASH_DISCOUNT_MGMT ma non viene aggiornato il valore di sconto.&#xA;La competenziazione calcolata nella WT_REV_COMPETENZIAZIONE_MGMT per alcune casistiche parte proprio&#xA;dallo sconto: questo genera un calcolo del valore Cash Discount ADJ pari al delta tra il cash discount&#xA;standard ed il cash discount che si dovrebbe ottenere dall'applicazione della configurazione di Revenue Share.&#xA;Pertanto per queste promo il cash discount competenziato viene martellato uguale al cash discount calcolato&#xA;nella WT_REV_CASH_DISCOUNT_MGMT.&#xA;&#xA;FRMR - 20150518 - CR251_UR5_CashDiscountADJ&#xA;Inserito condizione per cui viene aggiornato il valore di sconto &#xA;delle promo che presentano un Cash Discount Adj diverso da 0.&#xA;&#xA;FRMR - 201604 - Bug Fixing &#xA;Sum sul Cash Discount MGMT e aggiunto tabelle temporanee WTT&#xA;****/&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[WTT_REV_COMPETENZIAZIONE_MGMT]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[WTT_REV_COMPETENZIAZIONE_MGMT]&#xA;GO&#xA;&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[WTT_REV_CASH_DISCOUNT_MGMT]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[WTT_REV_CASH_DISCOUNT_MGMT]&#xA;GO&#xA;&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[WTT_REV_COMPETENZIAZIONE_MGMT_NUM_RIGHE]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[WTT_REV_COMPETENZIAZIONE_MGMT_NUM_RIGHE]&#xA;GO&#xA;&#xA;/***&#xA;Recupero solo per le promo in Revenue Share il valore di cash discount competenziato per confrontarlo con quello non competenziato&#xA;***/&#xA;select wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV,BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC, sum(SCONTO_PROMO) as CASH_DISCOUNT&#xA;&#x9;into WTT_REV_COMPETENZIAZIONE_MGMT&#xA;from WT_REV_COMPETENZIAZIONE_MGMT wt_mgmt&#xA;join M_ATT_PROMO m_promo&#xA;&#x9;on wt_mgmt.COD_PROMO_TEC = m_promo.PROMO_ID&#xA;join M_ATT_PROMO_PRODOTTI m_promoProd&#xA;&#x9;on wt_mgmt.RC_COD = m_promoProd.RC_COD and &#xA;&#x9;&#x9;wt_mgmt.COD_PROMO_TEC = m_promoProd.PROMO_ID&#xA;where m_promo.COD_REVENUE_SHARE != 'No Share' or m_promoProd.CASH_DIS_ADJ != 0.0&#xA;group by wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC&#xA;&#xA;/***&#xA;Recupero solo per le promo in Revenue Share il valore di cash discount non competenziato per confrontarlo con quello competenziato&#xA;***/&#xA;select wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC, SUM(CASH_DISCOUNT) as CASH_DISCOUNT&#xA;&#x9;into WTT_REV_CASH_DISCOUNT_MGMT &#xA;from WT_REV_CASH_DISCOUNT_MGMT wt_mgmt&#xA;join M_ATT_PROMO m_promo&#xA;&#x9;on wt_mgmt.COD_PROMO_TEC = m_promo.PROMO_ID&#xA;join M_ATT_PROMO_PRODOTTI m_promoProd&#xA;&#x9;on wt_mgmt.RC_COD = m_promoProd.RC_COD and &#xA;&#x9;&#x9;wt_mgmt.COD_PROMO_TEC = m_promoProd.PROMO_ID&#xA;where m_promo.COD_REVENUE_SHARE != 'No Share' or m_promoProd.CASH_DIS_ADJ != 0.0&#xA;group by wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV,BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC&#xA;&#xA;/***&#xA;Recupero il numero di righe su cui ogni promo/rate code è competenziato e per cui devo aggiornare il valore di cash discount&#xA;***/&#xA;select wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC, count(*) as NUM_RIGHE&#xA;&#x9;into WTT_REV_COMPETENZIAZIONE_MGMT_NUM_RIGHE &#xA;from WT_REV_COMPETENZIAZIONE_MGMT wt_mgmt&#xA;join M_ATT_PROMO m_promo&#xA;&#x9;on wt_mgmt.COD_PROMO_TEC = m_promo.PROMO_ID&#xA;join M_ATT_PROMO_PRODOTTI m_promoProd&#xA;&#x9;on wt_mgmt.RC_COD = m_promoProd.RC_COD and &#xA;&#x9;&#x9;wt_mgmt.COD_PROMO_TEC = m_promoProd.PROMO_ID&#xA;where m_promo.COD_REVENUE_SHARE != 'No Share' or m_promoProd.CASH_DIS_ADJ != 0.0&#xA;group by wt_mgmt.RC_COD, PRO_RATA, RC_PACCHETTO, COD_PROMO_TEC, FLG_CAP_ID, TECNOLOGIA, CLUSTER_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, &#xA;&#x9;&#x9;BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, DATA_ATT_RC&#xA;&#xA;update wt_comp&#xA;set wt_comp.SCONTO_PROMO = wt_update.VAL_TO_UPDATE&#xA;from WT_REV_COMPETENZIAZIONE_MGMT wt_comp&#xA;join (&#xA;&#x9;select distinct&#xA;&#x9;&#x9;&#x9;/* Cash Discount Competenziato */&#xA;&#x9;&#x9;comp_rs.RC_COD, comp_rs.PRO_RATA, comp_rs.RC_PACCHETTO, comp_rs.COD_PROMO_TEC, comp_rs.FLG_CAP_ID, &#xA;&#x9;&#x9;comp_rs.TECNOLOGIA, comp_rs.CLUSTER_IPTV, /* FRMA CR255-Progetto TELECOM */ comp_rs.BB_CONTRACT_STATUS, comp_rs.BB_PRD_ID, &#xA;&#x9;&#x9;comp_rs.BB_ORIGIN_CLUSTER, comp_rs.BB_VOICE_FLG, comp_rs.BUNDLE_FLG,&#xA;&#x9;&#x9;comp_rs.DATA_ATT_RC,comp_rs.CASH_DISCOUNT as CD_COMP, &#xA;&#x9;&#x9;&#x9;/* Cash Discount NON Competenziato */&#xA;&#x9;&#x9;cd_rs.CASH_DISCOUNT as CD_ORIG, count_righe.NUM_RIGHE,&#xA;&#x9;&#x9;cd_rs.CASH_DISCOUNT / count_righe.NUM_RIGHE  as VAL_TO_UPDATE&#xA;&#x9;from WTT_REV_COMPETENZIAZIONE_MGMT comp_rs&#xA;&#x9;left join WTT_REV_CASH_DISCOUNT_MGMT cd_rs&#xA;&#x9;&#x9;on comp_rs.RC_COD = cd_rs.RC_COD&#xA;&#x9;&#x9;&#x9;and comp_rs.PRO_RATA = cd_rs.PRO_RATA&#xA;&#x9;&#x9;&#x9;and comp_rs.RC_PACCHETTO = cd_rs.RC_PACCHETTO&#xA;&#x9;&#x9;&#x9;and comp_rs.COD_PROMO_TEC = cd_rs.COD_PROMO_TEC&#xA;&#x9;&#x9;&#x9;and comp_rs.FLG_CAP_ID = cd_rs.FLG_CAP_ID&#xA;&#x9;&#x9;&#x9;and comp_rs.TECNOLOGIA = cd_rs.TECNOLOGIA&#xA;&#x9;&#x9;&#x9;and comp_rs.CLUSTER_IPTV = cd_rs.CLUSTER_IPTV&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_CONTRACT_STATUS = cd_rs.BB_CONTRACT_STATUS&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_PRD_ID = cd_rs.BB_PRD_ID&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_ORIGIN_CLUSTER = cd_rs.BB_ORIGIN_CLUSTER&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_VOICE_FLG = cd_rs.BB_VOICE_FLG&#xA;&#x9;&#x9;&#x9;and comp_rs.BUNDLE_FLG = cd_rs.BUNDLE_FLG&#xA;&#x9;&#x9;&#x9;and comp_rs.DATA_ATT_RC = cd_rs.DATA_ATT_RC&#xA;&#x9;left join WTT_REV_COMPETENZIAZIONE_MGMT_NUM_RIGHE count_righe&#xA;&#x9;&#x9;on comp_rs.RC_COD = count_righe.RC_COD&#xA;&#x9;&#x9;&#x9;and comp_rs.PRO_RATA = count_righe.PRO_RATA&#xA;&#x9;&#x9;&#x9;and comp_rs.RC_PACCHETTO = count_righe.RC_PACCHETTO&#xA;&#x9;&#x9;&#x9;and comp_rs.COD_PROMO_TEC = count_righe.COD_PROMO_TEC&#xA;&#x9;&#x9;&#x9;and comp_rs.FLG_CAP_ID = count_righe.FLG_CAP_ID&#xA;&#x9;&#x9;&#x9;and comp_rs.TECNOLOGIA = count_righe.TECNOLOGIA&#xA;&#x9;&#x9;&#x9;and comp_rs.CLUSTER_IPTV = count_righe.CLUSTER_IPTV&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_CONTRACT_STATUS = count_righe.BB_CONTRACT_STATUS&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_PRD_ID = count_righe.BB_PRD_ID&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_ORIGIN_CLUSTER = count_righe.BB_ORIGIN_CLUSTER&#xA;&#x9;&#x9;&#x9;and comp_rs.BB_VOICE_FLG = count_righe.BB_VOICE_FLG&#xA;&#x9;&#x9;&#x9;and comp_rs.BUNDLE_FLG = count_righe.BUNDLE_FLG&#xA;&#x9;&#x9;&#x9;and comp_rs.DATA_ATT_RC = count_righe.DATA_ATT_RC&#xA;&#x9;where comp_rs.CASH_DISCOUNT != cd_rs.CASH_DISCOUNT&#xA;) wt_update&#xA;on wt_comp.RC_COD = wt_update.RC_COD&#xA;&#x9;and wt_comp.PRO_RATA = wt_update.PRO_RATA&#xA;&#x9;and wt_comp.RC_PACCHETTO = wt_update.RC_PACCHETTO&#xA;&#x9;and wt_comp.COD_PROMO_TEC = wt_update.COD_PROMO_TEC&#xA;&#x9;and wt_comp.FLG_CAP_ID = wt_update.FLG_CAP_ID&#xA;&#x9;and wt_comp.TECNOLOGIA = wt_update.TECNOLOGIA&#xA;&#x9;and wt_comp.CLUSTER_IPTV = wt_update.CLUSTER_IPTV&#xA;&#x9;and wt_comp.BB_CONTRACT_STATUS = wt_update.BB_CONTRACT_STATUS&#xA;&#x9;&#x9;&#x9;and wt_comp.BB_PRD_ID = wt_update.BB_PRD_ID&#xA;&#x9;&#x9;&#x9;and wt_comp.BB_ORIGIN_CLUSTER = wt_update.BB_ORIGIN_CLUSTER&#xA;&#x9;&#x9;&#x9;and wt_comp.BB_VOICE_FLG = wt_update.BB_VOICE_FLG&#xA;&#x9;&#x9;&#x9;and wt_comp.BUNDLE_FLG = wt_update.BUNDLE_FLG&#xA;&#x9;and wt_comp.DATA_ATT_RC = wt_update.DATA_ATT_RC" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Cancella dati dallo storico della competenziazione per churn promo"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{007b1f69-da8b-496a-bd6a-8ce237e75820}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cancella dati dallo storico della competenziazione per churn promo"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F28BDCAE-9994-4E54-8E97-C9DAF2B6D21D}"
          SQLTask:SqlStatementSource="/*** Cancella dati dalla tabella che conserva lo storico della competenziazione ***/&#xA;delete WT&#xA;from WT_REV_STORICO_COMPETENZIAZIONE_MGMT WT&#xA;join V_M_REV_PERIOD_ATTIVAZIONE V &#xA;on WT.ATT_PER_ID = V.Period_ID&#xA;where LEFT(FiscalWeek,8) = (select REPLACE(SCE_ID, 'ACT','FY')+'-'+WEEK_ID from M_REV_CALC_WEEK)&#xA;&#xA;/*** Inserimento nella tabella di storico delle promo esaurite ****/&#xA;insert into WT_REV_STORICO_COMPETENZIAZIONE_MGMT_PROMO_CONCLUSE&#xA;select calcWeek.SCE_ID, calcWeek.WEEK_ID,&#xA;&#x9;&#x9;stoComp.ATT_PER_ID,stoComp.CMP_PER_ID,&#xA;&#x9;&#x9;stoComp.RC_COD,stoComp.PRO_RATA,stoComp.SCONTO,stoComp.RC_PACCHETTO,&#xA;&#x9;&#x9;stoComp.COD_PROMO_TEC,stoComp.FLG_CAP_ID,stoComp.TECNOLOGIA,stoComp.CLUSTER_IPTV,&#xA;&#x9;&#x9;stoComp.BB_CONTRACT_STATUS,&#x9;stoComp.BB_PRD_ID, stoComp.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;stoComp.BB_VOICE_FLG, stoComp.BUNDLE_FLG,&#xA;&#x9;&#x9;stoComp.VOLUMI,stoComp.GIORNI_SETTIMANA,stoComp.DATA_ATT_RC,stoComp.DATA_CES_RC,&#xA;&#x9;&#x9;stoComp.SCONTO_PROMO&#xA;from WT_REV_STORICO_COMPETENZIAZIONE_MGMT stoComp&#xA;join M_REV_TEMPO mTempo&#xA;&#x9;on dateadd(day, -1, cast(DATA_CES_RC as datetime)) = mTempo.GIO_ID&#xA;cross join M_REV_CALC_WEEK calcWeek&#xA;where right(mTempo.ANN_ID,2)+right(mTempo.WEE2_ID,2) &lt; (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)&#xA;&#xA;/*** Cancellazione della competenziazione MGMT delle promo che hanno già esaurito il loro effetto ***/&#xA;delete stoComp &#xA;from WT_REV_STORICO_COMPETENZIAZIONE_MGMT stoComp&#xA;join M_REV_TEMPO mTempo&#xA;&#x9;on dateadd(day, -1, cast(DATA_CES_RC as datetime)) = mTempo.GIO_ID&#xA;where right(mTempo.ANN_ID,2)+right(mTempo.WEE2_ID,2) &lt; (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Competenziazione MGMT 28 Giorni"
      DTS:CreationName="Microsoft.Pipeline"
      DTS:Description="Data Flow Task"
