                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	cast(numGiorni.CONTEGGIO as numeric(18, 10)) as CONTEGGIO,
	cashDis.CASH_DISCOUNT,
	cashDis.DATA_INI,
	cast(datediff(day, LAST_DAY, DATA_FIN_12_MESI) as numeric(18,10)) as DATA_PER_CALCOLO_1,
	cast(datediff(day, DATA_ATT_RC, LAST_DAY) as numeric(18,10)) as DATA_PER_CALCOLO_2,
	mTempo.Period_ID as CMP_PER_ID,
	cashDis.RC_COD,
	cashDis.PRO_RATA,
	cashDis.SCONTO,
	cashDis.RC_PACCHETTO,
	cashDis.COD_PROMO_TEC,
	cashDis.FLG_CAP_ID,
