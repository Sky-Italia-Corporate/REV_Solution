      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F28BDCAE-9994-4E54-8E97-C9DAF2B6D21D}"
          SQLTask:SqlStatementSource="delete WT&#xA;from WT_REV_STORICO_COMPETENZIAZIONE_US_GAAP WT&#xA;join V_M_REV_PERIOD_ATTIVAZIONE V &#xA;on WT.ATT_PER_ID = V.Period_ID&#xA;where LEFT(FiscalWeek,8) = (select REPLACE(SCE_ID, 'ACT','FY')+'-'+WEEK_ID from M_REV_CALC_WEEK)&#xA;&#xA;/*** Inserimento nella tabella di storico delle promo esaurite ****/&#xA;insert into WT_REV_STORICO_COMPETENZIAZIONE_US_GAAP_PROMO_CONCLUSE&#xA;select calcWeek.SCE_ID, calcWeek.WEEK_ID, &#xA;&#x9;stoComp.ATT_PER_ID,stoComp.CMP_PER_ID,&#xA;&#x9;stoComp.RC_COD,stoComp.PRO_RATA,stoComp.SCONTO,stoComp.RC_PACCHETTO,&#xA;&#x9;stoComp.COD_PROMO_TEC,stoComp.FLG_CAP_ID,stoComp.TECNOLOGIA,stoComp.CLUSTER_IPTV,&#xA;&#x9;stoComp.BB_CONTRACT_STATUS,&#x9;stoComp.BB_PRD_ID, stoComp.BB_ORIGIN_CLUSTER,&#xA;&#x9;stoComp.BB_VOICE_FLG, stoComp.BUNDLE_FLG,&#xA;&#x9;stoComp.VOLUMI,stoComp.GIORNI_SETTIMANA,stoComp.DATA_ATT_RC,stoComp.DATA_CES_RC,&#xA;&#x9;stoComp.SCONTO_PROMO_US_GAAP&#xA;from WT_REV_STORICO_COMPETENZIAZIONE_US_GAAP stoComp&#xA;join M_REV_TEMPO mTempo&#xA;&#x9;on dateadd(day, -1, cast(DATA_CES_RC as datetime)) = mTempo.GIO_ID&#xA;cross join M_REV_CALC_WEEK calcWeek&#xA;where right(mTempo.ANN_ID,2)+right(mTempo.WEE2_ID,2) &lt; (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)&#xA;&#xA;/*** Cancellazione della competenziazione US_GAAP delle promo che hanno già esaurito il loro effetto ***/&#xA;delete stoComp&#xA;from WT_REV_STORICO_COMPETENZIAZIONE_US_GAAP stoComp&#xA;join M_REV_TEMPO mTempo&#xA;&#x9;on dateadd(day, -1, cast(DATA_CES_RC as datetime)) = mTempo.GIO_ID&#xA;where right(mTempo.ANN_ID,2)+right(mTempo.WEE2_ID,2) &lt; (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
