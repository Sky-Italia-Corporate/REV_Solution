	cashDis.BB_VOICE_FLG,
	cashDis.BUNDLE_FLG,
	cashDis.VOLUMI,
	cashDis.DATA_ATT_RC,
	cashDis.Period_ID as ATT_PER_ID,
	cashDis.DATA_FIN_12_MESI as DATA_CES_RC
from WTT_REV_CASH_DISCOUNT_ATTIVAZIONE cashDis 
join WTT_REV_TEMPO_COMPETENZA mTempo
	on mTempo.GIO_ID &gt;= cashDis.DATA_INI and mTempo.GIO_ID &lt;= cashDis.DATA_FIN_12_MESI
/* join per recuperare il numero giorni mese NC al quale appartiene il giorno esaminato */
join (select ANN_ID, MES2_ID, count(GIO_ID) as CONTEGGIO
		from M_REV_TEMPO
		group by ANN_ID, MES2_ID) numGiorni
	on mTempo.ANN_ID = numGiorni.ANN_ID and mTempo.MES2_ID = numGiorni.MES2_ID
where DURATA_MESI is not null and ((DURATA_MESI &lt; 12) or (DURATA_MESI = 12 and PRO_RATA = 0))</property>
