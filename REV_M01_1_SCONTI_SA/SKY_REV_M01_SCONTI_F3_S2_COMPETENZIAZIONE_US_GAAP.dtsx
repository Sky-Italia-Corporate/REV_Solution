    <DTS:Executable
      DTS:refId="Package\Inserisce i dati settimanali nello storico della competenziazione per churn promo"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{D2F42E9A-503B-409A-8D14-C4484F6DFECE}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Inserisce i dati settimanali nello storico della competenziazione per churn promo"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F28BDCAE-9994-4E54-8E97-C9DAF2B6D21D}"
          SQLTask:SqlStatementSource="/* CASH_DISCOUNT non competenziati */&#xA;select distinct a.*, v.Period_ID  &#xA;into #TABELLA_d&#xA;from WT_REV_CASH_DISCOUNT_US_GAAP a&#xA;join V_M_REV_PERIOD_ATTIVAZIONE v&#xA;on cast(a.DATA_ATT_RC as datetime) between v.PeriodStart and v.PeriodEnd&#xA;, M_REV_CALC_WEEK b&#xA;go&#xA;&#xA;/** MAX_DAY e MIN_DAY per ogni mese e anno a partire dalla week di caricamento **/&#xA;select&#x9;distinct 'ACT'+right(ANN_ID,2) as ANN_ID,&#xA;&#x9;&#x9;WEE2_ID, &#xA;&#x9;&#x9;Period_ID,&#xA;&#x9;&#x9;LEFT(replace(CONVERT(VARCHAR, PeriodEnd, 120),'-',''), 8) as MAX_DAY, &#xA;&#x9;&#x9;LEFT(replace(CONVERT(VARCHAR, PeriodStart, 120),'-',''), 8) as MIN_DAY&#xA;into #TABELLA_c&#xA;from M_REV_TEMPO M&#xA;join V_M_REV_PERIOD_COMPETENZA V&#xA;on cast(M.Gio_Id as datetime) between V.PeriodStart and V.PeriodEnd&#xA;where right(ANN_ID,2)+right(WEE2_ID,2) &gt;= (select right(SCE_ID,2)+right(WEEK_ID,2) from M_REV_CALC_WEEK)&#xA;go&#xA;&#xA;/*** La query recupera a partire dalla competenziazione a livello giornaliero la competenziazione a livello&#xA;settimanale. Per ogni settimana e a livello di RC_COD, RC_PCK, COD_PROMO_TEC, DATA_ATT_RC, DATA_CES_RC vengono&#xA;calcolati i volumi e i giorni della settimana per cui lo sconto è stato competenziato (questo per fare fronte&#xA;agli effetti del pro-rata che potrebbe non sempre determinare un numero pari a 7 di giorni della settimana o per&#xA;esempio nel caso di promo a durata giorni o promo a scadenza in cui la promo può finire non necessariamente &#xA;coprendo un'intera settimana). Per ottenere i valori di sconto giornalieri è sufficente dividere lo sconto promo&#xA;per i giorni della settimana e i volumi ***/&#xA;insert into WT_REV_STORICO_COMPETENZIAZIONE_US_GAAP&#xA;select&#xA;&#x9;ATT_PER_ID,&#xA;&#x9;CMP_PER_ID,&#xA;&#x9;RC_COD,&#xA;&#x9;PRO_RATA,&#xA;&#x9;SCONTO,&#xA;&#x9;RC_PACCHETTO,&#xA;&#x9;COD_PROMO_TEC,&#xA;&#x9;FLG_CAP_ID,&#xA;&#x9;TECNOLOGIA,&#xA;&#x9;CLUSTER_IPTV,&#xA;&#x9;BB_CONTRACT_STATUS,&#xA;&#x9;BB_PRD_ID,&#xA;&#x9;BB_ORIGIN_CLUSTER,&#xA;&#x9;BB_VOICE_FLG,&#xA;&#x9;BUNDLE_FLG,&#xA;&#x9;sum(VOLUMI) as VOLUMI,&#xA;&#x9;GIORNI_SETTIMANA,&#xA;&#x9;DATA_ATT_RC,&#xA;&#x9;DATA_CES_RC,&#xA;&#x9;sum(SCONTO_PROMO_US_GAAP) as SCONTO_PROMO_US_GAAP&#xA;from (&#xA;&#x9;&#x9;&#x9;select &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.CMP_PER_ID, &#xA;&#x9;&#x9;&#x9;&#x9;cashDis.Period_ID as ATT_PER_ID,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.RC_COD, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.PRO_RATA, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.SCONTO, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.RC_PACCHETTO, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.COD_PROMO_TEC,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.FLG_CAP_ID,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.TECNOLOGIA,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;case&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la promo attivati in giorni che stanno in un mese solare diverso dal mese NC e che portano data_ini&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;precedente alla data di attivazione del rate code ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when cashDis.DATA_INI &lt; wtComp.DATA_ATT_RC and mTempo.MIN_DAY = cashDis.DATA_INI &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then sum(wtComp.VOLUMI)/(datediff(day, cashDis.DATA_INI, mTempo.MAX_DAY)+1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la casistica in cui una promo termini prima della fine dell'ultima settimana di &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;competenziazione ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.CMP_PER_ID) &lt; 7 and mTempo.MIN_DAY &lt; wtComp.DATA_CES_RC  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then sum(wtComp.VOLUMI)/(datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC))&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.CMP_PER_ID) &lt; 7 and mTempo.MIN_DAY = wtComp.DATA_CES_RC  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then sum(wtComp.VOLUMI)/(datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC)+1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la casistica in cui una promo inizi non il primo giorno della prima settimana di &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;competenziazione ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_ATT_RC &gt; mTempo.MIN_DAY &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then sum(wtComp.VOLUMI)/(datediff(day, wtComp.DATA_ATT_RC, mTempo.MAX_DAY) + 1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Casistica in cui la competenziazione della week copre tutti i giorni della settimana ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else sum(wtComp.VOLUMI)/(datediff(day, mTempo.MIN_DAY, mTempo.MAX_DAY) + 1)&#xA;&#x9;&#x9;&#x9;&#x9;end as VOLUMI,&#xA;&#x9;&#x9;&#x9;&#x9;case&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la promo attivati in giorni che stanno in un mese solare diverso dal mese NC e che portano data_ini&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;precedente alla data di attivazione del rate code ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when cashDis.DATA_INI &lt; wtComp.DATA_ATT_RC and mTempo.MIN_DAY = cashDis.DATA_INI &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then (datediff(day, cashDis.DATA_INI, mTempo.MAX_DAY)+1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la casistica in cui una promo termini prima della fine dell'ultima settimana di &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;competenziazione ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.CMP_PER_ID) &lt; 7 and mTempo.MIN_DAY &lt; wtComp.DATA_CES_RC &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then (datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC))&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.CMP_PER_ID) &lt; 7 and mTempo.MIN_DAY = wtComp.DATA_CES_RC &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then (datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC)+1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Usata per gestire la casistica in cui una promo inizi non il primo giorno della prima settimana di &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;competenziazione ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when wtComp.DATA_ATT_RC &gt; mTempo.MIN_DAY &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then datediff(day, wtComp.DATA_ATT_RC, mTempo.MAX_DAY) + 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;/*** Casistica in cui la competenziazione della week copre tutti i giorni della settimana ***/&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else datediff(day, mTempo.MIN_DAY, mTempo.MAX_DAY) + 1&#xA;&#x9;&#x9;&#x9;&#x9;end as GIORNI_SETTIMANA, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.DATA_ATT_RC, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.DATA_CES_RC, &#xA;&#x9;&#x9;&#x9;&#x9;sum(SCONTO_PROMO_US_GAAP) as SCONTO_PROMO_US_GAAP&#xA;&#x9;&#x9;&#x9;from WT_REV_COMPETENZIAZIONE_US_GAAP wtComp&#xA;&#x9;&#x9;&#x9;&#x9;/*** La tabella c recupera per ogni settimana il primo e l'ultimo giorno ***/&#xA;&#x9;&#x9;&#x9;inner hash join #TABELLA_c mTempo&#xA;&#x9;&#x9;&#x9;&#x9;on wtComp.CMP_PER_ID = mTempo.Period_ID&#xA;&#x9;&#x9;&#x9;&#x9;/* recupera i cash discount us gaap */&#xA;&#x9;&#x9;&#x9;inner hash join #TABELLA_d cashDis&#xA;&#x9;&#x9;&#x9;&#x9;on&#x9;wtComp.RC_COD = cashDis.RC_COD and &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.DATA_ATT_RC = cashDis.DATA_ATT_RC and &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.RC_PACCHETTO = cashDis.RC_PACCHETTO and &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.COD_PROMO_TEC = cashDis.COD_PROMO_TEC and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.FLG_CAP_ID = cashDis.FLG_CAP_ID and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.TECNOLOGIA = cashDis.TECNOLOGIA and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.CLUSTER_IPTV = cashDis.CLUSTER_IPTV and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.BB_CONTRACT_STATUS = cashDis.BB_CONTRACT_STATUS and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.BB_PRD_ID = cashDis.BB_PRD_ID and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.BB_ORIGIN_CLUSTER = cashDis.BB_ORIGIN_CLUSTER and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.BB_VOICE_FLG = cashDis.BB_VOICE_FLG and&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;wtComp.BUNDLE_FLG = cashDis.BUNDLE_FLG &#xA;&#x9;&#x9;&#x9;group by wtComp.CMP_PER_ID, &#xA;&#x9;&#x9;&#x9;&#x9;cashDis.Period_ID,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.RC_COD, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.PRO_RATA,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.SCONTO, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.RC_PACCHETTO, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.COD_PROMO_TEC, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.FLG_CAP_ID, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.TECNOLOGIA,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.CLUSTER_IPTV,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_CONTRACT_STATUS,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_PRD_ID,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_ORIGIN_CLUSTER,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BB_VOICE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.BUNDLE_FLG,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.DATA_ATT_RC, &#xA;&#x9;&#x9;&#x9;&#x9;wtComp.DATA_CES_RC,&#xA;&#x9;&#x9;&#x9;&#x9;wtComp.VOLUMI,&#xA;&#x9;&#x9;&#x9;&#x9;mTempo.MAX_DAY,&#xA;&#x9;&#x9;&#x9;&#x9;mTempo.MIN_DAY,&#xA;&#x9;&#x9;&#x9;&#x9;cashDis.DATA_INI ) d&#xA;group by&#xA;&#x9;CMP_PER_ID,&#xA;&#x9;ATT_PER_ID,&#xA;&#x9;RC_COD,&#xA;&#x9;PRO_RATA,&#xA;&#x9;SCONTO,&#xA;&#x9;RC_PACCHETTO,&#xA;&#x9;COD_PROMO_TEC,&#xA;&#x9;FLG_CAP_ID,&#xA;&#x9;TECNOLOGIA,&#xA;&#x9;CLUSTER_IPTV,&#xA;&#x9;BB_CONTRACT_STATUS,&#xA;&#x9;BB_PRD_ID,&#xA;&#x9;BB_ORIGIN_CLUSTER,&#xA;&#x9;BB_VOICE_FLG,&#xA;&#x9;BUNDLE_FLG,&#xA;&#x9;GIORNI_SETTIMANA,&#xA;&#x9;DATA_ATT_RC,&#xA;&#x9;DATA_CES_RC&#xA;&#x9;&#xA;&#x9;&#xA;&#x9;&#xA;drop table #TABELLA_d&#xA;drop table #TABELLA_c" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
