                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	cast(numGiorni.CONTEGGIO as numeric(18, 10)) as CONTEGGIO,
	cashDis.CASH_DISCOUNT,
	cashDis.DATA_INI,
	cast(datediff(day, DATA_ATT_RC, DATA_INI) as numeric(18,10)) as DATA_PER_CALCOLO,
	mTempo.Period_ID as CMP_PER_ID,
	cashDis.RC_COD,
	cashDis.PRO_RATA,
	cashDis.SCONTO,
	cashDis.RC_PACCHETTO,
	cashDis.COD_PROMO_TEC,
