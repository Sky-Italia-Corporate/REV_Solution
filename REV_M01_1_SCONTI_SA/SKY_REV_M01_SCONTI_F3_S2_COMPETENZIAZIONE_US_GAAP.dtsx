from WTT_REV_CASH_DISCOUNT_ATTIVAZIONE cashDis
join WT_REV_COMPETENZIAZIONE_MGMT compMgmt 
	on cashDis.RC_COD = compMgmt.RC_COD and
		cashDis.PRO_RATA = compMgmt.PRO_RATA and
		cashDis.SCONTO = compMgmt.SCONTO and
		cashDis.RC_PACCHETTO = compMgmt.RC_PACCHETTO and
