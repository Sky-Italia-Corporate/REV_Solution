      DTS:Disabled="True"
      DTS:DTSID="{f1ceec9a-29ea-4783-8f62-abc51976b228}"
      DTS:ExecutableType="Microsoft.Pipeline"
      DTS:LocaleID="-1"
      DTS:ObjectName="Inserisce i dati settimanali nello storico della competenziazione per churn promo (OLD)"
      DTS:TaskContact="Performs high-performance data extraction, transformation and loading;Microsoft Corporation; Microsoft SQL Server v9; (C) 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <pipeline
          BLOBTempStoragePath=""
          bufferTempStoragePath=""
          engineThreads="5"
          version="1">
          <components>
            <component
              refId="Package\Inserisce i dati settimanali nello storico della competenziazione per churn promo (OLD)\WT_REV_COMPETENZIAZIONE_US_GAAP"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="WT_REV_COMPETENZIAZIONE_US_GAAP"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*** La query recupera a partire dalla competenziazione a livello giornaliero la competenziazione a livello
settimanale. Per ogni settimana e a livello di RC_COD, RC_PCK, COD_PROMO_TEC, DATA_ATT_RC, DATA_CES_RC vengono
calcolati i volumi e i giorni della settimana per cui lo sconto è stato competenziato (questo per fare fronte
agli effetti del pro-rata che potrebbe non sempre determinare un numero pari a 7 di giorni della settimana o per
esempio nel caso di promo a durata giorni o promo a scadenza in cui la promo può finire non necessariamente 
coprendo un'intera settimana). Per ottenere i valori di sconto giornalieri è sufficente dividere lo sconto promo
per i giorni della settimana e i volumi ***/
select
	SCE_ID,
	WEEK_ID,
	ACT_YEAR,
	ACT_WEEK,
	RC_COD,
	PRO_RATA,
	SCONTO,
	RC_PACCHETTO,
	COD_PROMO_TEC,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	sum(VOLUMI) as VOLUMI,
	GIORNI_SETTIMANA,
	DATA_ATT_RC,
	DATA_CES_RC,
	sum(SCONTO_PROMO_US_GAAP) as SCONTO_PROMO_US_GAAP
from (
			select 
				wtComp.SCE_ID, 
				wtComp.WEEK_ID, 
				calcWeek.SCE_ID as ACT_YEAR, 
				calcWeek.WEEK_ID as ACT_WEEK, 
				wtComp.RC_COD, 
				wtComp.PRO_RATA, 
				wtComp.SCONTO, 
				wtComp.RC_PACCHETTO, 
				wtComp.COD_PROMO_TEC,
				wtComp.FLG_CAP_ID,
				wtComp.TECNOLOGIA,
				wtComp.CLUSTER_IPTV,
				case
					/*** Usata per gestire la promo attivati in giorni che stanno in un mese solare diverso dal mese NC e che portano data_ini
						precedente alla data di attivazione del rate code ***/
					when cashDis.DATA_INI &lt; wtComp.DATA_ATT_RC and mTempo.MIN_DAY = cashDis.DATA_INI 
						then sum(wtComp.VOLUMI)/(datediff(day, cashDis.DATA_INI, mTempo.MAX_DAY)+1)
					/*** Usata per gestire la casistica in cui una promo termini prima della fine dell'ultima settimana di 
					competenziazione ***/
					when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.WEEK_ID) &lt; 7 
						then sum(wtComp.VOLUMI)/(datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC))
					/*** Usata per gestire la casistica in cui una promo inizi non il primo giorno della prima settimana di 
					competenziazione ***/
					when wtComp.DATA_ATT_RC &gt; mTempo.MIN_DAY 
						then sum(wtComp.VOLUMI)/(datediff(day, wtComp.DATA_ATT_RC, mTempo.MAX_DAY) + 1)
					/*** Casistica in cui la competenziazione della week copre tutti i giorni della settimana ***/
					else sum(wtComp.VOLUMI)/(datediff(day, mTempo.MIN_DAY, mTempo.MAX_DAY) + 1)
				end as VOLUMI,
				case
					/*** Usata per gestire la promo attivati in giorni che stanno in un mese solare diverso dal mese NC e che portano data_ini
						precedente alla data di attivazione del rate code ***/
					when cashDis.DATA_INI &lt; wtComp.DATA_ATT_RC and mTempo.MIN_DAY = cashDis.DATA_INI 
						then (datediff(day, cashDis.DATA_INI, mTempo.MAX_DAY)+1)
					/*** Usata per gestire la casistica in cui una promo termini prima della fine dell'ultima settimana di 
					competenziazione ***/
					when wtComp.DATA_CES_RC &gt; wtComp.DATA_ATT_RC and mTempo.MAX_DAY &gt;= wtComp.DATA_CES_RC and count(wtComp.WEEK_ID) &lt; 7 
						then (datediff(day, mTempo.MIN_DAY, wtComp.DATA_CES_RC))
					/*** Usata per gestire la casistica in cui una promo inizi non il primo giorno della prima settimana di 
					competenziazione ***/
					when wtComp.DATA_ATT_RC &gt; mTempo.MIN_DAY 
						then datediff(day, wtComp.DATA_ATT_RC, mTempo.MAX_DAY) + 1
					/*** Casistica in cui la competenziazione della week copre tutti i giorni della settimana ***/
					else datediff(day, mTempo.MIN_DAY, mTempo.MAX_DAY) + 1
				end as GIORNI_SETTIMANA, 
				wtComp.DATA_ATT_RC, 
				wtComp.DATA_CES_RC, 
				sum(SCONTO_PROMO_US_GAAP) as SCONTO_PROMO_US_GAAP
			from WT_REV_COMPETENZIAZIONE_US_GAAP wtComp
				/*** La tabella c recupera per ogni settimana il primo e l'ultimo giorno ***/
			inner hash join (	select 'ACT'+right(ANN_ID,2) as ANN_ID, WEE2_ID, max(GIO_ID) as MAX_DAY, min(GIO_ID) as MIN_DAY 
								from M_REV_TEMPO
								group by ANN_ID, WEE2_ID) mTempo
				on wtComp.SCE_ID = mTempo.ANN_ID and wtComp.WEEK_ID = mTempo.WEE2_ID
				/* recupera i cash discount us gaap */
			inner hash join (select distinct * from WT_REV_CASH_DISCOUNT_US_GAAP) cashDis
				on	wtComp.RC_COD = cashDis.RC_COD and 
					wtComp.DATA_ATT_RC = cashDis.DATA_ATT_RC and 
					wtComp.RC_PACCHETTO = cashDis.RC_PACCHETTO and 
					wtComp.COD_PROMO_TEC = cashDis.COD_PROMO_TEC and
					wtComp.FLG_CAP_ID = cashDis.FLG_CAP_ID and
					wtComp.TECNOLOGIA = cashDis.TECNOLOGIA and
					wtComp.CLUSTER_IPTV = cashDis.CLUSTER_IPTV
			,M_REV_CALC_WEEK calcWeek
			group by wtComp.SCE_ID, 
				wtComp.WEEK_ID, 
				calcWeek.SCE_ID, 
				calcWeek.WEEK_ID, 
				wtComp.RC_COD, 
				wtComp.PRO_RATA,
				wtComp.SCONTO, 
				wtComp.RC_PACCHETTO, 
				wtComp.COD_PROMO_TEC, 
				wtComp.FLG_CAP_ID, 
				wtComp.TECNOLOGIA,
				wtComp.CLUSTER_IPTV,
				wtComp.DATA_ATT_RC, 
				wtComp.DATA_CES_RC,
				wtComp.VOLUMI,
				mTempo.MAX_DAY,
				mTempo.MIN_DAY,
				cashDis.DATA_INI ) d
group by
	SCE_ID,
	WEEK_ID,
	ACT_YEAR,
	ACT_WEEK,
	RC_COD,
	PRO_RATA,
	SCONTO,
	RC_PACCHETTO,
	COD_PROMO_TEC,
	FLG_CAP_ID,
	TECNOLOGIA,
	CLUSTER_IPTV,
	GIORNI_SETTIMANA,
	DATA_ATT_RC,
	DATA_CES_RC</property>
