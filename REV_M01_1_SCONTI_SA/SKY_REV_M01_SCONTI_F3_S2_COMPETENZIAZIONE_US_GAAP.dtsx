          SQLTask:Connection="{F28BDCAE-9994-4E54-8E97-C9DAF2B6D21D}"
          SQLTask:SqlStatementSource="IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[WTT_REV_CASH_DISCOUNT_ATTIVAZIONE]'))&#xA;drop index IDX_V_ATT_PRO_RATA on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_SCONTO on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_RC_PACCHETTO on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_COD_PROMO_Tec on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_TECNOLOGIA on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_CLUSTER_IPTV on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_BB_CONTRACT_STATUS on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_BB_PRD_ID on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_BB_ORIGIN_CLUSTER on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_BB_VOICE_FLG on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_BUNDLE_FLG on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_VOLUMI on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_DATA_ATT_RC on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_FLG_CAP_ID on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_RC_COD on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;drop index IDX_V_ATT_DURATA_MESI on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE&#xA;GO&#xA;&#xA;&#xA;create index IDX_V_ATT_PRO_RATA on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(PRO_RATA)&#xA;create index IDX_V_ATT_SCONTO on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(SCONTO)&#xA;create index IDX_V_ATT_RC_PACCHETTO on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(RC_PACCHETTO)&#xA;create index IDX_V_ATT_COD_PROMO_Tec on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(COD_PROMO_TEC)&#xA;create index IDX_V_ATT_TECNOLOGIA on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(TECNOLOGIA)&#xA;create index IDX_V_ATT_CLUSTER_IPTV on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(CLUSTER_IPTV)&#xA;create index IDX_V_ATT_BB_CONTRACT_STATUS on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(BB_CONTRACT_STATUS)&#xA;create index IDX_V_ATT_BB_PRD_ID on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(BB_PRD_ID)&#xA;create index IDX_V_ATT_BB_ORIGIN_CLUSTER on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(BB_ORIGIN_CLUSTER)&#xA;create index IDX_V_ATT_BB_VOICE_FLG on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(BB_VOICE_FLG)&#xA;create index IDX_V_ATT_BUNDLE_FLG on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(BUNDLE_FLG)&#xA;create index IDX_V_ATT_VOLUMI on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(VOLUMI)&#xA;create index IDX_V_ATT_DATA_ATT_RC on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(DATA_ATT_RC)&#xA;create index IDX_V_ATT_FLG_CAP_ID on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(FLG_CAP_ID)&#xA;create index IDX_V_ATT_RC_COD on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(RC_COD)&#xA;create index IDX_V_ATT_DURATA_MESI on WTT_REV_CASH_DISCOUNT_ATTIVAZIONE(DURATA_MESI)&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
