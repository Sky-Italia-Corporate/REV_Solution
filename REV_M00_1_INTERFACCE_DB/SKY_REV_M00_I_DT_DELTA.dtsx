              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
'ACT'+right(SCE_ID, 2) as SCE_ID, GIO_ID, a.ACC_ID, PCK_ID, DAT_ATT, DAT_VEN, TCN_ID, upper(CLU_IPTV) as CLU_IPTV,
a.STA_ID, /* ANCC 20170403 Improvement Maschera Churn*/
----------Integrazione TV-BB ----------
ISNULL(LTRIM(RTRIM(a.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
		cast(CASE WHEN ISNULL(LTRIM(RTRIM(a.BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') ='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
		ELSE LTRIM(RTRIM(a.BB_PRD_ID))  END as varchar(80)) AS BB_PRD_ID,
		CAST(ISNULL(LTRIM(RTRIM(a.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER,
		case when ISNULL(LTRIM(RTRIM(a.BB_VOICE_FLG)),'N') = 'N_BB' then 'N'
		when  LTRIM(RTRIM(a.BB_VOICE_FLG)) = 'Y_BB' then 'Y' 
		else LTRIM(RTRIM(a.BB_VOICE_FLG)) end AS BB_VOICE_FLG,
		ISNULL(LTRIM(RTRIM(a.BB_FLG_BUNDLE)),'No Bundle') AS BUNDLE_FLG
from SF_SBD_DELTA a
	/*** Filtro sugli stati che identificano i churn ***/
where a.STA_ID in (
				select STA_ID
				from S_SUB_STA_STATO a
				join SKY_REV.dbo.M_REV_MAPPING_DIM_FACT_TABLE b
					on a.STA_LEV0_DES = b.ELEMENTO
				where FACT_TABLE = 'CHURN_PROMO'
				)
	/*** Filtro sul tipo di abbonato residenziale ***/
	and TAB_ID in (
				select TAB_ID 
				from S_SUB_TAB_TIPO_ABBONATO
				where TAB_DES like '%RESIDENTIAL%'
				)
	/*** Filtro sui dati inerenti alla week di caricamento ***/
	and SCE_ID in (
				select 'A'+right(SCE_ID,2) as SCE_ID 
				from SKY_REV.dbo.M_REV_CALC_WEEK
				)
	and GIO_ID in (
				select GIO_ID 
				from SKY_REV.dbo.M_REV_CALC_WEEK a 
				join SKY_REV.dbo.M_REV_TEMPO b 
					on a.WEEK_ID = b.WEE2_ID
					and right(SCE_ID, 2) = right(ANN_ID, 2)
				)</property>
