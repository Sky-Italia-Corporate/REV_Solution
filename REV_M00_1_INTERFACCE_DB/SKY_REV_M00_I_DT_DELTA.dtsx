            <component
              refId="Package\Import Delta Tables\SF_SBD_DELTA_RETENTION"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="SF_SBD_DELTA_RETENTION"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* STATI RELATIVI A MOVIMENTI DI RETENTION
  24 --&gt; RECOVERED 
  10017 --&gt; RECOVERY FROM PDISC
  10026 --&gt; RECOVERY FROM PDISC NO BILLED
  23 --&gt; RECONNECTION
  */
SELECT [STA_ID]
      ,[TCN_ID]
      ,[PCK_ID]
      ,[DUR_ID]
      ,[TAB_ID]
      ,[GIO_ID]
      ,[SCE_ID]
      ,[MOP_ID]
      ,[LST_ID]
      ,[CCE_ID]
      ,[ACC_ID]
      ,[DAT_ATT]
      ,[DAT_VEN]
      ,[PRS_DES]
      ,[CLS_ID]
      ,[FLG_BB]
      ,[STA_BB]
      ,[PDV_ID]
      ,[CNA_ID]
      ,[NUM_CLI]
      ,[SUBS]
      ,[IMP_CNN]
      ,[PVN_ID]
      ,[IMP_CVN]
      ,[INT_VN]
      ,[BAM_VN]
      ,[DOC_VN]
      ,[NEW_VN]
      ,[MUS_VN]
      ,[CIN_VN]
      ,[SPO_VN]
      ,[CAL_VN]
      ,[SUBENTRO]
      ,[TCR_ID]
      ,[SCR_ID]
      ,[FLG_CAP_ID]
      ,[CB_TEC]
      ,[CLU_IPTV]
      ,[BST_ID]
      ,[BST_CNN]
      ,[OAD_ID]
      ,[FLX_ID]
      ,[BB_CONTRACT_STATUS]
      ,[BB_PRD_ID]
      ,[BB_ORIGIN_CLUSTER]
      ,[BB_VOICE_FLG]
      ,[BB_FLG_BUNDLE]
      ,[FMM_ID]
      ,[IMP_CNN_PRE]
      ,CASE 
		WHEN STA_ID =24 THEN
			'Recovery Bad Payer'
		WHEN STA_ID= 10017 THEN
			'Recovery Pdisc'
		WHEN STA_ID= 10026 THEN
			'Recovery Pdisc'
		ELSE
			TIPO_RET
		END AS TIPO_RET
  FROM [SF_SBD_DELTA]
  WHERE STA_ID IN (24,10017,10026,23)
  AND GIO_ID IN 
  (
	  SELECT GIO_ID 
