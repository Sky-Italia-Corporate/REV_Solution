              description="OLE DB Source"
              name="SF_SBD_DELTA SOSPESI"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  
    sbd.STA_ID, 
    sbd.DUR_ID,
    sbd.TCN_ID, 
    sbd.PCK_ID, 
    sbd.CLS_ID,
    sbd.FLG_BB,
    sbd.GIO_ID,
    'ACT' + right(sbd.SCE_ID, 2) as SCE_ID, 
    sbd.MOP_ID, 
    UPPER(sbd.CLU_IPTV) as CLU_IPTV,
    sum(sbd.SUBS) as SUBS,
	  	ISNULL(LTRIM(RTRIM(sbd.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
		cast(CASE WHEN ISNULL(LTRIM(RTRIM(sbd.BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') ='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
		ELSE LTRIM(RTRIM(sbd.BB_PRD_ID))  END as varchar(80)) AS BB_PRD_ID,
		CAST(ISNULL(LTRIM(RTRIM(sbd.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER,
		case when ISNULL(LTRIM(RTRIM(sbd.BB_VOICE_FLG)),'N') = 'N_BB' then 'N'
		when  LTRIM(RTRIM(sbd.BB_VOICE_FLG)) = 'Y_BB' then 'Y' 
		else LTRIM(RTRIM(sbd.BB_VOICE_FLG)) end AS BB_VOICE_FLG,
		ISNULL(LTRIM(RTRIM(sbd.BB_FLG_BUNDLE)),'No Bundle') AS BUNDLE_FLG
from SF_SBD_DELTA sbd
/************************************************/
/*** Filtro sul tipo di abbonato residenziale ***/
/************************************************/
join (
    select 
	   TAB_ID 
    from S_SUB_TAB_TIPO_ABBONATO
    where TAB_DES like '%RESIDENTIAL%'
) tab
    on sbd.TAB_ID = tab.TAB_ID
join SKY_REV.dbo.V_M_REV_TEMPO V
	on sbd.GIO_ID = V.GIO_ID
/*********************************************************/
/*** Recupero i dati da Delta sono se sono nella week  ***/
/*** in cui cade il giorno N del mese dato dall inizio della fatturazione + X giorni ***/
/*** nel caso di fatturazione 28 giorni (e da delta recupero    ***/
/*** solo i dati del giorno N)                        **/
/*********************************************************/ 
left join (
    select distinct 
	  tempo.Gio_Id
   from SKY_REV.dbo.M_REV_CALC_WEEK calc_wk
   inner join SKY_REV.dbo.M_REV_TEMPO tempo 
	  on right(calc_wk.SCE_ID, 2) = right(tempo.Ann_Id, 2)
		 and calc_wk.WEEK_ID = tempo.Wee2_Id 
   inner join SKY_REV.dbo.M_REV_CALENDARIO_28_BILLING m
	  on  tempo.Gio_Id = convert(varchar(8),dateadd(dd,92,convert(datetime,convert(varchar(10),m.B28_ID,112))),112)
) tempo
    on v.GIO_ID = tempo.Gio_Id and V.Gio_Uda = 'FATT. 28'
/*********************************************************/
/*** Recupero i dati da Delta sono se sono nella week  ***/
/*** in cui cade il 7 del mese e il tipo ***/
/*** di fatturazione è solare (e da delta recupero    ***/
/*** solo i dati del 7 de mese)                        **/
/*********************************************************/ 
left join (
    select distinct 
	  tempo.Gio_Id
   from SKY_REV.dbo.M_REV_CALC_WEEK calc_wk
   inner join SKY_REV.dbo.M_REV_TEMPO tempo 
	  on right(calc_wk.SCE_ID, 2) = right(tempo.Ann_Id, 2)
		 and calc_wk.WEEK_ID = tempo.Wee2_Id 
		 and right(tempo.Gio_Id, 2) = '07'
) tempo2
    on v.GIO_ID = tempo2.Gio_Id and V.Gio_Uda = 'FATT. SOL'
/*********************************************************/
/*** Recupero i dati da Delta sono se sono nella week  ***/
/*** in cui cade il 7 del mese e il tipo ***/
/*** di fatturazione è 28 giorni (e da delta recupero    ***/
/*** solo i dati del 7 de mese)                        **/
/*********************************************************/ 
left join (
    select distinct 
	  tempo.Gio_Id
   from SKY_REV.dbo.M_REV_CALC_WEEK calc_wk
   inner join SKY_REV.dbo.M_REV_TEMPO tempo 
	  on right(calc_wk.SCE_ID, 2) = right(tempo.Ann_Id, 2)
		 and calc_wk.WEEK_ID = tempo.Wee2_Id 
		 and right(tempo.Gio_Id, 2) = '07'
) tempo3
    on v.GIO_ID = tempo3.Gio_Id and V.Gio_Uda = 'FATT. 28'
/**********************************/
/*** Filtro sullo stato sospesi ***/ 
/*** Filtro anche sulla tipologia di fatturazione da adoperare sulla base del giorno ***/ 
/**********************************/
where
    sbd.STA_ID = '12'
and  case  			when v.Gio_Uda='FATT. 28' and v.GIO_ID &gt;= '20180101'  then tempo.Gio_Id 
					when v.Gio_Uda='FATT. 28' and v.GIO_ID &lt; '20180101' then tempo3.Gio_Id 
					else tempo2.Gio_Id end = v.Gio_Id
group by sbd.STA_ID, 
    sbd.DUR_ID,
    sbd.TCN_ID, 
    sbd.PCK_ID,
    sbd.FLG_BB, 
    sbd.GIO_ID,
    sbd.CLS_ID,
    sbd.SCE_ID,
    sbd.MOP_ID, 
    UPPER(sbd.CLU_IPTV),
	ISNULL(LTRIM(RTRIM(sbd.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') ,
		cast(CASE WHEN ISNULL(LTRIM(RTRIM(sbd.BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') ='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
		ELSE LTRIM(RTRIM(sbd.BB_PRD_ID))  END as varchar(80)),
		CAST(ISNULL(LTRIM(RTRIM(sbd.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) ,
		case when ISNULL(LTRIM(RTRIM(sbd.BB_VOICE_FLG)),'N') = 'N_BB' then 'N'
		when  LTRIM(RTRIM(sbd.BB_VOICE_FLG)) = 'Y_BB' then 'Y' 
		else LTRIM(RTRIM(sbd.BB_VOICE_FLG)) end ,
		ISNULL(LTRIM(RTRIM(sbd.BB_FLG_BUNDLE)),'No Bundle')</property>
