                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	b.[GIO_ID],
	b.[SCE_ID],
	b.[DUR_ID],
	b.[MOP_ID],
	b.[TAB_ID],
	b.[TCN_ID],
	b.[SDE_ID],
	b.[PCK_ID],
	b.[STA_ID],
	b.[CAN_ID],
	b.[NUM_CLI],
	b.[SUBS],
	b.[CLS_ID],
	b.[FLG_BB],
	b.[CLU_IPTV],
	
	/* Integrazione TV-BB con aggiunta 4 dimensioni da BB */		
	
	ISNULL(LTRIM(RTRIM(b.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
	CASE WHEN BB_PRD_ID='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
	ELSE CAST(ISNULL(LTRIM(RTRIM(BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') AS varchar(80)) END AS BB_PRD_ID,
	CAST(ISNULL(LTRIM(RTRIM(b.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER,
	ISNULL(LTRIM(RTRIM(b.BB_VOICE_FLG)),'N') AS BB_VOICE_FLG,
	ISNULL(LTRIM(RTRIM(b.BB_FLG_BUNDLE)),'No Bundle') AS BUNDLE_FLG

from 
dbo.M_SUB_LST_RGR_LISTINO_MAP a right join SF_SBS_SITCOMP b  on (a.lst_rgr_id = b.lst_rgr_id )
join [SKY_REV].[dbo].[M_REV_TEMPO] t on (b.[GIO_ID] = t.[GIO_ID])
join [SKY_REV].[dbo].[M_REV_CALC_WEEK] m on (m.[WEEK_ID] = t.[Wee2_Id] and right(ltrim(rtrim(m.[SCE_ID])), 2) = right(ltrim(rtrim(t.[Ann_Id])), 2))
where a.lst_rev_mr = 1</property>
