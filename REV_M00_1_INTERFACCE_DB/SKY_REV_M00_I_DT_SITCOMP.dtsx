                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[SF_SBS_SITCOMP]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select [GIO_ID] 
      ,[SCE_ID]
      ,[DUR_ID]
      ,[MOP_ID]
      ,[TAB_ID]
      ,[TCN_ID]
      ,[CLS_ID]
      ,[FLG_BB] 
      ,[CLU_IPTV]     
      ,[SDE_ID]
      ,[PCK_ID] 
      ,[STA_ID]
      ,[CAN_ID]
      ,sum([NUM_CLI]) as [NUM_CLI]
      ,sum([SUBS]) as [SUBS]
      ,[IMP_CNN]
      ,[FLX_ID]
	  ,BB_CONTRACT_STATUS
	  ,BB_PRD_ID
	  ,BB_ORIGIN_CLUSTER
	  ,BB_VOICE_FLG
	  ,BUNDLE_FLG
from 
(select s.[GIO_ID] as [GIO_ID] 
      ,m.[SCE_ID]
      ,s.[DUR_ID] as [DUR_ID]
      ,s.[MOP_ID] as [MOP_ID]
      ,s.[TAB_ID] as [TAB_ID]
      ,s.[TCN_ID] as [TCN_ID]
      ,s.[CLS_ID] as [CLS_ID]
      ,s.[FLG_BB] as [FLG_BB] 
      ,s.[CLU_IPTV] as [CLU_IPTV]           
      ,s.[SDE_ID] as [SDE_ID]
      ,s.[PCK_ID] as [PCK_ID] 
      ,s.[STA_ID] as [STA_ID]
      ,s.[CAN_ID] as [CAN_ID]
      ,s.[NUM_CLI] as [NUM_CLI]
      ,s.[SUBS] as [SUBS]
      ,s.[IMP_CNN]+isnull(p.DELTA, 0) as [IMP_CNN]
      ,s.[FLX_ID]
	  ,	ISNULL(LTRIM(RTRIM(s.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
		CASE WHEN BB_PRD_ID='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
			ELSE CAST(ISNULL(LTRIM(RTRIM(BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') AS varchar(80)) END AS BB_PRD_ID,
		CAST(ISNULL(LTRIM(RTRIM(s.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER,
		ISNULL(LTRIM(RTRIM(s.BB_VOICE_FLG)),'N') AS BB_VOICE_FLG,
		ISNULL(LTRIM(RTRIM(s.BB_FLG_BUNDLE)),'No Bundle') AS BUNDLE_FLG
from SF_SBS_SITCOMP s 
join [SKY_REV].[dbo].[M_REV_TEMPO] t on (s.[GIO_ID] = t.[GIO_ID])
join [SKY_REV].[dbo].[M_REV_CALC_WEEK] m on (m.[WEEK_ID] = t.[Wee2_Id] and right(ltrim(rtrim(m.[SCE_ID])), 2) = right(ltrim(rtrim(t.[Ann_Id])), 2))
left join [SKY_REV].[dbo].M_REV_PRICING_DELTA p on (p.[PCK_ID]=s.[PCK_ID] and p.[LST_ID]=s.[LST_ID])) as a
group by [GIO_ID] 
      ,[SCE_ID]
      ,[DUR_ID]
      ,[MOP_ID]
      ,[TAB_ID]
      ,[TCN_ID]
      ,[CLS_ID]
      ,[FLG_BB]
      ,[CLU_IPTV]   
      ,[SDE_ID]
      ,[PCK_ID]
      ,[STA_ID]
      ,[CAN_ID]
      ,[IMP_CNN]
      ,[FLX_ID]
	  ,BB_CONTRACT_STATUS
	  ,BB_PRD_ID
	  ,BB_ORIGIN_CLUSTER
	  ,BB_VOICE_FLG
	  ,BUNDLE_FLG</property>
