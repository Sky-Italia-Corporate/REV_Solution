              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
    week_days.SCE_ID
    ,sf.GIO_ID
    ,sf.PCK_ID
    ,sf.OPT_ID
    ,sf.STM_ID
    ,sf.TCN_ID
    ,sf.CLU_IPTV
    ,sf.SUBS,
	'ZZZ_NO_BB_CONTRACT_STATUS' as BB_CONTRACT_STATUS,
	'ZZZ_NO_BB_PRODUCT' AS BB_PRODUCT_ID,
	'ZZZ_NO_ORIGIN_CLUSTER' as BB_ORIGIN_CLUSTER,
	'N_BB' AS BB_VOICE_FLG,
	'No Bundle' AS BUNDLE_FLG
from SF_SBO_OPTIONS sf 
join (
    select
		  m.SCE_ID, t.Gio_Id
	   from SKY_REV.dbo.M_REV_CALC_WEEK m 
	   inner join SKY_REV.dbo.M_REV_TEMPO t 
		  on right(m.SCE_ID, 2) = right(t.Ann_Id, 2)
			 and m.WEEK_ID = t.Wee2_Id
) week_days
    on sf.GIO_ID = week_days.Gio_Id
join SKY_REV.dbo.T_SUB_OPT_OPTIONS anag_opt
    on sf.OPT_ID = anag_opt.OPT_ID
join (
    select distinct 
	   STM_ID
    from SKY_REV.dbo.M_REV_MAPPING_STATO_OPTIONS
    where MAP = 'Y'
) stm
    on sf.STM_ID = stm.STM_ID
where sf.FM_WK like '%WK%'
    and (
	   anag_opt.OPT_TIPO_VIS = 'STANDARD'or
	   anag_opt.OPT_TIPO_VIS = 'VISIONE CHANGE' or
	   anag_opt.OPT_TIPO_VIS is null
    )
    and (
	   anag_opt.OPT_LEV1_DES &lt;&gt; '3D'
	   or (anag_opt.OPT_LEV1_DES = '3D' and sf.FLG_3D_FREE = 'N')
    )</property>
                <property
