		'ZZZ_NO_ORIGIN_CLUSTER' as BB_ORIGIN_CLUSTER,
		'N' AS BB_VOICE_FLG,
		'No Bundle' AS BUNDLE_FLG
	from SF_SBP_PREMIUMQ sf
	cross join SKY_REV.dbo.M_REV_CALC_WEEK sce 
	where sf.FM_WK like '%WK%' 
		and sf.STM_ID IN ('CLO','ACT') 
		and sf.GIO_ID in (
			select 
				m_tempo.GIO_ID
			from SKY_REV.dbo.M_REV_CALC_WEEK calc_week
			cross join SKY_REV.dbo.M_REV_TEMPO m_tempo
			where right(ltrim(rtrim(calc_week.SCE_ID)), 2) = right(ltrim(rtrim(m_tempo.ANN_ID)), 2) 
				and m_tempo.WEE2_ID = calc_week.WEEK_ID
		)
) import</property>
