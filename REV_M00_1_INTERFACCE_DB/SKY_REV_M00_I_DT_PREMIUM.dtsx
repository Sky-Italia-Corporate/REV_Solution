              description="OLE DB Source"
              name="SF_SBP_PREMIUM"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
s.[GIO_ID]
      ,s.[SCE_ID]
      ,s.[PCK_ID]
      ,s.[STM_ID]
      ,sum(s.[SUBS]) as SUBS
      ,s.[PRS_RC]
      ,s.[MOD_ID]
      ,s.[NEX_DES]
      ,s.[MOP_ID]
      ,s.[TCN_ID]
      ,s.[CLU_IPTV]
      ,s.[CNP_ID]
      ,s.[TIPO_SERVIZIO]
      ,s.[IMP_RC_CANONE_SERV]
	  /* Integrazione TV-BB con aggiunta 4 dimensioni da BB */		
	  ,s.BB_CONTRACT_STATUS
	  ,s.BB_PRD_ID
	  ,s.BB_ORIGIN_CLUSTER
	  ,s.BB_VOICE_FLG
	  ,s.BUNDLE_FLG
	
  FROM 
(
	/*** PREMIUM SERVICES - Solo gli stati CLO e ACT***/
	select	STM_ID, GIO_ID, 'A'+right(ltrim(rtrim(sce.[SCE_ID])), 2) as SCE_ID, PRS_RC, PCK_ID, MOD_ID, NEX_DES, MOP_ID, TCN_ID, UPPER(CLU_IPTV) as CLU_IPTV, CNP_ID, TIPO_SERVIZIO, SUBS, IMP_RC_CANONE_SERV
			,ISNULL(LTRIM(RTRIM(BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS
			,CASE WHEN BB_PRD_ID='ZZZ_NO_BB_PRODUCT_ID' THEN 'ZZZ_NO_BB_PRODUCT'
			ELSE CAST(ISNULL(LTRIM(RTRIM(BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') AS varchar(80)) END AS BB_PRD_ID
			,CAST(ISNULL(LTRIM(RTRIM(BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER
			,ISNULL(LTRIM(RTRIM(BB_VOICE_FLG)),'N') AS BB_VOICE_FLG
			,ISNULL(LTRIM(RTRIM(BB_FLG_BUNDLE)),'No Bundle') AS BUNDLE_FLG
	from [SF_SBP_PREMIUM],
	[SKY_REV].[dbo].[M_REV_CALC_WEEK] sce
	where FM_WK like '%WK%' and STM_ID IN ('CLO','ACT') and GIO_ID in 
	(select b.Gio_Id
	from [SKY_REV].[dbo].[M_REV_CALC_WEEK] a,
	[SKY_REV].[dbo].[M_REV_TEMPO] b
	WHERE right(ltrim(rtrim(a.[SCE_ID])), 2) = right(ltrim(rtrim(b.[Ann_Id])), 2) and b.[Wee2_Id] = a.[WEEK_ID])
) s
GROUP BY  STM_ID, GIO_ID, SCE_ID, PRS_RC, PCK_ID, MOD_ID, NEX_DES, MOP_ID, TCN_ID, CLU_IPTV, CNP_ID, TIPO_SERVIZIO, IMP_RC_CANONE_SERV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG</property>
