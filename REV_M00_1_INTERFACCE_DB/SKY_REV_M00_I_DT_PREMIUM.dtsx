              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*** 13/11/2012 Recupero i soli stati CLO e ACT per tutti i giorni della week corrente, sia per PS che per SKYGO***/

SELECT s.[GIO_ID]
      ,s.[SCE_ID]
      ,s.[PCK_ID]
      ,s.[STM_ID]
      ,s.[SUBS]
      ,s.[PRS_RC]
      ,s.[MOD_ID]
      ,s.[NEX_DES]
      ,s.[MOP_ID]
      ,s.[TCN_ID]
      ,s.[CLU_IPTV]
      ,s.[CNP_ID]
      ,s.[TIPO_SERVIZIO]
      ,s.[IMP_RC_CANONE_SERV]
	  ,s.BB_CONTRACT_STATUS
	  ,s.BB_ORIGIN_CLUSTER
	  ,s.BB_PRD_ID
	  ,s.BB_VOICE_FLG
	  ,s.BUNDLE_FLG
  FROM 
(
/*** ANYWHERE - Solo gli stati CLO e ACT ***/
select STM_ID, GIO_ID, 'A'+right(ltrim(rtrim(sce.[SCE_ID])), 2) as SCE_ID, PRS_RC, PCK_REA_ID PCK_ID, MOD_ID, NEX_DES, MOP_ID, TCN_ID, UPPER(CLU_IPTV) as CLU_IPTV, CNP_ID, TIPO_SERVIZIO, sum(SUBS) SUBS, IMP_RC_SERVIZIO IMP_RC_CANONE_SERV,
'ZZZ_NO_BB_CONTRACT_STATUS' AS BB_CONTRACT_STATUS,
'ZZZ_NO_BB_PRODUCT' AS BB_PRD_ID,
'ZZZ_NO_ORIGIN_CLUSTER' as BB_ORIGIN_CLUSTER,
'N' AS BB_VOICE_FLG,
'No Bundle' AS BUNDLE_FLG
from SF_SBP_ANYWHERE,
[SKY_REV].[dbo].[M_REV_CALC_WEEK] sce
where STM_ID IN ('CLO','ACT') and GIO_ID in 
(select b.Gio_Id
from [SKY_REV].[dbo].[M_REV_CALC_WEEK] a,
[SKY_REV].[dbo].[M_REV_TEMPO] b
WHERE right(ltrim(rtrim(a.[SCE_ID])), 2) = right(ltrim(rtrim(b.[Ann_Id])), 2) and b.[Wee2_Id] = a.[WEEK_ID])
group by STM_ID, GIO_ID, sce.SCE_ID, PRS_RC, PCK_REA_ID, MOD_ID, NEX_DES, MOP_ID, TCN_ID, UPPER(CLU_IPTV), CNP_ID, TIPO_SERVIZIO, IMP_RC_SERVIZIO
)s</property>
