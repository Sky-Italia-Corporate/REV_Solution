                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	upper(CAST(CLS_DES as varchar(50))) as CLS_DES,
		upper(CAST(CLS_ALIAS as varchar(50))) as CLS_ALIAS
from(
	select distinct CLS_DES, 
		case
			when CLS_DES != '***-NO_CLUSTER' then 'Cluster '+CLS_DES
			else CLS_DES
		end CLS_ALIAS
	from S_REV_CLS_CLUSTER  
union
	select distinct CLU_IPTV as CLS_DES, 
		case
			when CLU_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLU_IPTV
			else CLU_IPTV
		end as CLS_ALIAS 
	from SF_REV_TCK_TICKET
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_ARPU /* 83ARPU */
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_ATTIVAZIONI /* 91ActDis */
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_SCONTI_MGMT /* 92mgmt */
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_SCONTI_US_GAAP /* 92mgmt - 95UsGpTT - 94UsGaap */	
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_CHURN /* 97ChuDis - 98ChuMgt */
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_CHURN_MGMT /* 98ChuMgt */
union
	select distinct CLUSTER_IPTV as CLS_DES, 
		case
			when CLUSTER_IPTV != '***-NO_CLUSTER' then 'Cluster '+CLUSTER_IPTV
			else CLUSTER_IPTV
		end as CLS_ALIAS 
	from SF_REV_DIS_CHURN_US_GAAP /* 99ChuGaa */
)cluster
order by 1</property>
