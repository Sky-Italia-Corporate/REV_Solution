                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	RC_SCONTO_PROTEZIONE, DES_SCONTO_PROTEZIONE, IMP_SCONTO_PROTEZIONE, STATO_SCONTO, STATO_CNTR_SIEBEL, 
		RC_PACCHETTO_REALE, TIPO_SCONTO, FLG_PREV_CESS, 
		sum(cast(VOLUMI as int)) as VOLUMI, 
		TECNOLOGIA, CLUSTER_IPTV,
		ACCOUNT,
LEFT(RTRIM(LTRIM(DATARIFERIMENTO)),8) as DATARIFERIMENTO
from TMP_T_REV_DT_AP
where  LEFT(RTRIM(LTRIM(DATARIFERIMENTO)),8) = (	select	cast(MAX(tempo.Gio_Id) as int) as DATARIFERIMENTO
							from	M_REV_TEMPO tempo
							inner join 
									M_REV_CALC_WEEK week 
										on tempo.Wee2_Id  =week.WEEK_ID 
										and SUBSTRING(week.SCE_ID,4,2)=SUBSTRING(tempo.Ann_id,3,2)
						  ) 
group by 
		RC_SCONTO_PROTEZIONE, DES_SCONTO_PROTEZIONE, IMP_SCONTO_PROTEZIONE, STATO_SCONTO, STATO_CNTR_SIEBEL, 
		RC_PACCHETTO_REALE, TIPO_SCONTO, FLG_PREV_CESS, TECNOLOGIA, CLUSTER_IPTV, DATARIFERIMENTO, ACCOUNT</property>
