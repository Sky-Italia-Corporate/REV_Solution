      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CC7084EC-775F-49DD-A3F2-D5A80F853CFC}"
          SQLTask:SqlStatementSource="select cast(MIN(tempo.Gio_Id) as int) as START_DAY&#xA; from M_REV_TEMPO tempo&#xA;inner join M_REV_CALC_WEEK week on tempo.Wee2_Id  =week.WEEK_ID &#xA;        and SUBSTRING(week.SCE_ID,4,2)=SUBSTRING(tempo.Ann_id,3,2)&#xA;order by 1 asc"
