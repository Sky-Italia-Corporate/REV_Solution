    sf.DUR_DES,
    /*************************************************************/
    --- LERO - 20150320 - CR255
    --- Inserita logia che riporta il cluster IPTV se la tecnologia
    --- è IPTV Telecom, se no mantiene il cluster originale
    --- (in caso valorizzato con il fittizio)
    case
	   when sf.TCN_DES like '%SKY IPTV TELECOM%' then upper(sf.CLU_IPTV)
	   else sf.CLS_DES
    end as CLS_DES,
    sum(sf.NUM_CLI) as NUM_CLI,
	ISNULL(LTRIM(RTRIM(sf.BB_CONTRACT_STATUS)),'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS
	,CAST(ISNULL(LTRIM(RTRIM(sf.BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT_ID') AS varchar(80)) AS BB_PRD_ID
	,CAST(ISNULL(LTRIM(RTRIM(sf.BB_ORIGIN_CLUSTER)),'ZZZ_NO_ORIGIN_CLUSTER') AS varchar(80)) as BB_ORIGIN_CLUSTER
	,ISNULL(LTRIM(RTRIM(sf.BB_VOICE_FLG)),'N') AS BB_VOICE_FLG
	,ISNULL(LTRIM(RTRIM(sf.BUNDLE_FLG)),'No Bundle') AS BUNDLE_FLG
from SF_REV_SOSPESI sf
join (
    select
	    Gio_Id
    from M_REV_CALC_WEEK wk
    join M_REV_TEMPO tmp
	   on right(wk.SCE_ID, 2) + right(wk.WEEK_ID, 2) = right(tmp.Ann_id, 2) + right(tmp.Wee2_id, 2)
) week_days
    on sf.GIO_ID = week_days.Gio_Id
group by sf.GIO_ID,
    sf.PCK_ID,
    sf.MOP_ID,
    sf.TCN_DES,
    sf.DUR_DES,
    case
	   when sf.TCN_DES like '%SKY IPTV TELECOM%' then upper(sf.CLU_IPTV)
	   else sf.CLS_DES
    end,
	sf.BB_CONTRACT_STATUS,
	sf.BB_ORIGIN_CLUSTER,
	sf.BB_PRD_ID,
	sf.BB_VOICE_FLG,
	sf.BUNDLE_FLG</property>
