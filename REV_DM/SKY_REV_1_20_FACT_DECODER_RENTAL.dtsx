	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG
from [SF_REV_DECODER_RENTAL] a
where a.SCE_ID in (select SCE_ID FROM M_REV_CALC_WEEK)
  and a.WEEK_ID = (select WEEK_ID FROM M_REV_CALC_WEEK) 
group by 
       a.[GIO_ID]
      ,a.[MOP_ID]
      ,a.[DUR_DES]
      ,a.[TCN_DES]
