                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select convert(varchar(8), cast(a.Gio_Id as datetime) + 1, 112) AS GIO_ID
      ,a.[MOP_ID]
      ,a.[DUR_DES]
      ,a.[TCN_DES]
      ,case
		when a.TCN_DES like '%SKY IPTV TELECOM%' then upper(a.CLU_IPTV)
			else a.CLS_DES
		end as CLS_DES
      ,sum(a.[NUM_CLI]) as [NUM_CLI]
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG
from [SF_REV_DECODER_RENTAL] a
where a.SCE_ID in (select SCE_ID FROM M_REV_CALC_WEEK)
	and a.WEEK_ID = (select WEEK_ID FROM M_REV_CALC_WEEK) 
group by  convert(varchar(8), cast(a.Gio_Id as datetime) + 1, 112)
      ,a.[MOP_ID]
      ,a.[DUR_DES]
      ,a.[TCN_DES]
      ,case
		when a.TCN_DES like '%SKY IPTV TELECOM%' then upper(a.CLU_IPTV)
			else a.CLS_DES
		end
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG


union

select convert(varchar(8), cast(a.Gio_Id as datetime) + 1, 112) AS GIO_ID
      ,a.[MOP_ID]
      ,a.[DUR_DES]
      ,a.[TCN_DES]
      ,case
		when a.TCN_DES like '%SKY IPTV TELECOM%' then upper(a.CLU_IPTV)
			else a.CLS_DES
		end as CLS_DES
      ,sum(a.[NUM_CLI]) as [NUM_CLI]
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG	
	  ,a.BUNDLE_FLG

from [SF_REV_DECODER_RENTAL] a
where gio_id in
	(
		select max(t.Gio_Id)
		from V_REV_PREV_CALC_WEEK m 
		inner join M_REV_TEMPO t 
			on right(ltrim(rtrim(m.SCE_ID)), 2) = right(ltrim(rtrim(t.Ann_Id)), 2)
			and m.WEEK_ID = t.Wee2_Id
	)
group by convert(varchar(8), cast(a.Gio_Id as datetime) + 1, 112)
      ,a.[MOP_ID]
      ,a.[DUR_DES]
      ,a.[TCN_DES]
      ,case
		when a.TCN_DES like '%SKY IPTV TELECOM%' then upper(a.CLU_IPTV)
			else a.CLS_DES
		end
	  ,a.BB_CONTRACT_STATUS
	  ,a.BB_ORIGIN_CLUSTER
	  ,a.BB_PRD_ID
	  ,a.BB_VOICE_FLG
	  ,a.BUNDLE_FLG</property>
