	ELSE CAST(ISNULL(LTRIM(RTRIM(s.BB_PRD_ID)),'ZZZ_NO_BB_PRODUCT') AS varchar(80)) END AS BB_PRD_ID	  
		,s.BB_VOICE_FLG
	  ,s.BUNDLE_FLG
  FROM [SKY_REV].[dbo].[SF_REV_PREZZI_OPE] s
