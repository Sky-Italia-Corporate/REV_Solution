                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	'AUTO' as TIPO_DATO, a.ATT_PER_ID, a.END_PER_ID, b.Period_ID as CMP_PER_ID, TCK2_DES, PCK_ID, CLUSTER, TCN_ID, CLU_IPTV, TCK_PRICE, 
