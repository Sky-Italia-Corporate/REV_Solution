              description="Looks up values in a reference dataset by using exact matching."
              name="Recupera la durata (in giorni) del ticket"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement that generates the lookup table."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	c.TIPO_DATO, c.ATT_PER_ID, c.END_PER_ID, c.TCK2_DES, c.PCK_ID, c.CLUSTER, c.TCN_ID, c.CLU_IPTV, c.TCK_PRICE, 
	SUM(c.PER_DUR) as TCK_DUR
from (
	select
		'AUTO' as TIPO_DATO, a.ATT_PER_ID, a.END_PER_ID, b.Period_ID as CMP_PER_ID, TCK2_DES, PCK_ID, CLUSTER, TCN_ID, CLU_IPTV, TCK_PRICE, 
		DATEDIFF(DD, b.PeriodStart, b.PeriodEnd)+1 as PER_DUR
	from (
		select distinct 
			st.ATT_PER_ID, st.END_PER_ID,
			st.TCK2_DES,st.TCN_ID,st.CLU_IPTV,st.MOP_ID,
			st.PCK_ID,st.CLUSTER,st.TCK_PRICE,st.SUBS,
			st.REV
		from ST_REV_TCK_TICKET st
		) a
	join
		V_M_REV_PERIOD_COMPETENZA b
		on (b.Period_ID between a.ATT_PER_ID and a.END_PER_ID)
	group by a.ATT_PER_ID, a.END_PER_ID, b.Period_ID, TCK2_DES, PCK_ID, CLUSTER, TCN_ID, CLU_IPTV, TCK_PRICE, b.PeriodStart, b.PeriodEnd
	) c
group by c.TIPO_DATO, c.ATT_PER_ID, c.END_PER_ID, c.TCK2_DES, c.PCK_ID, c.CLUSTER, c.TCN_ID, c.CLU_IPTV, c.TCK_PRICE</property>
                <property
                  dataType="System.String"
                  description="Specifies a SQL statement that uses parameters to generate the lookup table."
                  expressionType="Notify"
                  name="SqlCommandParam"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select * from (select
	c.TIPO_DATO, c.ATT_PER_ID, c.END_PER_ID, c.TCK2_DES, c.PCK_ID, c.CLUSTER, c.TCN_ID, c.CLU_IPTV, c.TCK_PRICE, 
	SUM(c.PER_DUR) as TCK_DUR
from (
	select
		'AUTO' as TIPO_DATO, a.ATT_PER_ID, a.END_PER_ID, b.Period_ID as CMP_PER_ID, TCK2_DES, PCK_ID, CLUSTER, TCN_ID, CLU_IPTV, TCK_PRICE, 
		DATEDIFF(DD, b.PeriodStart, b.PeriodEnd)+1 as PER_DUR
	from (
		select distinct 
			st.ATT_PER_ID, st.END_PER_ID,
			st.TCK2_DES,st.TCN_ID,st.CLU_IPTV,st.MOP_ID,
			st.PCK_ID,st.CLUSTER,st.TCK_PRICE,st.SUBS,
			st.REV
		from ST_REV_TCK_TICKET st
		) a
	join
		V_M_REV_PERIOD_COMPETENZA b
		on (b.Period_ID between a.ATT_PER_ID and a.END_PER_ID)
	group by a.ATT_PER_ID, a.END_PER_ID, b.Period_ID, TCK2_DES, PCK_ID, CLUSTER, TCN_ID, CLU_IPTV, TCK_PRICE, b.PeriodStart, b.PeriodEnd
	) c
group by c.TIPO_DATO, c.ATT_PER_ID, c.END_PER_ID, c.TCK2_DES, c.PCK_ID, c.CLUSTER, c.TCN_ID, c.CLU_IPTV, c.TCK_PRICE) [refTable]
where [refTable].[TCK2_DES] = ? and [refTable].[PCK_ID] = ? and [refTable].[CLUSTER] = ? and [refTable].[TCK_PRICE] = ? and [refTable].[TIPO_DATO] = ? and [refTable].[TCN_ID] = ? and [refTable].[CLU_IPTV] = ? and [refTable].[ATT_PER_ID] = ? and [refTable].[END_PER_ID] = ?</property>
                <property
