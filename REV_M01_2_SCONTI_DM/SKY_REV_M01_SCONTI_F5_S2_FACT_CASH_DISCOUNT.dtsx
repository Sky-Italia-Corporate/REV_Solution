              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="PROMO ON FA - Dati da SF_REV_DIS_ATTIVAZIONI"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	/* MAMU 20171130 Aggiunti periodini */
	vAtt.Period_DES as ATT_PER_DES,
	vCmp.Period_DES as CMP_PER_DES,
	case
		when pck.PCK_RGR_DES is null then '***-Raggruppamento Pacchetto Mancante'
		else pck.PCK_RGR_DES
	end as PCK_ID,
	/*vPromo.PRS_L02 as PRS_ID,*/
	/*promo.PROMO_WEEKLY_RGR as PRS_ID,*/ --AGGIUNTA MAMU 14/04/2016       /*ANCC 20170411 Ristrutturazione Gerarchia Promo - esclusione dettaglio Promo Weekly RGR*/        
                case
		when flc.FLG_CAP_COD is null then '***-NO_FLG_CAP'
		else flc.FLG_CAP_COD
	end as FLG_CAP_COD,
	disAtt.TECNOLOGIA as TCN_ID,
	disAtt.CLUSTER_IPTV as CLU_IPTV,
    disAtt.COD_PROMO_TEC as PRT_ID, /*ANCC 20170410 Ristrutturazione Gerarchia Promo*/
	disAtt.BB_CONTRACT_STATUS,
    disAtt.BB_PRD_ID,
    disAtt.BB_ORIGIN_CLUSTER,
    disAtt.BB_VOICE_FLG,
    disAtt.BUNDLE_FLG,
	case 
		when disAtt.COD_PROMO_TEC like '%Q%' and sum(CASH_DISCOUNT)=0 then 0
		else sum(VOLUMI) 
	end as VOLUMI,
	case 
		when disAtt.COD_PROMO_TEC like '%Q%' and sum(CASH_DISCOUNT)=0 then 0
		else sum(VOLUMI_SCONTO) 
	end as VOLUMI_SCONTO,
	sum(CASH_DISCOUNT) as CASH_DISCOUNT,
/**LUME 25/06/2013 : Integrazione ADJ REVDIS**/
	disAtt.TIPO_DATO,
	disAtt.VOUCHER
from SF_REV_DIS_ATTIVAZIONI disAtt
left join S_REV_PCK_PACCHETTO pck
	on disAtt.RC_PACCHETTO = pck.PCK_RC
join S_PROMO promo
	on disAtt.COD_PROMO_TEC = promo.PROMO_ID
join V_M_REV_PERIOD_ATTIVAZIONE vAtt
	on disAtt.ATT_PER_ID = vAtt.Period_ID
join V_M_REV_PERIOD_COMPETENZA vCmp
	on disAtt.ATT_PER_ID = vCmp.Period_ID
/*MAMU 14/04/2016 : decommentato questo join perche inutile e inoltre fatto su una descrizione */
/*join V_REV_PRS_PROMO_SKY vPromo
	on 'PRA - ' + promo.PROMO_DES = vPromo.PRS_L00*/

left join S_REV_FLC_FLAG_CAP_CRITICO flc
	on disAtt.FLG_CAP_ID = flc.FLG_CAP_ID
where ATT_PER_ID in (	select distinct Period_ID
						from V_M_REV_PERIOD_ATTIVAZIONE V
						join M_REV_CALC_WEEK M
						on replace(M.SCE_ID,'ACT','FY')+'-'+M.WEEK_ID = left(V.FiscalWeek,8))
	and promo.PROMO_TYPE = 'FA'
group by vAtt.Period_DES,
		vCmp.Period_DES, 
		PCK_RGR_DES, 
		/*vPromo.PRS_L02,*/ 
		promo.PROMO_WEEKLY_RGR,   --AGGIUNTA MAMU 14/04/2016
        TIPO_DATO, 
		VOUCHER, 
		flc.FLG_CAP_COD,
		disAtt.TECNOLOGIA,
		disAtt.CLUSTER_IPTV,
        disAtt.COD_PROMO_TEC,
		disAtt.BB_CONTRACT_STATUS,
		disAtt.BB_PRD_ID,
		disAtt.BB_ORIGIN_CLUSTER,
		disAtt.BB_VOICE_FLG,
		disAtt.BUNDLE_FLG</property>
