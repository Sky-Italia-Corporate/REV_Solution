            <component
              refId="Package\Crea Fact Table\CHURN PROMO - Competenziazione MGMT - (Precedenti al rilascio &quot;Ristrutturazione Gerarchia Promo&quot;)"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              localeId="1033"
              name="CHURN PROMO - Competenziazione MGMT - (Precedenti al rilascio &quot;Ristrutturazione Gerarchia Promo&quot;)"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	vAtt.Period_DES as ATT_PER_DES,
	vCmp.Period_DES as CMP_PER_DES,
	'AUTO' as TIPO_DATO, 
	'V_NO' as VOUCHER,
	case
		when pck.PCK_RGR_DES is null then '***-Raggruppamento Pacchetto Mancante'
		else pck.PCK_RGR_DES
	end as PCK_ID,
	/*PRS_L02 as PRS_ID,*/
	/*promo.PROMO_WEEKLY_RGR as PRS_ID,*/ --AGGIUNTA MAMU 14/04/2016       /*ANCC 20170411 Ristrutturazione Gerarchia Promo - esclusione dettaglio Promo Weekly RGR*/  
	disChurnMGMT.TECNOLOGIA as TCN_ID,
	disChurnMGMT.CLUSTER_IPTV as CLU_IPTV,
		/* VIST 07/07/2020 Integrazione TV-BB */
	disChurnMGMT.BB_CONTRACT_STATUS, 
	disChurnMGMT.BB_PRD_ID, 
	disChurnMGMT.BB_ORIGIN_CLUSTER,
	disChurnMGMT.BB_VOICE_FLG, 
	disChurnMGMT.BUNDLE_FLG,
    storico.PROMO_ID as PRT_ID,    /*ANCC 20170410 Ristrutturazione Gerarchia Promo - nel passaggio da Promo Weekly RGR a Promo Tecnica, si è deciso di mettere a fittizio lo storico*/
	sum(VOLUMI) as VOLUMI,
	sum(VALORE_PROMO) as VALORE_PROMO,
	flag.FLG_CAP_COD
from SF_REV_DIS_CHURN_MGMT disChurnMGMT
left join S_REV_PCK_PACCHETTO pck
	on disChurnMGMT.RC_PACCHETTO = pck.PCK_RC
join S_PROMO promo
	on disChurnMGMT.COD_PROMO_TEC = promo.PROMO_ID
left join S_REV_FLC_FLAG_CAP_CRITICO flag
               on disChurnMGMT.FLG_CAP_ID = flag.FLG_CAP_ID
/*ANCC 20170410 Ristrutturazione Gerarchia Promo - nel passaggio da Promo Weekly RGR a Promo Tecnica, si è deciso di mettere a fittizio lo storico*/
join S_PROMO_STORICO storico
	on CASE WHEN REPLACE(LEFT(promo.PROMO_WEEKLY_RGR,75),'€','E') = 'PREV_1 Premium Pack "Decreasing Discount" for 12 Months' THEN 'PREV_1 Premium Pack  Decreasing Discount  for 12 Months'
		ELSE REPLACE(LEFT(promo.PROMO_WEEKLY_RGR,75),'€','E')
		END
		= REPLACE(storico.PROMO_WEEKLY_RGR,'€','E')
join V_M_REV_PERIOD_ATTIVAZIONE vAtt
	on disChurnMGMT.ATT_PER_ID = vAtt.Period_ID
join V_M_REV_PERIOD_COMPETENZA vCmp
	on disChurnMGMT.CMP_PER_ID = vCmp.Period_ID
, M_REV_RELEASE_DATE_MOTORE_SCONTI reldat
where right(left(vAtt.FiscalWeek,4),2)+right(left(vAtt.FiscalWeek,8),2)&lt;=RIGHT(RELDAT.SCE_ID,2)+RIGHT(RELDAT.WEEK_ID,2)         /*ANCC 20170410 Ristrutturazione Gerarchia Promo*/

/*MAMU 14/04/2016 : decommentato questo join perche inutile e inoltre fatto su una descrizione */
/*join V_REV_PRS_PROMO_SKY vPromo
	on 
		case
			when promo.PROMO_TYPE = 'FA' then 'PRA - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'SB' then 'PRB - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'TK' then 'PRK - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'AC' then 'PRC - ' + promo.PROMO_DES
			else 'PRP - ' + promo.PROMO_DES
		end
	= vPromo.PRS_L00*/

group by  
		PCK_RGR_DES, 
		vAtt.Period_DES,
		vCmp.Period_DES,
		/*PRS_L02,*/
        /*promo.PROMO_WEEKLY_RGR,*/
        storico.PROMO_ID, /*ANCC 20170410 Ristrutturazione Gerarchia Promo - nel passaggio da Promo Weekly RGR a Promo Tecnica, si è deciso di mettere a fittizio lo storico*/
		disChurnMGMT.TECNOLOGIA,
		disChurnMGMT.CLUSTER_IPTV,
		disChurnMGMT.BB_CONTRACT_STATUS, 
	disChurnMGMT.BB_PRD_ID, 
	disChurnMGMT.BB_ORIGIN_CLUSTER,
	disChurnMGMT.BB_VOICE_FLG, 
	disChurnMGMT.BUNDLE_FLG,
	flag.FLG_CAP_COD</property>
