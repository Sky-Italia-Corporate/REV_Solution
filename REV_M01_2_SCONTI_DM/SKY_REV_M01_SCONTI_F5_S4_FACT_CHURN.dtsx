			when promo.PROMO_TYPE = 'TK' then 'PRK - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'AC' then 'PRC - ' + promo.PROMO_DES
			else 'PRP - ' + promo.PROMO_DES
		end = vPromo.PRS_L00*/
group by 
	vAtt.Period_DES,
	vCmp.Period_DES,
	pck.PCK_RGR_DES, 
	/*vPromo.PRS_L02, */
    promo.PROMO_WEEKLY_RGR,
	disChurnUsGAAP.TECNOLOGIA,
	disChurnUsGAAP.CLUSTER_IPTV,
		disChurnUsGAAP.BB_CONTRACT_STATUS, 
	disChurnUsGAAP.BB_PRD_ID, 
	disChurnUsGAAP.BB_ORIGIN_CLUSTER,
	disChurnUsGAAP.BB_VOICE_FLG, 
	disChurnUsGAAP.BUNDLE_FLG,
                flag.FLG_CAP_COD,
	case
			when disChurnUsGAAP.COD_PROMO_TEC = '***-NO_PROMO_TECNICA' then disChurnUsGAAP.COD_PROMO_TEC
            when promo.PROMO_TYPE = 'FA' and  CHARINDEX('-9', disChurnUsGAAP.COD_PROMO_TEC,1) =0 then 'PRA - ' + disChurnUsGAAP.COD_PROMO_TEC
			when promo.PROMO_TYPE = 'SB' and  CHARINDEX('-9', disChurnUsGAAP.COD_PROMO_TEC,1) =0then 'PRB - ' + disChurnUsGAAP.COD_PROMO_TEC
			when promo.PROMO_TYPE = 'TK' and  CHARINDEX('-9', disChurnUsGAAP.COD_PROMO_TEC,1) =0 then 'PRK - ' + disChurnUsGAAP.COD_PROMO_TEC
			when promo.PROMO_TYPE = 'AC' and  CHARINDEX('-9', disChurnUsGAAP.COD_PROMO_TEC,1) =0then 'PRC - ' + disChurnUsGAAP.COD_PROMO_TEC
			when promo.PROMO_TYPE = 'PS' and  CHARINDEX('-9', disChurnUsGAAP.COD_PROMO_TEC,1) =0 then 'PRP - ' + disChurnUsGAAP.COD_PROMO_TEC
            else disChurnUsGAAP.COD_PROMO_TEC
		end</property>
