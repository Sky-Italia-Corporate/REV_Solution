                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	vAtt.Period_DES as ATT_PER_DES,
	vCmp.Period_DES as CMP_PER_DES,
	'AUTO' as TIPO_DATO, 
	'V_NO' as VOUCHER,
	case
		when pck.PCK_RGR_DES is null then '***-Raggruppamento Pacchetto Mancante'
		else pck.PCK_RGR_DES
	end as PCK_ID,
	/*PRS_L02 as PRS_ID,*/
	/*promo.PROMO_WEEKLY_RGR as PRS_ID,*/ --AGGIUNTA MAMU 14/04/2016       /*ANCC 20170411 Ristrutturazione Gerarchia Promo - esclusione dettaglio Promo Weekly RGR*/  
	disChurnMGMT.TECNOLOGIA as TCN_ID,
	disChurnMGMT.CLUSTER_IPTV as CLU_IPTV,
			/* VIST 07/07/2020 Integrazione TV-BB */
	disChurnMGMT.BB_CONTRACT_STATUS, 
	disChurnMGMT.BB_PRD_ID, 
	disChurnMGMT.BB_ORIGIN_CLUSTER,
	disChurnMGMT.BB_VOICE_FLG, 
	disChurnMGMT.BUNDLE_FLG,
	/*ANCC 20170410 Ristrutturazione Gerarchia Promo*/
                cast(case
						when disChurnMGMT.COD_PROMO_TEC = '***-NO_PROMO_TECNICA' then disChurnMGMT.COD_PROMO_TEC
						when promo.PROMO_TYPE = 'FA' and CHARINDEX('-9', disChurnMGMT.COD_PROMO_TEC,1) =0  then 'PRA - ' + disChurnMGMT.COD_PROMO_TEC
						when promo.PROMO_TYPE = 'SB' and CHARINDEX('-9', disChurnMGMT.COD_PROMO_TEC,1) =0 then 'PRB - ' + disChurnMGMT.COD_PROMO_TEC
						when promo.PROMO_TYPE = 'TK' and CHARINDEX('-9', disChurnMGMT.COD_PROMO_TEC,1) =0  then 'PRK - ' + disChurnMGMT.COD_PROMO_TEC
						when promo.PROMO_TYPE = 'AC' and CHARINDEX('-9', disChurnMGMT.COD_PROMO_TEC,1) =0  then 'PRC - ' + disChurnMGMT.COD_PROMO_TEC
						when promo.PROMO_TYPE = 'PS' and CHARINDEX('-9', disChurnMGMT.COD_PROMO_TEC,1) =0  then 'PRP - ' + disChurnMGMT.COD_PROMO_TEC
						else disChurnMGMT.COD_PROMO_TEC
					end as varchar(80)) as PRT_ID,	
                sum(VOLUMI) as VOLUMI,
	sum(VALORE_PROMO) as VALORE_PROMO,
	flag.FLG_CAP_COD
from SF_REV_DIS_CHURN_MGMT disChurnMGMT
left join S_REV_PCK_PACCHETTO pck
	on disChurnMGMT.RC_PACCHETTO = pck.PCK_RC
join S_PROMO promo
	on disChurnMGMT.COD_PROMO_TEC = promo.PROMO_ID
left join S_REV_FLC_FLAG_CAP_CRITICO flag
               on disChurnMGMT.FLG_CAP_ID = flag.FLG_CAP_ID
join V_M_REV_PERIOD_ATTIVAZIONE vAtt
	on disChurnMGMT.ATT_PER_ID = vAtt.Period_ID
join V_M_REV_PERIOD_COMPETENZA vCmp
	on disChurnMGMT.CMP_PER_ID = vCmp.Period_ID
, M_REV_RELEASE_DATE_MOTORE_SCONTI reldat
where  right(left(vAtt.FiscalWeek,4),2)+right(left(vAtt.FiscalWeek,8),2)&gt;RIGHT(RELDAT.SCE_ID,2)+RIGHT(RELDAT.WEEK_ID,2)     /*ANCC 20170410 Ristrutturazione Gerarchia Promo*/

