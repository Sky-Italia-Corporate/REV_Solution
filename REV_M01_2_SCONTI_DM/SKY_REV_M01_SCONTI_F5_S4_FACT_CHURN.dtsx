                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	vAtt.Period_DES as ATT_PER_DES,
	vCmp.Period_DES as CMP_PER_DES,
	'AUTO' as TIPO_DATO, 
	'V_NO' as VOUCHER,
	case
		when pck.PCK_RGR_DES is null then '***-Raggruppamento Pacchetto Mancante'
		else pck.PCK_RGR_DES 
	end as PCK_ID,
	/*vPromo.PRS_L02 as PRS_ID,*/
	/*promo.PROMO_WEEKLY_RGR as PRS_ID,*/ --AGGIUNTA MAMU 14/04/2016       /*ANCC 20170411 Ristrutturazione Gerarchia Promo - esclusione dettaglio Promo Weekly RGR*/ 
	disChurn.TECNOLOGIA as TCN_ID,
	disChurn.CLUSTER_IPTV as CLU_IPTV,
		/* VIST 07/07/2020 Integrazione TV-BB */
	disChurn.BB_CONTRACT_STATUS, 
	disChurn.BB_PRD_ID, 
	disChurn.BB_ORIGIN_CLUSTER,
	disChurn.BB_VOICE_FLG, 
	disChurn.BUNDLE_FLG,

       cast(case
				when disChurn.COD_PROMO_TEC = '***-NO_PROMO_TECNICA' then disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'FA' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRA - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'SB' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRB - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'TK' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRK - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'AC' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRC - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'PS' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRP - ' + disChurn.COD_PROMO_TEC
				else  disChurn.COD_PROMO_TEC
			end as varchar(80)) as PRT_ID,
	case 
		when 
			(
			case
				when disChurn.COD_PROMO_TEC = '***-NO_PROMO_TECNICA' then disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'FA' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRA - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'SB' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRB - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'TK' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRK - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'AC' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRC - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'PS' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRP - ' + disChurn.COD_PROMO_TEC
				else  disChurn.COD_PROMO_TEC
			end
			) like '%Q%' and sum(CHURN_DISCOUNT)=0 then 0
		else sum(VOLUMI_CHURN)
	end as VOLUMI_CHURN,
	sum(CHURN_DISCOUNT) as CHURN_DISCOUNT,
	flag.FLG_CAP_COD
from SF_REV_DIS_CHURN disChurn
left join S_REV_PCK_PACCHETTO pck
	on disChurn.RC_PACCHETTO = pck.PCK_RC
left join S_REV_FLC_FLAG_CAP_CRITICO flag
               on disChurn.FLG_CAP_ID = flag.FLG_CAP_ID
join S_PROMO promo
	on cast(disChurn.COD_PROMO_TEC as varchar(80))= cast(promo.PROMO_ID as varchar(80))
/*MAMU 14/04/2016 : decommentato questo join perche inutile e inoltre fatto su una descrizione */
/*join V_REV_PRS_PROMO_SKY vPromo
	on 
		case
			when promo.PROMO_TYPE = 'FA' then 'PRA - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'SB' then 'PRB - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'TK' then 'PRK - ' + promo.PROMO_DES
			when promo.PROMO_TYPE = 'AC' then 'PRC - ' + promo.PROMO_DES
			else 'PRP - ' + promo.PROMO_DES
		end
	= vPromo.PRS_L00*/
join V_M_REV_PERIOD_ATTIVAZIONE vAtt
	on disChurn.ATT_PER_ID = vAtt.Period_ID
join V_M_REV_PERIOD_COMPETENZA vCmp
	on disChurn.ATT_PER_ID = vCmp.Period_ID
where ATT_PER_ID in (select distinct Period_ID
					from V_M_REV_PERIOD_ATTIVAZIONE vAtt
					join M_REV_CALC_WEEK M
					on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
group by 	
		vAtt.Period_DES,
		vCmp.Period_DES,
		PCK_RGR_DES,
		/*vPromo.PRS_L02,*/
        promo.PROMO_WEEKLY_RGR,
		disChurn.TECNOLOGIA,
		disChurn.CLUSTER_IPTV,	
			disChurn.BB_CONTRACT_STATUS, 
	disChurn.BB_PRD_ID, 
	disChurn.BB_ORIGIN_CLUSTER,
	disChurn.BB_VOICE_FLG, 
	disChurn.BUNDLE_FLG,
		flag.FLG_CAP_COD,
		
            case
				when disChurn.COD_PROMO_TEC = '***-NO_PROMO_TECNICA' then disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'FA' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0  then 'PRA - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'SB' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRB - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'TK' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRK - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'AC' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRC - ' + disChurn.COD_PROMO_TEC
				when promo.PROMO_TYPE = 'PS' and CHARINDEX('-9', disChurn.COD_PROMO_TEC,1) =0 then 'PRP - ' + disChurn.COD_PROMO_TEC
				else disChurn.COD_PROMO_TEC
			end</property>
