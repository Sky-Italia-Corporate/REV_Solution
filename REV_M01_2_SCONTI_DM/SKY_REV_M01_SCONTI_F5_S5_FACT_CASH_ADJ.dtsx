          <components>
            <component
              refId="Package\Inserisce i dati settimanali\Dati relativi al cash discount ADJ settimanale"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Dati relativi al cash discount ADJ settimanale"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*** Discounts on FA ***/
select 
	CMP_PER_DES, ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
	BB_CONTRACT_STATUS, 
	BB_PRD_ID, 
	BB_ORIGIN_CLUSTER,
	BB_VOICE_FLG, 
	BUNDLE_FLG,
	sum(CASH_ADJ) as CASH_ADJ
from (
	select 
		CMP_PER_DES, a.ATT_PER_DES, a.TIPO_DATO, a.VOUCHER, a.PCK_ID, CAST(case when A.PRT_ID ='***-NO_PROMO_TECNICA' then A.PRT_ID  else  'PRA - '+replace(a.PRT_ID,'€', 'E') end AS VARCHAR(80)) as PRT_ID, a.FLG_CAP_COD, 
		a.TCN_ID, a.CLU_IPTV, 
		a.BB_CONTRACT_STATUS, 
		a.BB_PRD_ID, 
		a.BB_ORIGIN_CLUSTER,
		a.BB_VOICE_FLG, 
		a.BUNDLE_FLG,
		(a.VAL_FIRST - b.VAL_LAST) * (-1) as CASH_ADJ
	from (  select
				REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES,
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, 
				BB_PRD_ID, 
				BB_ORIGIN_CLUSTER,
				BB_VOICE_FLG, 
				BUNDLE_FLG,
				sum(US_GAAP) as VAL_FIRST
			from F_REV_ACT_US_GAAP F 
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
											from V_M_REV_PERIOD_ATTIVAZIONE vAtt
											join M_REV_CALC_WEEK M
											on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, TIPO_DATO, VOUCHER, BB_CONTRACT_STATUS, 
						BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG) a
	join (  select 
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, 
				BB_PRD_ID, 
				BB_ORIGIN_CLUSTER,
				BB_VOICE_FLG, 
				BUNDLE_FLG,
				sum(CASH_DISCOUNT) as VAL_LAST
			from F_REV_ACT_PROMO F 
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
											from V_M_REV_PERIOD_ATTIVAZIONE vAtt
											join M_REV_CALC_WEEK M
											on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, 
					BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,TIPO_DATO, VOUCHER ) b
		on a.ATT_PER_DES = b.ATT_PER_DES and a.PCK_ID = b.PCK_ID
			and a.PRT_ID = b.PRT_ID and a.TIPO_DATO = b.TIPO_DATO and a.VOUCHER = b.VOUCHER
			and a.FLG_CAP_COD = b.FLG_CAP_COD
			and a.TCN_ID = b.TCN_ID and a.CLU_IPTV = b.CLU_IPTV and a.BB_CONTRACT_STATUS = b.BB_CONTRACT_STATUS 
			and a.BB_PRD_ID = b.BB_PRD_ID and a.BB_ORIGIN_CLUSTER = b.BB_ORIGIN_CLUSTER 
			and a.BB_VOICE_FLG = b.BB_VOICE_FLG and a.BUNDLE_FLG = b.BUNDLE_FLG
	----------------------------------------
	union
	----------------------------------------
	select
		REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES, 
		ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, CAST(case when PRT_ID ='***-NO_PROMO_TECNICA' then PRT_ID else  replace(PRT_ID,'€', 'E') end AS VARCHAR(80)) as PRT_ID, '***-NO_FLG_CAP' as FLG_CAP_COD, TCN_ID, CLU_IPTV, 
		BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(US_GAAP * -1) as CHURN
	from F_REV_DIS_CHURN_US_GAAP F
	join V_M_REV_PERIOD_ATTIVAZIONE V
		on F.ATT_PER_DES = V.Period_DES
	where v.FiscalWeek in (select distinct vAtt.FiscalWeek
											from V_M_REV_PERIOD_ATTIVAZIONE vAtt
											join M_REV_CALC_WEEK M
											on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
	group by ATT_PER_DES, PCK_ID, PRT_ID, TIPO_DATO, VOUCHER, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, 
					BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG
) FA
group by CMP_PER_DES, ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, 
					BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG
----------------------------------------------------------------------------------------------------------------------------------------------------
union all
----------------------------------------------------------------------------------------------------------------------------------------------------
select 
	CMP_PER_DES, ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
	BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(CASH_ADJ) as CASH_ADJ
from (
	/*** Discounts on SB ***/
	select 
		CMP_PER_DES, a.ATT_PER_DES, a.TIPO_DATO, a.VOUCHER, a.PCK_ID,case when A.PRT_ID ='***-NO_PROMO_TECNICA' then A.PRT_ID else  'PRB - '+replace(a.PRT_ID,'€', 'E') end as PRT_ID, a.FLG_CAP_COD, a.TCN_ID, a.CLU_IPTV, 
		a.BB_CONTRACT_STATUS, a.BB_PRD_ID, a.BB_ORIGIN_CLUSTER, a.BB_VOICE_FLG, a.BUNDLE_FLG,(a.VAL_FIRST - b.VAL_LAST) * (-1) as CASH_ADJ
	from (  select
				REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES,
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(VALORE_PROMO) as VAL_FIRST
			from F_REV_SUB_PROMO_SKY F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, 
					TIPO_DATO, VOUCHER  ) a
	join (  select 
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(CASH_DISCOUNT) as VAL_LAST
			from F_REV_SUB_PROMO F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,
					TIPO_DATO, VOUCHER ) b
		on  a.ATT_PER_DES = b.ATT_PER_DES and a.PCK_ID = b.PCK_ID
			and a.PRT_ID = b.PRT_ID and a.TIPO_DATO = b.TIPO_DATO and a.VOUCHER = b.VOUCHER
			and a.FLG_CAP_COD = b.FLG_CAP_COD
			and a.TCN_ID = b.TCN_ID and a.CLU_IPTV = b.CLU_IPTV
			and a.BB_CONTRACT_STATUS = b.BB_CONTRACT_STATUS 
			and a.BB_PRD_ID = b.BB_PRD_ID and a.BB_ORIGIN_CLUSTER = b.BB_ORIGIN_CLUSTER 
			and a.BB_VOICE_FLG = b.BB_VOICE_FLG and a.BUNDLE_FLG = b.BUNDLE_FLG

	union all

	/*** Discounts on PS ***/
	select 
		CMP_PER_DES, a.ATT_PER_DES, a.TIPO_DATO, a.VOUCHER, a.PCK_ID, case when A.PRT_ID ='***-NO_PROMO_TECNICA' then A.PRT_ID else  'PRP - '+replace(a.PRT_ID,'€', 'E') end as PRT_ID, a.FLG_CAP_COD, a.TCN_ID, a.CLU_IPTV, 
		a.BB_CONTRACT_STATUS, a.BB_PRD_ID, a.BB_ORIGIN_CLUSTER, a.BB_VOICE_FLG, a.BUNDLE_FLG, (a.VAL_FIRST - b.VAL_LAST) * (-1) as CASH_ADJ
	from (  select
				REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES,
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(VALORE_PROMO) as VAL_FIRST
			from F_REV_PRS_PROMO_SKY F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, TIPO_DATO, VOUCHER, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG  ) a
	join (  select 
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(CASH_DISCOUNT) as VAL_LAST
			from F_REV_PRS_PROMO F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,
					TIPO_DATO, VOUCHER ) b
		on  a.ATT_PER_DES = b.ATT_PER_DES and a.PCK_ID = b.PCK_ID
			and a.PRT_ID = b.PRT_ID and a.TIPO_DATO = b.TIPO_DATO and a.VOUCHER = b.VOUCHER
			and a.FLG_CAP_COD = b.FLG_CAP_COD
			and a.TCN_ID = b.TCN_ID and a.CLU_IPTV = b.CLU_IPTV
			and a.BB_CONTRACT_STATUS = b.BB_CONTRACT_STATUS 
			and a.BB_PRD_ID = b.BB_PRD_ID and a.BB_ORIGIN_CLUSTER = b.BB_ORIGIN_CLUSTER 
			and a.BB_VOICE_FLG = b.BB_VOICE_FLG and a.BUNDLE_FLG = b.BUNDLE_FLG

	union all

	/*** Discounts on TK ***/
	select 
		CMP_PER_DES, a.ATT_PER_DES, a.TIPO_DATO, a.VOUCHER, a.PCK_ID,case when A.PRT_ID ='***-NO_PROMO_TECNICA' then A.PRT_ID else  'PRK - '+replace(a.PRT_ID,'€', 'E') end as PRT_ID, a.FLG_CAP_COD, a.TCN_ID, a.CLU_IPTV, 
		a.BB_CONTRACT_STATUS, a.BB_PRD_ID, a.BB_ORIGIN_CLUSTER, a.BB_VOICE_FLG, a.BUNDLE_FLG, (a.VAL_FIRST - b.VAL_LAST) * (-1) as CASH_ADJ
	from (  select
				REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES,
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				 BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(VALORE_PROMO) as VAL_FIRST
			from F_REV_TCK_PROMO_SKY F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV,  BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, 
					TIPO_DATO, VOUCHER  ) a
	join (  select 
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				 BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(CASH_DISCOUNT) as VAL_LAST
			from F_REV_TCK_PROMO F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV,  BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,
						TIPO_DATO, VOUCHER ) b
		on  a.ATT_PER_DES = b.ATT_PER_DES and a.PCK_ID = b.PCK_ID
			and a.PRT_ID = b.PRT_ID and a.TIPO_DATO = b.TIPO_DATO and a.VOUCHER = b.VOUCHER
			and a.FLG_CAP_COD = b.FLG_CAP_COD
			and a.TCN_ID = b.TCN_ID and a.CLU_IPTV = b.CLU_IPTV
			and a.BB_CONTRACT_STATUS = b.BB_CONTRACT_STATUS 
			and a.BB_PRD_ID = b.BB_PRD_ID and a.BB_ORIGIN_CLUSTER = b.BB_ORIGIN_CLUSTER 
			and a.BB_VOICE_FLG = b.BB_VOICE_FLG and a.BUNDLE_FLG = b.BUNDLE_FLG

	union all

	/*** Discounts on AC ***/
	select 
		CMP_PER_DES, a.ATT_PER_DES, a.TIPO_DATO, a.VOUCHER, a.PCK_ID,case when A.PRT_ID ='***-NO_PROMO_TECNICA' then A.PRT_ID else  'PRC - '+replace(a.PRT_ID,'€', 'E') end as PRT_ID, a.FLG_CAP_COD, a.TCN_ID, a.CLU_IPTV, 
		a.BB_CONTRACT_STATUS, a.BB_PRD_ID, a.BB_ORIGIN_CLUSTER, a.BB_VOICE_FLG, a.BUNDLE_FLG,(a.VAL_FIRST - b.VAL_LAST) * (-1) as CASH_ADJ
	from (  select
				REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES,
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(VALORE_PROMO) as VAL_FIRST
			from F_REV_ALC_PROMO_SKY F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,
					TIPO_DATO, VOUCHER  ) a
	join (  select 
				ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, 
				BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, sum(CASH_DISCOUNT) as VAL_LAST
			from F_REV_ALC_PROMO F
			join V_M_REV_PERIOD_ATTIVAZIONE V
				on F.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
			group by ATT_PER_DES, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG,
					TIPO_DATO, VOUCHER ) b
		on  a.ATT_PER_DES = b.ATT_PER_DES and a.PCK_ID = b.PCK_ID
			and a.PRT_ID = b.PRT_ID and a.TIPO_DATO = b.TIPO_DATO and a.VOUCHER = b.VOUCHER
			and a.FLG_CAP_COD = b.FLG_CAP_COD
			and a.TCN_ID = b.TCN_ID and a.CLU_IPTV = b.CLU_IPTV
			and a.BB_CONTRACT_STATUS = b.BB_CONTRACT_STATUS 
			and a.BB_PRD_ID = b.BB_PRD_ID and a.BB_ORIGIN_CLUSTER = b.BB_ORIGIN_CLUSTER 
			and a.BB_VOICE_FLG = b.BB_VOICE_FLG and a.BUNDLE_FLG = b.BUNDLE_FLG
	union
	
	select
		REPLACE(ATT_PER_DES,'Att.','Cmp.') as CMP_PER_DES, 
		ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, 
		CASE WHEN churnMGMT.PRT_ID='***-NO_PROMO_TECNICA' THEN churnMGMT.PRT_ID 
			else replace(churnMGMT.PRT_ID,'€', 'E')
		END as PRT_ID, 
		'***-NO_FLG_CAP' as FLG_CAP_COD, TCN_ID, CLU_IPTV, 
		BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG, 
		sum(VALORE_PROMO * -1) as CHURN
	from F_REV_DIS_CHURN_MGMT churnMGMT
	join ( 
		select distinct PROMO_ID as PRT_ID, PROMO_TYPE
		from S_PROMO
		where PROMO_TYPE != 'FA'
	) promo 
		on substring(churnMGMT.PRT_ID,7,10) = promo.PRT_ID 
	join V_M_REV_PERIOD_ATTIVAZIONE V
				on churnMGMT.ATT_PER_DES = V.Period_DES
			where v.FiscalWeek in (select distinct vAtt.FiscalWeek
													from V_M_REV_PERIOD_ATTIVAZIONE vAtt
													join M_REV_CALC_WEEK M
													on left(vAtt.FiscalWeek,8) = REPLACE(SCE_ID,'ACT','FY')+'-'+WEEK_ID)
	group by ATT_PER_DES, PCK_ID, CASE WHEN churnMGMT.PRT_ID='***-NO_PROMO_TECNICA' THEN churnMGMT.PRT_ID 
		else replace(churnMGMT.PRT_ID,'€', 'E')
		END , TIPO_DATO, VOUCHER, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG
		
) SbPsTkAc
group by CMP_PER_DES, ATT_PER_DES, TIPO_DATO, VOUCHER, PCK_ID, PRT_ID, FLG_CAP_COD, TCN_ID, CLU_IPTV, BB_CONTRACT_STATUS, BB_PRD_ID, BB_ORIGIN_CLUSTER, BB_VOICE_FLG, BUNDLE_FLG</property>
