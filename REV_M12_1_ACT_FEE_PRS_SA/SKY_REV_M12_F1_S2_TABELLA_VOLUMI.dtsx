                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
	ACCOUNT, COMBINAZIONE_PREMIUM_ATVZ,
	case
		/*** MYSKYHD - casistica attivazione multipla dove trova MYSKYHD+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 8
			then 1 + len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-8)), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-8)))
		/*** MYSKYHD - casistica attivazione multipla dove trova UP MV MYSKYHD+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 14
			then 1 + len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-14)), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-14)))
		/*** MYSKYHD - casistica attivazione multipla dove non trova MYSKYHD+ o UP MV MYSKYHD+ ***/
		when (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 8) and (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 14 ) and len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) &gt; 0 
			then 0 + len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ))), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ))))
		/*** MYSKYHD - casistica attivazione singola ***/
		when len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) = 0
			and len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				+ len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) &gt;= 0
			then len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
				+ len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)
		else
			0
	end as COUNT_MYSKYHD,
	case
		/*** MYSKY - casistica attivazione multipla dove trova MYSKY+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 6 and left(COMBINAZIONE_PREMIUM_ATVZ, 5) = 'MYSKY'
			then 1 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-6)), 'UP MV MYSKY', 'UP MV MYSKY#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-6))))
				- (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-6)), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-6))))
		/*** MYSKY - casistica attivazione multipla dove trova UP MV MYSKY+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 12
			then 1 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-12)), 'UP MV MYSKY', 'UP MV MYSKY#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-12))))
				- (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-12)), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-12))))
		/*** MYSKY - casistica attivazione multipla dove NON trova MYSKY+ o UP MV MYSKY+ ***/
		when (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 6) and (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 12) and len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) &gt; 0
			then 0 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ))), 'UP MV MYSKY', 'UP MV MYSKY#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ)))))
				- (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ))), 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ)))))
		/*** MYSKY - casistica attivazione singola ***/
		when len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) = 0
			and len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKY', 'MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				+ (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKY', 'UP MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) &gt;= 0
			then len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKY', 'MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
				+ (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKY', 'UP MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
		else
			0
	end as COUNT_MYSKY,
	case
		/*** HD - casistica attivazione multipla dove trova HD+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 3
			then 1 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-3)), 'UP MV HD', 'UP MV HD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-3))))
		/*** HD - casistica attivazione multipla dove trova UP MV HD+ ***/
		when charindex('+', COMBINAZIONE_PREMIUM_ATVZ) = 9
			then 1 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-9)), 'UP MV HD', 'UP MV HD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-9))))
		/*** HD - casistica attivazione multipla dove NON trova HD+ o UP MV HD+***/
		when (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 3) and (charindex('+', COMBINAZIONE_PREMIUM_ATVZ) != 9) and len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) &gt; 0
			then 0 + (len(replace(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ))), 'UP MV HD', 'UP MV HD#')) - len(right(COMBINAZIONE_PREMIUM_ATVZ,(len(COMBINAZIONE_PREMIUM_ATVZ)-charindex('+', COMBINAZIONE_PREMIUM_ATVZ)))))
		/*** HD - casistica attivazione singola ***/
		when len(replace(COMBINAZIONE_PREMIUM_ATVZ, '+', '+#')) - len(COMBINAZIONE_PREMIUM_ATVZ) = 0
			and len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'HD', 'HD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) &gt;= 0
			then len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'HD', 'HD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV', 'MV#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
				- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MYSKYHD', 'MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
				+ len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV HD', 'UP MV HD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)
		else
			0
	end as COUNT_HD,
	(len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKYHD', 'MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
		- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKYHD', 'UP MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) as COUNT_MV_MYSKYHD,
	case
		when (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKY', 'MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKYHD', 'MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKY', 'UP MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) &gt;= 0 
		then (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKY', 'MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKYHD', 'MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKY', 'UP MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ))
		when (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKY', 'MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV MYSKYHD', 'MV MYSKYHD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
			- (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV MYSKY', 'UP MV MYSKY#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) &lt; 0 
		then 0
	end as COUNT_MV_MYSKY,
	(len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV HD', 'MV HD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) 
		-  (len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'UP MV HD', 'UP MV HD#')) - len(COMBINAZIONE_PREMIUM_ATVZ)) as COUNT_MV_HD,
	len(replace(COMBINAZIONE_PREMIUM_ATVZ, 'MV SD', 'MV SD#')) - len(COMBINAZIONE_PREMIUM_ATVZ) as COUNT_MV_SD
from dbo.WT_REV_ACT_ACTFEE_PRS</property>
