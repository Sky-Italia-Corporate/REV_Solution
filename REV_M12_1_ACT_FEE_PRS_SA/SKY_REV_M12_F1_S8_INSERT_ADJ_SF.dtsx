                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	a.GIO_ID,
	'ACT'+right(m.ann_id,2) as SCENARIO, 
	m.wee2_id as WEEK_ID,
	a.TIPO_DATO, 
	/* FRMR aggiunto dim TECNOLOGIA e CLUSTER */
	TECNOLOGIA,
	CLUSTER,
	case 
		when a.MULTIVISION='***-NO_MULTIVISION' then '2'
		when a.MULTIVISION='YES-C' or a.MULTIVISION='YES-NC' then '1'
		else '0'
 	end as MV_YES_NO,
 	case 
		when a.MULTIVISION='***-NO_MULTIVISION' then NULL
		when a.MULTIVISION='YES-C' then 'RGR_DEF_MVC'
		when a.MULTIVISION='YES-NC' then 'RGR_DEF'
		else NULL
 	end as LST_ID,
	a.PACCHETTO, 
	a.PROMO,
	a.TCN_ATV,
	a.NEW_EXISTING, 
	
    [BB_CONTRACT_STATUS] ,
    [BB_PRD_ID] ,
    [BB_ORIGIN_CLUSTER] ,
    [BB_VOICE_FLG] ,
    [BUNDLE_FLG] ,
	cast(a.ACT_FEE_N AS NUMERIC(38,10)) AS ACT_FEE_N
FROM T_REV_EXPORT_REVACT_ADJ_PRS a join M_REV_TEMPO m on a.GIO_ID=m.Gio_Id</property>
