            <component
              refId="Package\From SF_REV_OPTIONS To F_REV_OPTIONS_CLO\SF_REV_OPTIONS"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="SF_REV_OPTIONS"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* ATT69082_SkyGo_Extra - FRMR: Spaccatura cubo REVDAY e creazione cubo REVOP */
select 
    week_days.Gio_Id,
    isnull(curr_week.OPT_ID, prev_week.OPT_ID) as OPT_ID,  
    isnull(curr_week.RGR_OPT, prev_week.RGR_OPT) as RGR_OPT,
    -- Viene perso il dettaglio di pacchetto che non viene riportato sul cubo
    isnull(curr_week.TCN_ID, prev_week.TCN_ID) as TCN_ID,
    upper(isnull(curr_week.CLU_IPTV, prev_week.CLU_IPTV)) as CLU_IPTV,
    cast((isnull(curr_week.SUBS, 0) + isnull(prev_week.SUBS, 0))/2.0 as decimal(18, 5)) as SUBS,
	ISNULL(curr_week.BB_CONTRACT_STATUS,'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
	ISNULL(curr_week.BB_ORIGIN_CLUSTER,'ZZZ_NO_ORIGIN_CLUSTER') AS BB_ORIGIN_CLUSTER,
	ISNULL(curr_week.BB_PRD_ID,'ZZZ_NO_BB_PRODUCT') AS BB_PRD_ID,
	CASE WHEN curr_week.BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN curr_week.BB_VOICE_FLG='N_BB' THEN 'N' ELSE ISNULL(curr_week.BB_VOICE_FLG,'N') END AS BB_VOICE_FLG,
	ISNULL(curr_week.BUNDLE_FLG,'No Bundle') AS BUNDLE_FLG

from (
    select 
	   SCE_ID, 
	   Wee2_Id, 
	   PCK_ID, 
	   OPT_ID,
	   RGR_OPT,
	   TCN_ID,
	   CLU_IPTV, 
	   sum(SUBS) as SUBS
	   ,BB_CONTRACT_STATUS
	   ,BB_ORIGIN_CLUSTER
	   ,BB_PRD_ID
	   ,BB_VOICE_FLG
	   ,BUNDLE_FLG
    from SF_REV_OPTIONS sf
    join M_REV_PREV_WEEK prev_wk
	   on right(sf.SCE_ID, 2) + right(sf.Wee2_Id, 2) = right(prev_wk.CURR_YEAR, 2) + right(prev_wk.CURR_WEEK, 2)
    group by SCE_ID, 
	   Wee2_Id, 
	   PCK_ID, 
	   OPT_ID,
	   RGR_OPT,
	   TCN_ID,
	   CLU_IPTV	
	   ,BB_CONTRACT_STATUS
	   ,BB_ORIGIN_CLUSTER
	   ,BB_PRD_ID
	   ,BB_VOICE_FLG
	   ,BUNDLE_FLG
) prev_week
full outer join (
    select 
	   SCE_ID, 
	   Wee2_Id, 
	   PCK_ID, 
	   OPT_ID,
	   RGR_OPT,
	   TCN_ID,
	   CLU_IPTV, 
	   sum(SUBS) as SUBS
	   ,BB_CONTRACT_STATUS
	   ,BB_ORIGIN_CLUSTER
	   ,BB_PRD_ID
	   ,BB_VOICE_FLG
	   ,BUNDLE_FLG
    from SF_REV_OPTIONS sf
    join M_REV_PREV_WEEK prev_wk
	   on right(sf.SCE_ID, 2) + right(sf.Wee2_Id, 2) = right(prev_wk.PREV_YEAR, 2) + right(prev_wk.PREV_WEEK, 2)
    group by SCE_ID, 
	   Wee2_Id, 
	   PCK_ID, 
	   OPT_ID,
	   RGR_OPT,
	   TCN_ID,
	   CLU_IPTV
	   ,BB_CONTRACT_STATUS
	   ,BB_ORIGIN_CLUSTER
	   ,BB_PRD_ID
	   ,BB_VOICE_FLG
	   ,BUNDLE_FLG
) curr_week 
    on prev_week.OPT_ID = curr_week.OPT_ID 
	   and prev_week.RGR_OPT = curr_week.RGR_OPT
	   and prev_week.PCK_ID = curr_week.PCK_ID
	   and prev_week.TCN_ID = curr_week.TCN_ID
	   and prev_week.CLU_IPTV = curr_week.CLU_IPTV
cross join (
    select
		  t.Gio_Id
	   from SKY_REV.dbo.M_REV_CALC_WEEK m 
	   inner join SKY_REV.dbo.M_REV_TEMPO t 
		  on right(m.SCE_ID, 2) = right(t.Ann_Id, 2)
			 and m.WEEK_ID = t.Wee2_Id
) week_days</property>
