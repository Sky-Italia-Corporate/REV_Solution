                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****************************************************/
/* Recupera i valori dei sospesi per i giorni della */
/* settimana che stanno prima del sette nel caso di */
/* logica solare oppure prima del giorno N dato da */
/* inizio fatturazione + X giorni, prendendo   */
/* valore presente al giorno 7 (o N) del mese  */
/* precedente								   */
/****************************************************/
select
	st.SCE_ID, v.Gio_Id, st.PCK_ID, st.MOP_ID, st.TCN_DES, st.DUR_DES, st.CLS_DES, st.CLU_IPTV, st.NUM_CLI,
   st.BB_CONTRACT_STATUS, st.BB_ORIGIN_CLUSTER, st.BB_PRD_ID, st.BB_VOICE_FLG, st.BUNDLE_FLG
from M_REV_CALC_WEEK wk
join V_M_REV_TEMPO v
    on right(wk.SCE_ID, 2) + right(wk.WEEK_ID, 2) = right(v.Ann_id, 2) + right(v.Wee2_id, 2)
/*****************************************************************/
/*** Se all'interno della settimana è presente il giorno N dato da*/
/*** il giorno di inizio fatturazione + x giorni filtra i giorni della week in  */
/*** corso prendendo solo quelli che stanno prima di tale N	*/
/*****************************************************************/    
left join (select
		distinct convert(varchar(8),dateadd(dd,92,convert(datetime,convert(varchar(10),b.B28_ID,112))),112) as X_DEL_MESE
from M_REV_TEMPO tmp
join M_REV_CALENDARIO_28_BILLING B
on right(b.FY_YEAR_ID, 2) + right(b.FY_WEEK_ID,2) = right(tmp.Ann_id, 2) + right(tmp.Wee2_id, 2)
join M_REV_CALC_WEEK M
on	right(M.WEEK_ID,2) = B.FY_WEEK_ID and
	RIGHT(M.SCE_ID,2) = RIGHT(B.FY_YEAR_ID,2)
    where tmp.Gio_Id &gt;= convert(varchar(8),dateadd(dd,92,convert(datetime,convert(varchar(10),b.B28_ID,112))),112)
) x_del_mese
on v.Gio_Id &lt; x_del_mese.X_DEL_MESE and v.Gio_Uda = 'FATT. 28'
/*** [GITR]  2018-02-13 aggiunta condizione per gestire la prima settimana di Aprile in Fatt. Solare ***/
or ( v.Gio_Id between 20180401 and 20180406 and v.Gio_Id &lt; x_del_mese.X_DEL_MESE and v.Gio_Uda = 'FATT. SOL')
/*****************************************************************/
/*** Se all'interno della settimana è presente il sette del mese */
/*** ma con dati di Fatt. 28, recupera il sette del mese e filtra i giorni della week in  */
/*** corso prendendo solo quelli che stanno prima del 7		*/
/*****************************************************************/
left join (select
	    left(max(tmp.Gio_Id), 6) + '07' as SETTE_DEL_MESE
    from M_REV_CALC_WEEK wk
    join M_REV_TEMPO tmp
	   on right(wk.SCE_ID, 2) + right(wk.WEEK_ID, 2) = right(tmp.Ann_id, 2) + right(tmp.Wee2_id, 2)
    where cast(right(tmp.Gio_Id, 2) as integer) &gt;= 7
) sette_del_mese_28
    on v.Gio_Id &lt; sette_del_mese_28.SETTE_DEL_MESE and v.Gio_Uda = 'FATT. 28'
/*****************************************************************/
/*** Se all'interno della settimana è presente il sette del mese */
/*** recupera il sette del mese e filtra i giorni della week in  */
/*** corso prendendo solo quelli che stanno prima del 7		*/
/*****************************************************************/
left join (select
	    left(max(tmp.Gio_Id), 6) + '07' as SETTE_DEL_MESE
    from M_REV_CALC_WEEK wk
    join M_REV_TEMPO tmp
	   on right(wk.SCE_ID, 2) + right(wk.WEEK_ID, 2) = right(tmp.Ann_id, 2) + right(tmp.Wee2_id, 2)
    where cast(right(tmp.Gio_Id, 2) as integer) &gt;= 7
) sette_del_mese
    on v.Gio_Id &lt; sette_del_mese.SETTE_DEL_MESE and v.Gio_Uda = 'FATT. SOL'

cross join (
    select
	   st.SCE_ID, st.GIO_ID, st.PCK_ID, st.MOP_ID, st.TCN_DES, st.DUR_DES, st.CLS_DES, st.CLU_IPTV, st.NUM_CLI,
	   st.BB_CONTRACT_STATUS, st.BB_ORIGIN_CLUSTER, st.BB_PRD_ID, st.BB_VOICE_FLG, st.BUNDLE_FLG
    from ST_REV_SOSPESI st
    join (
	   select min(GIO_ID) as MIN_GIO from ST_REV_SOSPESI
    ) first_month_st
	   on st.GIO_ID = first_month_st.MIN_GIO
) st
where case	when v.Gio_Uda= 'FATT. 28' and v.Gio_Id &gt;= '20180101' then x_del_mese.X_DEL_MESE 
			when v.Gio_Uda= 'FATT. 28' and v.Gio_Id &lt; '20180101' then sette_del_mese_28.SETTE_DEL_MESE 
			else   sette_del_mese.SETTE_DEL_MESE end &gt; v.Gio_Id

