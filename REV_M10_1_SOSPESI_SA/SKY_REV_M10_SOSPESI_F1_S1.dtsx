    <DTS:Executable
      DTS:refId="Package\Cancella ultimo mese ST in caso di nuovo import"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{EBEB9BFB-D99D-4815-83E7-1765B71B4225}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Cancella ultimo mese ST in caso di nuovo import"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{71930B18-0220-4ABF-B10D-31CCDC0FF164}"
          SQLTask:SqlStatementSource="/*** Se importa un nuovo mese per i dati dei sospesi cancella l'ultimo mese presente in tabella ***/&#xA;/*** Nella tabella devono esserci sempre e solo due mesi, nel caso di una week con il giorno 7  ***/&#xA;/*** a cavallo (ad esempio mercoledì) uso i dati del primo mese per tutti i giorni fino al 7&#x9; ***/&#xA;/*** (in questo caso lunedì e martedì) e i dati dell'altro mese per tutti i giorni dal 7 in poi ***/&#xA;/*** (in questo caso mercoledì, giovedì, venerdì, sabato e domenica)&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; ***/&#xA;&#xA;if (select count(distinct GIO_ID) from ST_REV_SOSPESI) &gt; 2 &#xA;begin&#xA;      &#xA;      delete st&#xA;      from ST_REV_SOSPESI st&#xA;      join (&#xA;&#x9;   select min(GIO_ID) as GIO_ID from ST_REV_SOSPESI&#xA;&#x9; ) min_day&#xA;&#x9;   on st.GIO_ID = min_day.GIO_ID&#xA;&#xA;end" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Delete SF - ST e TMP_SF_REV_SOSPESI"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{0754CED4-4D36-4DDC-9A23-F62BAFEB53A9}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Delete SF - ST e TMP_SF_REV_SOSPESI"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9; © 2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:LoggingOptions
        DTS:FilterKind="0" />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{71930B18-0220-4ABF-B10D-31CCDC0FF164}"
          SQLTask:SqlStatementSource="delete sf&#xA;from SF_REV_SOSPESI sf&#xA;join (&#xA;&#x9;select &#xA;&#x9;   tmp.Gio_Id&#xA;&#x9;from M_REV_CALC_WEEK wk &#xA;&#x9;inner join M_REV_TEMPO tmp &#xA;&#x9;&#x9;on right(wk.SCE_ID, 2) = right(tmp.Ann_Id, 2)&#xA;&#x9;&#x9;&#x9;and wk.WEEK_ID = tmp.Wee2_Id&#xA;) week_days&#xA;    on sf.GIO_ID = week_days.Gio_Id&#xA;&#xA;&#xA;delete st&#xA;from ST_REV_SOSPESI st&#xA;join (&#xA;&#x9;select &#xA;&#x9;   tmp.Gio_Id&#xA;&#x9;from M_REV_CALC_WEEK wk &#xA;&#x9;inner join M_REV_TEMPO tmp &#xA;&#x9;&#x9;on right(wk.SCE_ID, 2) = right(tmp.Ann_Id, 2)&#xA;&#x9;&#x9;&#x9;and wk.WEEK_ID = tmp.Wee2_Id&#xA;) week_days&#xA;    on st.GIO_ID = week_days.Gio_Id" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
