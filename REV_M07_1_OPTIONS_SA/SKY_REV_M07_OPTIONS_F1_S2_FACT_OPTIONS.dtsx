                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
    t.SCE_ID,
    week_days.Wee2_Id,
    t.PCK_ID,
    t.OPT_ID,
    t.STM_ID,
    t.TCN_ID,
    t.CLU_IPTV, 
    t.SUBS,
	t.BB_CONTRACT_STATUS,
	t.BB_ORIGIN_CLUSTER,
	t.BB_PRD_ID,
	t.BB_VOICE_FLG,
	t.BUNDLE_FLG
from T_SBO_OPTIONS t
join (
    select
		  m.SCE_ID, t.Wee2_Id, t.Gio_Id
	   from SKY_REV.dbo.M_REV_CALC_WEEK m 
	   inner join SKY_REV.dbo.M_REV_TEMPO t 
		  on right(m.SCE_ID, 2) = right(t.Ann_Id, 2)
			 and m.WEEK_ID = t.Wee2_Id
) week_days
    on t.GIO_ID = week_days.Gio_Id</property>
