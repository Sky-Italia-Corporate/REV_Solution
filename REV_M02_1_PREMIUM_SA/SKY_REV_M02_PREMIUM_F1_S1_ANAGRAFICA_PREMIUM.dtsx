      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{EAF50812-CC5E-4D58-BE21-42401A2CA3B6}"
          SQLTask:SqlStatementSource="update S_REV_PRS_PREMIUM_SETTIMANALE&#xA;set PRS1_DES = '***-NO_PREMIUM'&#xA;where PRS1_DES is null&#xA;&#xA;GO&#xA;&#xA;drop table TMP_S_REV_PRS_PREMIUM&#xA;go&#xA;&#xA;select PRS_RC, PRS_DES, PRS2_DES, REV_DES, MOD_ID&#xA;into TMP_S_REV_PRS_PREMIUM&#xA;from (&#xA;/*Caso update*/&#x9;&#xA;&#x9;select prs.PRS_RC,&#xA;&#x9;&#x9;case&#xA;&#x9;&#x9;&#x9;when prs.PRS2_DES in ('SKYGO','BOX SETS','MV','SKYGO PLUS','MS', 'UHD') then REPLACE(prs.PRS1_DES, 'FROM', '-')&#xA;&#x9;&#x9;&#x9;when prs.PRS2_DES='SKY Q PLUS' and PRS_RC like '%0.00%' then LEFT(PRS_RC, 3) + ' - @' + RIGHT(PRS_RC, 4)&#xA;&#x9;&#x9;&#x9;when prs.PRS2_DES='SKY Q PLUS' and PRS_RC not like '%0.00%' then LEFT(PRS_RC, 3) + ' - @' + RIGHT(PRS_RC, 5)&#xA;&#x9;&#x9;&#x9;when prs.PRS1_DES &lt;&gt; '***-NO_PREMIUM' then prs.PRS1_DES +' - '+prs.MOD_DES&#xA;&#x9;&#x9;&#x9;else prs.PRS1_DES&#xA;&#x9;&#x9;end as PRS_DES, &#xA;&#x9;&#x9;PRS2_DES as PRS2_DES,&#xA;&#x9;&#x9;REV_DES,&#xA;&#x9;&#x9;CAST(prs.MOD_ID as varchar(255)) as MOD_ID&#xA;&#x9;from (&#x9;select distinct PRS_RC, PRS2_DES, PRS1_DES, REV_DES, MOD_DES, MOD_ID&#xA;&#x9;&#x9;&#x9;from S_REV_PRS_PREMIUM_SETTIMANALE&#xA;&#x9;) prs &#xA;&#xA;&#x9;union all&#xA;&#xA;/*Caso not update*/&#xA;&#x9;select PRS_RC, PRS_DES, PRS2_DES, REV_DES, MOD_ID&#xA;&#x9;from S_REV_PRS_PREMIUM &#xA;&#x9;WHERE (PRS_RC + cast(MOD_ID as varchar(255))) NOT IN&#xA;&#x9;&#x9;(&#x9;select a.PRS_RC + cast(a.MOD_ID as varchar(255)) as b&#xA;&#x9;&#x9;&#x9;from (&#x9;select distinct PRS_RC, PRS2_DES, PRS1_DES, REV_DES, MOD_DES, MOD_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;from S_REV_PRS_PREMIUM_SETTIMANALE )a&#xA;&#x9;&#x9;) &#xA;) a&#xA;go&#xA;&#xA;drop table S_REV_PRS_PREMIUM&#xA;go&#xA;&#xA;select PRS_RC, PRS_DES, PRS2_DES, REV_DES, MOD_ID&#xA;into S_REV_PRS_PREMIUM&#xA;from TMP_S_REV_PRS_PREMIUM&#xA;go&#xA;&#xA;if(&#x9;select count(*) &#xA;&#x9;from S_REV_PRS_PREMIUM&#xA;&#x9;where  PRS_DES = '***-NO_PREMIUM' ) = 0&#xA;insert into S_REV_PRS_PREMIUM values (NULL, '***-NO_PREMIUM', NULL, NULL, 0)&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
