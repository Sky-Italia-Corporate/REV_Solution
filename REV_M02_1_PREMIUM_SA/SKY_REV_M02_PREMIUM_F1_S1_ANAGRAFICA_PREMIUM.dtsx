                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
		f.MOD_ID, 
		f.MOD_DES, 
		f.PRS_RC,
		f.PRS1_DES,  
		f.PRS2_DES
		--f.IMP_RC_CANONE_SERV
from
	(
		select distinct e.IMP_RC_CANONE_SERV, e.MOD_ID, e.MOD_DES, e.PRS_RC, 
						cast(e.PRS_DES as varchar(80)) as PRS1_DES, d.PRS2_ID, f.PRS2_DES
		from (	select a.IMP_RC_CANONE_SERV, b.MOD_ID, b.MOD_DES, c.PRS1_ID, a.PRS_RC, c.PRS_DES
				from T_SBP_PREMIUM a 
				join T_SUB_MOD_MODALITA b on a.MOD_ID = b.MOD_ID
				join T_SUB_PRS_PREMIUM c on a.PRS_RC = c.PRS_RC
		) e left outer join T_SUB_PRS_PREMIUM1 d
			on e.PRS1_ID = d.PRS1_ID
		join T_SUB_PRS_PREMIUM2 f
			on d.PRS2_ID = f.PRS2_ID
	) f 
where PRS2_DES = 'BOX SETS'</property>
