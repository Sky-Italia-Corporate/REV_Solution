      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{AF090958-7732-480D-AA0A-D803C9EE749F}"
          SQLTask:SqlStatementSource="delete &#xA;from M_REV_STO_PREZZI_PREMIUM&#xA;where&#x9;WEEK_ID = (select top 1 WEEK_ID from M_REV_CALC_WEEK) and &#xA;&#x9;&#x9;SCE_ID = (select top 1 SCE_ID from M_REV_CALC_WEEK) and&#xA;&#x9;&#x9;PRS_DES &lt;&gt; 'HD - SKY Q PLUS'&#xA;&#xA;insert into M_REV_STO_PREZZI_PREMIUM&#xA;select SCE_ID, WEEK_ID, PRS_DES, PREZZO_CDW, PREZZO, VOLUMI_SETTIMANALI&#xA;from M_REV_PREZZI_PREMIUM a, (select top 1 SCE_ID, WEEK_ID from M_REV_CALC_WEEK) b&#xA;where a.PRS_DES &lt;&gt; 'HD - SKY Q PLUS'&#xA;&#xA;--drop table M_REV_PREZZI_PREMIUM&#xA;delete from M_REV_PREZZI_PREMIUM&#xA;where PRS_DES &lt;&gt; 'HD - SKY Q PLUS'&#xA;&#xA;insert into M_REV_PREZZI_PREMIUM&#xA;select d.PRS_DES, d.PREZZO_CDW, c.PREZZO, d.VOLUMI_SETTIMANALI&#xA;--into M_REV_PREZZI_PREMIUM&#xA;from (&#xA;&#x9;&#x9;select&#x9;case&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when PRS2_DES in ('SKYGO','BOX SETS','SKYGO PLUS','SKY Q PLUS','UHD','MS') then b.PRS1_DES&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else b.PRS1_DES+' - '+b.MOD_DES &#xA;&#x9;&#x9;&#x9;&#x9;end as PRS_DES,&#xA;&#x9;&#x9;&#x9;&#x9;a.IMP_RC_CANONE_SERV as PREZZO_CDW, &#xA;&#x9;&#x9;&#x9;&#x9;sum(SUBS) as VOLUMI_SETTIMANALI&#xA;&#x9;&#x9;from ST_REV_PREMIUM_VOLUMI a&#xA;&#x9;&#x9;join S_REV_PRS_PREMIUM_SETTIMANALE b&#xA;&#x9;&#x9;&#x9;on a.PRS_RC = b.PRS_RC and a.MOD_ID = b.MOD_ID&#xA;&#x9;&#x9;where a.GIO_ID = (&#x9;select max(Gio_Id)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from M_REV_TEMPO&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where Wee2_Id = (select top 1 WEEK_ID from M_REV_CALC_WEEK)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and right(Ann_Id, 2) = (select top 1 right(SCE_ID, 2) from M_REV_CALC_WEEK))&#xA;&#x9;&#x9;group by case&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when PRS2_DES in ('SKYGO','BOX SETS','SKYGO PLUS','SKY Q PLUS','UHD','MS') then b.PRS1_DES&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;else b.PRS1_DES+' - '+b.MOD_DES &#xA;&#x9;&#x9;&#x9;&#x9;end, a.IMP_RC_CANONE_SERV&#xA;) d&#xA;left outer join (&#x9;select a.*&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;from M_REV_STO_PREZZI_PREMIUM a&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;join (&#x9;select max(SCE_ID+WEEK_ID)as FYWEEK&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from M_REV_STO_PREZZI_PREMIUM&#xA;&#x9;&#x9;&#x9;&#x9;) b on (a.SCE_ID+a.WEEK_ID) = b.FYWEEK&#xA;) c on d.PRS_DES = c.PRS_DES and &#xA;&#x9;&#x9;d.PREZZO_CDW = c.PREZZO_CDW&#xA;--0:03&#xA;&#xA;update M_REV_PREZZI_PREMIUM&#xA;set PREZZO = PREZZO_CDW&#xA;where PREZZO is null&#xA;&#x9;&#x9;and PRS_DES &lt;&gt; 'HD - SKY Q PLUS' &#xA;--0:00" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
