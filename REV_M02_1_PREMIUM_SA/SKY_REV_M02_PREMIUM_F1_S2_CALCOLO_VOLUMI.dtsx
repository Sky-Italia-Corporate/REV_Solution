              name="T_SBP_PREMIUM"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[T_SBP_PREMIUM]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	GIO_ID, SCE_ID, PCK_ID, STM_ID, SUBS, 
		a.PRS_RC, a.MOD_ID, NEX_DES, MOP_ID, 
		TCN_ID, CLU_IPTV, CNP_ID, TIPO_SERVIZIO, 
		case 
			when b.PRS2_DES in ('SKYGO','BOX SETS','SKYGO PLUS','SKY Q PLUS','UHD','MS') then IMP_RC_CANONE_SERV/(1+VAL_IVA)
			else IMP_RC_CANONE_SERV
		end as IMP_RC_CANONE_SERV,
		isnull(RPS_ID,0) as RPS_ID, --'***-NO_RGR_PREMIUM'
		BB_CONTRACT_STATUS,
		BB_ORIGIN_CLUSTER,
		BB_PRD_ID,
		BB_VOICE_FLG,
		BUNDLE_FLG
from T_SBP_PREMIUM a
left join S_REV_PRS_PREMIUM_SETTIMANALE b
	on	a.PRS_RC=b.PRS_RC and 
		a.MOD_ID=b.MOD_ID
