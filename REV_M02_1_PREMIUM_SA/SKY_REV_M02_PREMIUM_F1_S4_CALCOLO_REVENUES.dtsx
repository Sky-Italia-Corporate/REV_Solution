              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	prs.GIO_ID, 
		prs.SCE_ID, 
		prs.PCK_ID, 
		prs.PRS_RC, 
		prs.MOD_ID, 
		prs.MOP_ID, 
		prs.TCN_ID, 
		prs.CLU_IPTV,
		prs.RPS_ID,
		prs.IMP_RC_CANONE_SERV,
		prs.SUBS,
		case when tm.Gio_Uda = 'FATT. SOL'
                                      then (m.PREZZO * prs.SUBS)/365*12
                                      else (m.PREZZO * prs.SUBS)/28
                                      end as REVENUES,
			prs.BB_CONTRACT_STATUS,
			prs.BB_ORIGIN_CLUSTER,
			prs.BB_PRD_ID,
			prs.BB_VOICE_FLG,
			prs.BUNDLE_FLG
from (
	select	case
	   			when rgr.RPS_RGR_DES is null and prs.PRS2_DES &lt;&gt; 'SKY Q PLUS' then prs.PRS_DES
	   			when rgr.RPS_RGR_DES is null and prs.PRS2_DES = 'SKY Q PLUS' then prs.PRS2_DES + SUBSTRING(prs.PRS_DES,charindex('-',prs.PRS_DES)-1,LEN(prs.PRS_DES))
	   			else prs.PRS_DES+' - '+rgr.RPS_DES
			end PRS_DES,
			GIO_ID, SCE_ID, PCK_ID, st.PRS_RC, MOP_ID, TCN_ID, CLU_IPTV, st.MOD_ID, st.RPS_ID, IMP_RC_CANONE_SERV, SUBS,
			BB_CONTRACT_STATUS,
			BB_ORIGIN_CLUSTER,
			BB_PRD_ID,
			BB_VOICE_FLG,
			BUNDLE_FLG
	from ST_REV_PREMIUM_VOLUMI st
	join S_REV_PRS_PREMIUM prs
		on st.PRS_RC = prs.PRS_RC and 
			st.MOD_ID = prs.MOD_ID
	join S_REV_RPS_RGR_PREMIUM rgr
				on st.RPS_ID = rgr.RPS_ID
) prs
left join M_REV_PREZZI_PREMIUM m
	on prs.PRS_DES = m.PRS_DES and 
	   prs.IMP_RC_CANONE_SERV = m.PREZZO_CDW
left join V_M_REV_TEMPO tm 
	on prs.GIO_ID = tm.GIO_ID</property>
