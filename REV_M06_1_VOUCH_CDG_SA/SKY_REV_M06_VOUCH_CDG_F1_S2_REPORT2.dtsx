                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
	a.COD_PROD_SAP, a.DES_PROD_SAP, a.DMBTR, right(b.COD_PROMO_SBL,4)+'-'+b.GROUP_CODE_VOUCHER as RC_COD,
	right(b.COD_PROMO_SBL,4) as COD_PROMO_SBL,
	b.GROUP_CODE_VOUCHER,
	case
		when c.delta_act_fee is null then 0
		else c.delta_act_fee
	end as delta_act_fee,
	case
		when d.PRT_DES is null then 'DESCRIZIONE PROMO NON TROVATA'
		else d.PRT_DES
	end as PROMO_DES
from T_REV_SAP_RV_AN_S02 a
join T_REV_SAP_RV_DT_S04 b
	on a.COD_PROD_SAP = b.COD_PROD_SAP
left join (
	select
		cast(a.prt_id as varchar(255)) as PRT_ID, a.voucher, a.delta_act_fee
	from M_REV_STO_ACTFEE_DELTA a
	join (
		select
			PRT_ID, VOUCHER, MAX(right(SCE_ID,2)+right(WEEK_ID,2)) as MAX_WEEK
		from M_REV_STO_ACTFEE_DELTA
		group by prt_id, voucher
	) b
		on RIGHT(a.SCE_ID,2)+RIGHT(a.WEEK_ID,2) = b.MAX_WEEK and a.prt_id = b.prt_id and a.voucher = b.voucher
) c
	on right(b.COD_PROMO_SBL,4) = c.PRT_ID and b.GROUP_CODE_VOUCHER = c.voucher
left join T_REV_PRT_PROMO_TECNICA d
	on right(b.COD_PROMO_SBL,4) = d.PRT_ID
where ORD_INTERNO = 'SZ0ATT600'
	and right(b.COD_PROMO_SBL,4)+'-'+GROUP_CODE_VOUCHER not in (select PRT_ID+'-'+VOUCHER from WT_REV_M06_REPORT2_DATI_HYP)</property>
