                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
      'ACT' + SUBSTRING(v.Period_DES,3,2) as sce_id,
      'W' + SUBSTRING(v.Period_DES,7,2) as week_id,
      b.gio_id,
      a.rc_pacchetto,
      a.cod_promo_tec,
	  a.BB_CONTRACT_STATUS,
	  a.BB_PRD_ID,
	  a.BB_ORIGIN_CLUSTER,
	  a.BB_VOICE_FLG,
	  a.BUNDLE_FLG,
      sum(volumi) volumi
FROM  SF_REV_DIS_SCONTI_MGMT a
join V_M_REV_PERIOD_COMPETENZA v on a.CMP_PER_ID = v.Period_ID
join s_promo c on a.cod_promo_tec = c.promo_id
join (
            select  distinct convert(varchar(8), cast(t.Gio_Id as datetime)+1, 112) as Gio_Id,
            week_ID,
            SCE_ID
            from M_REV_TEMPO t
            inner join M_REV_CALC_WEEK m
                  on m.Week_ID = t.Wee2_Id
                        and right(ltrim(rtrim(t.Ann_Id)), 2) = right(ltrim(rtrim(m.SCE_ID)), 2)
) b on 'W' + SUBSTRING(v.Period_DES,7,2) = b.WEEK_ID and 'ACT' + SUBSTRING(v.Period_DES,3,2) = b.SCE_ID
where c.voucher_no_voucher = 'Yes'
and a.TIPO_DATO in ('AUTO','MAN')
and 'W' + SUBSTRING(v.Period_DES,7,2) in
(select distinct week_id from M_REV_CALC_WEEK)
group by
      'W' + SUBSTRING(v.Period_DES,7,2),
      'ACT' + SUBSTRING(v.Period_DES,3,2),
      b.gio_id,
      a.rc_pacchetto,
      a.cod_promo_tec,
	  a.BB_CONTRACT_STATUS,
	  a.BB_PRD_ID,
	  a.BB_ORIGIN_CLUSTER,
	  a.BB_VOICE_FLG,
	  a.BUNDLE_FLG</property>
