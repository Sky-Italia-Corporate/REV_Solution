                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Aggiunto il dettaglio PROMO_ID

select c.PCK_ID, c.GIO_ID, c.SCE_ID ,sum(c.VOLUMI) as SUBS, d.IMP_CNN, c.PROMO_ID,
		c.BB_CONTRACT_STATUS, c.BB_ORIGIN_CLUSTER, c.BB_PRD_ID, c.BB_VOICE_FLG,
		c.BUNDLE_FLG
from ST_REV_VOUC_OPE c join
		(select pck_id, dat_id, max(imp_cnn) imp_cnn 
		from SF_REV_PREZZI_OPE a
		join
		 (select  distinct convert(varchar(8), cast(t.Gio_Id as datetime)+1, 112) as Gio_Id,
				week_ID,
				SCE_ID
				from M_REV_TEMPO t 
				inner join M_REV_CALC_WEEK m 
					on m.Week_ID = t.Wee2_Id 
						and right(ltrim(rtrim(t.Ann_Id)), 2) = right(ltrim(rtrim(m.SCE_ID)), 2)) b 
		on a.dat_id = b.gio_id
		group by pck_id, dat_id) d
on c.GIO_ID = d.DAT_ID 
