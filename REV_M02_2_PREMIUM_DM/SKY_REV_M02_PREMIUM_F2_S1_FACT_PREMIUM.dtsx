                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">----modifica dato opening-----
select 
	convert(varchar(8), cast(f.[GIO_ID] as datetime)+1, 112) AS GIO_ID, 
	a.PCK_DES, 
	a.PRS_ID as PRS_ID, 
	a.TCN_DES,
	a.CLU_IPTV as CLS_DES,
	cast(rps.RPS_DES as varchar(80)) as RPS_DES,
	cast(a.REV_DES as varchar(80)) as REV_DES,
	sum(a.VOL_CLOSING) as VOLUME, 
	sum(a.REVENUES) as REVENUES,
	ISNULL(BB_CONTRACT_STATUS,'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
	ISNULL(BB_ORIGIN_CLUSTER,'ZZZ_NO_ORIGIN_CLUSTER') AS BB_ORIGIN_CLUSTER,
	ISNULL(BB_PRD_ID,'ZZZ_NO_BB_PRODUCT') AS BB_PRD_ID,
	CASE WHEN BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN BB_VOICE_FLG='N_BB' THEN 'N' ELSE ISNULL(BB_VOICE_FLG,'N') END AS BB_VOICE_FLG,
	ISNULL(BUNDLE_FLG,'No Bundle') AS BUNDLE_FLG
from SF_REV_PREMIUM a
join (	select Gio_Id
		from M_REV_CALC_WEEK e
		join M_REV_TEMPO d
			on e.WEEK_ID = d.Wee2_Id and right(ltrim(rtrim(d.Ann_Id)), 2) = right(ltrim(rtrim(e.SCE_ID)), 2)
) f on a.GIO_ID = f.GIO_ID
left join [SKY_SA].[dbo].[M_SUB_MOD_RGR_MODALITA] M
	on  right(a.PRS_ID,len(M.MOD_DES)) = M.MOD_DES
join S_REV_RPS_RGR_PREMIUM rps
	on a.RPS_ID = rps.RPS_ID
group by f.Gio_Id, a.PCK_DES, a.PRS_ID, a.TCN_DES,a.CLU_IPTV, rps.RPS_DES, a.REV_DES,
	ISNULL(BB_CONTRACT_STATUS,'ZZZ_NO_BB_CONTRACT_STATUS'),
	ISNULL(BB_ORIGIN_CLUSTER,'ZZZ_NO_ORIGIN_CLUSTER'),
	ISNULL(BB_PRD_ID,'ZZZ_NO_BB_PRODUCT'),
	CASE WHEN BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN BB_VOICE_FLG='N_BB' THEN 'N' ELSE ISNULL(BB_VOICE_FLG,'N') END,
	ISNULL(BUNDLE_FLG,'No Bundle')

		union all

select 
	convert(varchar(8), cast(f.[GIO_ID] as datetime)+1, 112) AS GIO_ID,
	a.PCK_DES, 
	a.PRS_ID as PRS_ID,
	a.TCN_DES,
	a.CLU_IPTV as CLS_DES,
	cast(rps.RPS_DES as varchar(80)) as RPS_DES,
	cast(a.REV_DES as varchar(80)) as REV_DES,
	sum(a.VOL_CLOSING) as VOLUME, 
	sum(a.REVENUES) as REVENUES,
	ISNULL(BB_CONTRACT_STATUS,'ZZZ_NO_BB_CONTRACT_STATUS') AS BB_CONTRACT_STATUS,
	ISNULL(BB_ORIGIN_CLUSTER,'ZZZ_NO_ORIGIN_CLUSTER') AS BB_ORIGIN_CLUSTER,
	ISNULL(BB_PRD_ID,'ZZZ_NO_BB_PRODUCT') AS BB_PRD_ID,
	CASE WHEN BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN BB_VOICE_FLG='N_BB' THEN 'N' ELSE ISNULL(BB_VOICE_FLG,'N') END AS BB_VOICE_FLG,
	ISNULL(BUNDLE_FLG,'No Bundle') AS BUNDLE_FLG
from SF_REV_PREMIUM a
join (	select 
			convert(varchar(8), cast(MIN([GIO_ID]) as datetime)-1, 112) AS GIO_ID  --min(gio_id) as GIO_ID 
		from M_REV_CALC_WEEK e
		join M_REV_TEMPO d
			on e.WEEK_ID = d.Wee2_Id and 
				right(ltrim(rtrim(d.Ann_Id)), 2) = right(ltrim(rtrim(e.SCE_ID)), 2)
) f on a.GIO_ID = f.GIO_ID
left join [SKY_SA].[dbo].[M_SUB_MOD_RGR_MODALITA] M
	on  right(a.PRS_ID,len(M.MOD_DES)) = M.MOD_DES
join S_REV_RPS_RGR_PREMIUM rps
	on a.RPS_ID = rps.RPS_ID
group by f.Gio_Id, a.PCK_DES, a.PRS_ID, a.TCN_DES, a.CLU_IPTV, rps.RPS_DES, a.REV_DES,
	ISNULL(BB_CONTRACT_STATUS,'ZZZ_NO_BB_CONTRACT_STATUS'),
	ISNULL(BB_ORIGIN_CLUSTER,'ZZZ_NO_ORIGIN_CLUSTER'),
	ISNULL(BB_PRD_ID,'ZZZ_NO_BB_PRODUCT'),
	CASE WHEN BB_VOICE_FLG='Y_BB' THEN 'Y'
		WHEN BB_VOICE_FLG='N_BB' THEN 'N' ELSE ISNULL(BB_VOICE_FLG,'N') END,
	ISNULL(BUNDLE_FLG,'No Bundle')

